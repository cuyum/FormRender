  <h:body>
    <group ref="/C.1.3/repeat">
      <label>Infraestructura de Red</label>
      <repeat nodeset="/C.1.3/repeat">
        <group ref="/C.1.3/repeat/equipamiento">
          <label>Equipamiento</label>
          <select1 appearance="minimal" ref="/C.1.3/repeat/equipamiento/spinner">
            <label>Tecnología</label>
            <hint>Seleccione Tecnología</hint>
            <item>
              <label>SDH</label>
              <value>a</value>
            </item>
            <item>
              <label>DWDM</label>
              <value>b</value>
            </item>
            <item>
              <label>PDH</label>
              <value>c</value>
            </item>
            <item>
              <label>IP</label>
              <value>d</value>
            </item>
            <item>
              <label>Otros</label>
              <value>e</value>
            </item>
          </select1>
          <input ref="/C.1.3/repeat/equipamiento/my_int1">
            <label>Capacidad Disponible</label>
            <hint>Agregue numero entero</hint>
          </input>
          <input ref="/C.1.3/repeat/equipamiento/my_int2">
            <label>Capacidad Utilizada</label>
            <hint>Agregue numero entero</hint>
          </input>
        </group>
        <group ref="/C.1.3/repeat/medio">
          <label>Medio</label>
          <select1 appearance="minimal" ref="/C.1.3/repeat/medio/spinner">
            <label>Tecnología</label>
            <hint>Seleccione Tecnología</hint>
            <item>
              <label>FO</label>
              <value>1</value>
            </item>
            <item>
              <label>MW</label>
              <value>2</value>
            </item>
            <item>
              <label>Otros</label>
              <value>3</value>
            </item>
          </select1>
          <input ref="/C.1.3/repeat/medio/my_int1">
            <label>Capacidad Disponible</label>
            <hint>Agregue numero entero</hint>
          </input>
          <input ref="/C.1.3/repeat/medio/my_int2">
            <label>Capacidad Utilizada</label>
            <hint>Agregue numero entero</hint>
          </input>
        </group>
        <group ref="/C.1.3/repeat/tendido">
          <label>Tendido</label>
          <select1 appearance="minimal" ref="/C.1.3/repeat/tendido/spinner">
            <label>Tecnología</label>
            <hint>Seleccione Tecnología</hint>
            <item>
              <label>AÉREO</label>
              <value>1</value>
            </item>
            <item>
              <label>SUBTERRANEO</label>
              <value>2</value>
            </item>
            <item>
              <label>Otros</label>
              <value>3</value>
            </item>
          </select1>
          <input ref="/C.1.3/repeat/tendido/my_int1">
            <label>Capacidad Disponible</label>
            <hint>Agregue numero entero</hint>
          </input>
          <input ref="/C.1.3/repeat/tendido/my_int2">
            <label>Capacidad Utilizada</label>
            <hint>Agregue numero entero</hint>
          </input>
        </group>
        <group ref="/C.1.3/repeat/gral">
          <label>Ubicación</label>
          <select1 ref="/C.1.3/repeat/gral/provincia">
            <label>Provincia/Región</label>
            <item>
              <label>Buenos Aires</label>
              <value>buenosAires</value>
            </item>
            <item>
              <label>Catamarca</label>
              <value>catamarca</value>
            </item>
          </select1>
          <select1 ref="/C.1.3/repeat/gral/partido">
            <label>Partido/Dpto</label>
            <itemset nodeset="instance('partidos')/root/item[provincia= /C.1.3/repeat/gral/provincia ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <select1 ref="/C.1.3/repeat/gral/localidad">
            <label>Localidad</label>
            <itemset nodeset="instance('localidades')/root/item[provincia= /C.1.3/repeat/gral/provincia  and partido= /C.1.3/repeat/gral/partido ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
        </group>
      </repeat>
    </group>
