<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario I1.5 - Clientes</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <I1.5 id="I1.5">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <operativos/>
              <prepago/>
              <pospago/>
              <mixto/>
              <suscripcion/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </I1.5>
      </instance>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/I1.5/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/I1.5/meses/mes/provincia" nodeset="/I1.5/meses/mes/partido" required="true()" type="select1"/>
      <bind nodeset="/I1.5/meses/mes/partido_otro" relevant=" /I1.5/meses/mes/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/I1.5/meses/mes/partido" nodeset="/I1.5/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/I1.5/meses/mes/localidad_otro" relevant=" /I1.5/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/I1.5/meses/mes/operativos" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/I1.5/meses/mes/prepago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/I1.5/meses/mes/pospago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/I1.5/meses/mes/mixto" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/I1.5/meses/mes/suscripcion" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/I1.5/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/I1.5/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/I1.5/meses">
        <group appearance="col4 well-white variable" ref="/I1.5/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/I1.5/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/I1.5/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/I1.5/meses/mes/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/I1.5/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/I1.5/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <input ref="/I1.5/meses/mes/operativos">
            <label>Operativos</label>
            <hint>Operativos</hint>
          </input>
          <input ref="/I1.5/meses/mes/prepago">
            <label>Prepago</label>
            <hint>Prepago</hint>
          </input>
          <input ref="/I1.5/meses/mes/pospago">
            <label>Pospago</label>
            <hint>Pospago</hint>
          </input>
          <input ref="/I1.5/meses/mes/mixto">
            <label>Mixto</label>
            <hint>Mixto</hint>
          </input>
          <input ref="/I1.5/meses/mes/suscripcion">
            <label>Suscripción de servicios adicionales</label>
            <hint>Suscripción de servicios adicionales</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
