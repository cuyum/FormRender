<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.6 - Clientes operativos (modalidad de contratación)</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <C1.6 id="C1.6">
          <titulo/>
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <clientes/>
              <prepago/>
              <pospago/>
              <mixto/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.6>
      </instance>
      <bind nodeset="/C1.6/titulo" readonly="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/C1.6/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/C1.6/meses/mes/provincia" nodeset="/C1.6/meses/mes/partido" required="true()" type="select1"/>
      <bind nodeset="/C1.6/meses/mes/partido_otro" relevant=" /C1.6/meses/mes/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/C1.6/meses/mes/partido" nodeset="/C1.6/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/C1.6/meses/mes/localidad_otro" relevant=" /C1.6/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/areas and depends=/C1.6/meses/mes/localidad" nodeset="/C1.6/meses/mes/area" required="true()" type="select1"/>
      <bind nodeset="/C1.6/meses/mes/area_otro" relevant=" /C1.6/meses/mes/area =9999" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/C1.6/meses/mes/clientes" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/C1.6/meses/mes/prepago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/C1.6/meses/mes/pospago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99999999" nodeset="/C1.6/meses/mes/mixto" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.6/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input appearance="tooltip" ref="/C1.6/titulo">
      <hint>Informar datos al final del período considerado.</hint>
    </input>
    <group ref="/C1.6/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/C1.6/meses">
        <group appearance="col4 well-white variable" ref="/C1.6/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/C1.6/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.6/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/C1.6/meses/mes/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/C1.6/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/C1.6/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/C1.6/meses/mes/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/C1.6/meses/mes/area_otro">
            <label>Área Otros</label>
            <hint>Área Otros</hint>
          </input>
          <input ref="/C1.6/meses/mes/clientes">
            <label>Clientes</label>
            <hint>Clientes</hint>
          </input>
          <input ref="/C1.6/meses/mes/prepago">
            <label>Prepago</label>
            <hint>Prepago</hint>
          </input>
          <input ref="/C1.6/meses/mes/pospago">
            <label>Pospago</label>
            <hint>Pospago</hint>
          </input>
          <input ref="/C1.6/meses/mes/mixto">
            <label>Mixto</label>
            <hint>Mixto</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
