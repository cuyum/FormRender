<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.2.1-A Eficiencia de llamada - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <ict4.2.2.1-ax id="ICT4.2.2.1-ax">
			<trimestres jr:template="">
              <hora_pico_inicio/>
              <hora_pico_fin/>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.2.1-ax>
      </instance>
      <bind constraint="agrupador=3" nodeset="/ict4.2.2.1-ax/trimestres/relacion/hora_pico_inicio" required="true()" type="time"/>
      <bind constraint="url=/servicios/rest/parameter?name=minutos_hora and hora_delta= /ict4.2.2.1-ax/trimestres/relacion/hora_pico_inicio " nodeset="/ict4.2.2.1-ax/trimestres/relacion/hora_pico_fin" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.2.1-ax/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.2.1-ax/trimestres">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.2.2.1-ax/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.2.1-ax/trimestres/relacion">
          <label>Relaci√≥n entre la cantidad de llamadas completadas y el total de intentos</label>
          <input ref="/ict4.2.2.1-ax/trimestres/relacion/hora_pico_inicio">
            <label>Hora Pico Inicio</label>
            <hint>Hora Pico Inicio</hint>
          </input>
          <input ref="/ict4.2.2.1-ax/trimestres/relacion/hora_pico_fin">
            <label>Hora Pico Fin</label>
            <hint>Hora Pico Final</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
