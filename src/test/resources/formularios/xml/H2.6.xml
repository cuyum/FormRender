            <santa_Cruz/>
            <santa_Fe/>
            <santiago_Estero/>
            <tierra_Fuego/>
            <tucuman/>
            <total/>
          </provincias>
          <meta>
            <instanceID/>
          </meta>
        </H2.6>
      </instance>
      <bind nodeset="/H2.6/titulo" readonly="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/H2.6/provincias/caba" required="true()" type="int"/>
