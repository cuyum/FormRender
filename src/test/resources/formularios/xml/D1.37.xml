<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.37 - Ingresos por Provincia</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <D1.37 id="D1.37">
          <titulo/>
          <meses jr:template="">
            <mes>
              <caba/>
              <buenosAires/>
              <catamarca/>
              <chaco/>
              <chubut/>
              <cordoba/>
              <corrientes/>
              <entreRios/>
              <formosa/>
              <jujuy/>
              <laPampa/>
              <laRioja/>
              <mendoza/>
              <misiones/>
              <neuquen/>
              <rioNegro/>
              <salta/>
              <sanJuan/>
              <sanLuis/>
              <santaCruz/>
              <santaFe/>
              <santiagoDelEstero/>
              <tierraDelFuego/>
              <tucuman/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </D1.37>
      </instance>
      <bind nodeset="/D1.37/titulo" readonly="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999" nodeset="/D1.37/meses/mes/caba" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999" nodeset="/D1.37/meses/mes/buenosAires" required="true()" type="int"/>
