      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999" nodeset="/D1.37/meses/mes/tucuman" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999" nodeset="/D1.37/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.37/meta/instanceID" readonly="true()" type="string"/>
    </model>
