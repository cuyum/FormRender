<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.50 - Inversiones</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <C1.50 id="C1.50">
          <titulo/>
          <meses jr:template="">
            <mes>
              <redes_de_acceso>
                <redAccesoFijo/>
                <redAccesoMovil/>
              </redes_de_acceso>
              <transporte>
                <transporteOptico/>
                <transporteCobre/>
                <transporteRadioelectRadioenlaces/>
                <transporteRadioelectSatelital/>
              </transporte>
              <obra_civil>
                <ocivilTransporte/>
                <ocivilInalambricas/>
                <ocivilAcceso/>
                <ocivilPlantel/>
              </obra_civil>
              <equipamiento>
                <equipRedesTrasnFijoyMovil/>
                <equipInstalacionesClientes/>
              </equipamiento>
              <software>
                <softAdministracion/>
                <softMonitoreo/>
                <softMantenimiento/>
                <softNodo/>
              </software>
              <licencias>
                <licenciasSoftware/>
                <licenciasHardware/>
                <licenciasRegulatorias/>
              </licencias>
              <infraestAdministComercial/>
              <desarrolloServicioUniversal/>
              <otros/>
              <total_total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.50>
      </instance>
      <bind nodeset="/C1.50/titulo" readonly="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/redes_de_acceso/redAccesoFijo" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/redes_de_acceso/redAccesoMovil" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/transporte/transporteOptico" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/transporte/transporteCobre" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/transporte/transporteRadioelectRadioenlaces" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/transporte/transporteRadioelectSatelital" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/obra_civil/ocivilTransporte" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/obra_civil/ocivilInalambricas" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/obra_civil/ocivilAcceso" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/obra_civil/ocivilPlantel" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/equipamiento/equipRedesTrasnFijoyMovil" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/equipamiento/equipInstalacionesClientes" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/software/softAdministracion" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/software/softMonitoreo" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/software/softMantenimiento" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/software/softNodo" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/licencias/licenciasSoftware" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/licencias/licenciasHardware" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/licencias/licenciasRegulatorias" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/infraestAdministComercial" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/desarrolloServicioUniversal" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/otros" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C1.50/meses/mes/total_total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.50/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input appearance="tooltip" ref="/C1.50/titulo">
      <hint>Completar en base a los mismos criterios utilizados en los EECC.</hint>
    </input>
    <group ref="/C1.50/meses">
      <label></label>
      <repeat nodeset="/C1.50/meses">
        <group appearance="col1 well-white variable" ref="/C1.50/meses/mes">
          <label>{title}</label>
          <group appearance="col1 well-white" ref="/C1.50/meses/mes/redes_de_acceso">
            <label>Redes de Acceso</label>
            <input ref="/C1.50/meses/mes/redes_de_acceso/redAccesoFijo">
              <label>Fijo</label>
              <hint>Acceso Fijo [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/redes_de_acceso/redAccesoMovil">
              <label>Móvil</label>
              <hint>Acceso Móvil [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/transporte">
            <label>Transporte</label>
            <input ref="/C1.50/meses/mes/transporte/transporteOptico">
              <label>Óptico</label>
              <hint>Transporte Óptico [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporteCobre">
              <label>Cobre</label>
              <hint>Transporte Cobre [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporteRadioelectRadioenlaces">
              <label>Radioeléctrico Radioenlaces</label>
              <hint>Transporte Radioeléctrico. Radioenlaces [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporteRadioelectSatelital">
              <label>Radioeléctrico Satelital</label>
              <hint>Transporte Radioeléctrico. Satelital [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/obra_civil">
            <label>Obra Civil</label>
            <input ref="/C1.50/meses/mes/obra_civil/ocivilTransporte">
              <label>Redes de Transporte</label>
              <hint>Obra Civil. Redes de Transporte [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivilInalambricas">
              <label>Redes Inalámbricas</label>
              <hint>Obra Civil. Redes Inalámbricas [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivilAcceso">
              <label>Redes de Acceso</label>
              <hint>Obra Civil. Redes de Acceso [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivilPlantel">
              <label>Plantel Exterior</label>
              <hint>Obra Civil. Plantel Exterior [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col1 well-white" ref="/C1.50/meses/mes/equipamiento">
            <label>Equipamiento</label>
            <input ref="/C1.50/meses/mes/equipamiento/equipRedesTrasnFijoyMovil">
              <label>Redes de Transporte Acceso fijo y móvil</label>
              <hint>Equipamiento asociado a las redes de transporte y acceso fijo y móvil [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/equipamiento/equipInstalacionesClientes">
              <label>Instalaciones de los Clientes</label>
              <hint>Equipamiento en Instalaciones de los Clientes [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/software">
            <label>Software</label>
            <input ref="/C1.50/meses/mes/software/softAdministracion">
              <label>Administración la Red</label>
              <hint>Software para administración de red [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/softMonitoreo">
              <label>Monitoreo y Control</label>
              <hint>Software para Monitoreo y control [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/softMantenimiento">
              <label>Mantenimiento</label>
              <hint>Software para mantenimiento [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/softNodo">
              <label>Nodo o Datacenter de Alojamiento</label>
              <hint>Software para Nodo o Datacenter de Alojamiento [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/licencias">
            <label>Licencias</label>
            <input ref="/C1.50/meses/mes/licencias/licenciasSoftware">
              <label>Software</label>
              <hint>Licencias Software [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/licencias/licenciasHardware">
              <label>Hardware</label>
              <hint>Licencias Hardware [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/licencias/licenciasRegulatorias">
              <label>Regulatorias</label>
              <hint>Licencias Regulatorias [en miles de pesos]</hint>
            </input>
          </group>
          <input ref="/C1.50/meses/mes/infraestAdministComercial">
            <label>Infraestructura Administrativa/Comercial</label>
            <hint>Infraestructura administrativa/comercial [en miles de pesos]</hint>
          </input>
          <input ref="/C1.50/meses/mes/desarrolloServicioUniversal">
            <label>Desarrollo del Servicio Universal</label>
            <hint>Desarrollo del Servicio Universal, conf Pto 10.1 f) Anexo I Dtoo. 764/2000 [en miles de pesos]</hint>
          </input>
          <input ref="/C1.50/meses/mes/otros">
            <label>Otros</label>
            <hint>Otros [en miles de pesos]</hint>
          </input>
          <input ref="/C1.50/meses/mes/total_total">
            <label>Total</label>
            <hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
