<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C.1.26- INGRESOS INTERCONEXION</h:title>
    <model>
      <instance>
        <C.1.26 id="C_1_26">
          <nota/>
          <cuadro1>
            <arriendo_1>0</arriendo_1>
            <transporte_1>0</transporte_1>
            <otros_1>0</otros_1>
            <calculate1/>
            <calculate_test_output1/>
          </cuadro1>
          <cuadro2>
            <arriendo_2>0</arriendo_2>
            <trafico_2>0</trafico_2>
            <corresponsalesEntrada_2>0</corresponsalesEntrada_2>
            <corresponsalesSalida_2>0</corresponsalesSalida_2>
            <netoTotal_2>0</netoTotal_2>
            <calculate2/>
            <calculate_test_output2/>
          </cuadro2>
          <meta>
            <instanceID/>
          </meta>
        </C.1.26>
      </instance>
      <bind nodeset="/C.1.26/nota" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro1/arriendo_1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro1/transporte_1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro1/otros_1" required="true()" type="int"/>
      <bind calculate=" /C.1.26/cuadro1/arriendo_1 + /C.1.26/cuadro1/transporte_1 + /C.1.26/cuadro1/otros_1 " nodeset="/C.1.26/cuadro1/calculate1" type="string"/>
      <bind nodeset="/C.1.26/cuadro1/calculate_test_output1" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro2/arriendo_2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro2/trafico_2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro2/corresponsalesEntrada_2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro2/corresponsalesSalida_2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.26/cuadro2/netoTotal_2" required="true()" type="int"/>
      <bind calculate=" /C.1.26/cuadro2/arriendo_2 + /C.1.26/cuadro2/trafico_2 + /C.1.26/cuadro2/corresponsalesEntrada_2 + /C.1.26/cuadro2/corresponsalesSalida_2 + /C.1.26/cuadro2/netoTotal_2 " nodeset="/C.1.26/cuadro2/calculate2" type="string"/>
      <bind nodeset="/C.1.26/cuadro2/calculate_test_output2" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C.1.26/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/C.1.26/nota">
      <label>Expresar en miles de pesos 12 dígitos</label>
    </input>
    <group ref="/C.1.26/cuadro1">
      <label>A LA RED FIJA -LARGA DISTANCIA NACIONAL A VINCULADAS-</label>
      <input ref="/C.1.26/cuadro1/arriendo_1">
        <label>Arriendo de Circuitos LDN</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro1/transporte_1">
        <label>Transporte LDN</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro1/otros_1">
        <label>Otros</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro1/calculate_test_output1">
        <label>Total: $ <output value=" /C.1.26/cuadro1/calculate1 "/></label></input>
    </group>
    <group ref="/C.1.26/cuadro2">
      <label>INTERNACIONAL A VINCULADAS</label>
      <input ref="/C.1.26/cuadro2/arriendo_2">
        <label>Arriendos Circuitos LDI</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro2/trafico_2">
        <label>Tráfico LDI</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro2/corresponsalesEntrada_2">
        <label>Corresponsales de Entrada</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro2/corresponsalesSalida_2">
        <label>Corresponsales de Salida</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro2/netoTotal_2">
        <label>Total Neto de Corresp. de Salida</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.26/cuadro2/calculate_test_output2">
        <label>Total: $ <output value=" /C.1.26/cuadro2/calculate2 "/></label></input>
    </group>
  </h:body>
</h:html>
