      <bind constraint="true()" nodeset="/C.1.2/localidad" required="true()" type="select1"/>
      <bind constraint="true()" nodeset="/C.1.2/localidad_otro" relevant=" /C.1.2/localidad ='otros1' or  /C.1.2/localidad ='otros2' or  /C.1.2/localidad ='otros3' or  /C.1.2/localidad ='otros4' or  /C.1.2/localidad ='otros5' or  /C.1.2/localidad ='otros6' or  /C.1.2/localidad ='otros7' or  /C.1.2/localidad ='otros8'" type="string"/>
