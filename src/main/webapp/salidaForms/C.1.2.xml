      <bind constraint="true()" nodeset="/C.1.2/medida" required="true()" type="select1"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$')" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C.1.2/cuit_prestador" required="true()" type="int"/>
