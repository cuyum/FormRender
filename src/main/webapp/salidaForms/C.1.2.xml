  <h:body>
    <select1 ref="/C.1.2/provincia">
      <label>Provincia/Región</label>
      <item>
        <label>Buenos Aires</label>
        <value>buenosAires</value>
      </item>
      <item>
        <label>Catamarca</label>
        <value>catamarca</value>
      </item>
    </select1>
    <select1 ref="/C.1.2/partido">
      <label>Partido/Dpto</label>
      <itemset nodeset="instance('partidos')/root/item[provincia= /C.1.2/provincia ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/C.1.2/localidad">
      <label>Localidad</label>
      <itemset nodeset="instance('localidades')/root/item[provincia= /C.1.2/provincia  and partido= /C.1.2/partido ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/C.1.2/localidad_otro">
      <label>Por favor especifique</label>
    </input>
    <input ref="/C.1.2/cantidad">
      <label>Capacidad Disponible</label>
      <hint>Ingrese número entero</hint>
    </input>
    <select1 appearance="minimal" ref="/C.1.2/medida">
      <label>Seleccione unidad correspondiente</label>
      <item>
        <label>Mbps</label>
        <value>mbps</value>
      </item>
      <item>
        <label>n*E1</label>
        <value>xxe1</value>
      </item>
    </select1>
    <input ref="/C.1.2/cuit_prestador">
      <label>CUIT Prestador</label>
      <hint>11 dígitos sin guiones ni espacios</hint>
    </input>
