<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C.1.25 - INGRESOS INTERCONEXION A LA RED FIJA -LOCAL A  VINCULADAS-</h:title>
    <model>
      <instance>
        <C.1.25 id="C_1_25">
          <nota/>
          <facilidades>
            <facilidades1>0</facilidades1>
            <facilidades2>0</facilidades2>
            <facilidades3>0</facilidades3>
            <facilidades4>0</facilidades4>
            <facilidades5>0</facilidades5>
            <facilidades6>0</facilidades6>
            <facilidades7>0</facilidades7>
            <facilidades8>0</facilidades8>
            <calculate/>
            <facilidades9/>
          </facilidades>
          <meta>
            <instanceID/>
          </meta>
        </C.1.25>
      </instance>
      <bind nodeset="/C.1.25/nota" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades3" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades4" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades5" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades6" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades7" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.25/facilidades/facilidades8" required="true()" type="int"/>
      <bind calculate=" /C.1.25/facilidades/facilidades1 + /C.1.25/facilidades/facilidades2 + /C.1.25/facilidades/facilidades3 + /C.1.25/facilidades/facilidades4 + /C.1.25/facilidades/facilidades5 + /C.1.25/facilidades/facilidades6 + /C.1.25/facilidades/facilidades7 + /C.1.25/facilidades/facilidades8 " nodeset="/C.1.25/facilidades/calculate" type="string"/>
      <bind nodeset="/C.1.25/facilidades/facilidades9" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C.1.25/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/C.1.25/nota">
      <label>Expresar en miles de pesos 12 dígitos</label>
    </input>
    <group ref="/C.1.25/facilidades">
      <label>Facilidades Esenciales y Otros Servicios</label>
      <input ref="/C.1.25/facilidades/facilidades1">
        <label>Origen y Terminación Local Fijo</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades2">
        <label>Origen y Terminación Móvil</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades3">
        <label>Puertos</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades4">
        <label>Coubicación</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades5">
        <label>Tránsito Local</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades6">
        <label>Otras Facilidades Esenciales</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades7">
        <label>Facturación y Cobranzas</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades8">
        <label>Otros</label>
        <hint>En miles de pesos, 12 dígitos</hint>
      </input>
      <input ref="/C.1.25/facilidades/facilidades9">
        <label>TOTAL: $ <output value=" /C.1.25/facilidades/calculate "/></label></input>
    </group>
  </h:body>
</h:html>
