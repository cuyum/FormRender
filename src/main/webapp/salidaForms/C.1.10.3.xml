  <h:body>
    <input ref="/C.1.10.3/my_int2">
      <label>Prestador</label>
      <hint>11 dígitos sin guiones ni espacios</hint>
    </input>
    <select1 ref="/C.1.10.3/provincia">
      <label>Provincia/Región</label>
      <item>
        <label>Buenos Aires</label>
        <value>buenosAires</value>
      </item>
      <item>
        <label>Catamarca</label>
        <value>catamarca</value>
      </item>
    </select1>
    <select1 ref="/C.1.10.3/partido">
      <label>Partido/Dpto</label>
      <itemset nodeset="instance('partidos')/root/item[provincia= /C.1.10.3/provincia ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/C.1.10.3/localidad">
      <label>Localidad</label>
      <itemset nodeset="instance('localidades')/root/item[provincia= /C.1.10.3/provincia  and partido= /C.1.10.3/partido ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/C.1.10.3/areasLocal">
      <label>Área Local</label>
      <hint>Seleccione Área Local</hint>
      <itemset nodeset="instance('areas')/root/item[provincia= /C.1.10.3/provincia ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/C.1.10.3/centros_atencion">
      <label>Centros de Atención</label>
      <hint>Ingrese cantidad</hint>
    </input>
    <input ref="/C.1.10.3/personal">
      <label>Personal</label>
      <hint>Ingrese cantidad</hint>
    </input>
    <input ref="/C.1.10.3/espera_promedio">
      <label>Espera Promedio</label>
      <hint>Ingrese promedio</hint>
    </input>
  </h:body>
