<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.1.4 A Cuadro A - IROAU</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <ict4.1.4.a id="ICT4.1.4.A">
          <meses jr:template="">
            <mes>
              <cant_llamada_perdida/>
              <cant_req_atencion/>
              <porcentaje/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </ict4.1.4.a>
      </instance>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.4.a/meses/mes/cant_llamada_perdida" required="true()" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.4.a/meses/mes/cant_req_atencion" required="true()" type="int"/>
      <bind nodeset="/ict4.1.4.a/meses/mes/porcentaje" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.1.4.a/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.1.4.a/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.1.4.a/meses">
        <group appearance="col3 well-white variable" ref="/ict4.1.4.a/meses/mes">
          <label>{title}</label>
          <input appearance="numerador" ref="/ict4.1.4.a/meses/mes/cant_llamada_perdida">
            <label>Cant. llamadas atendidas por operador humano dentro de los 30&quot;</label>
            <hint>Cantidad de llamadas atendidas por un operador humano dentro de los 30&quot; de requerida esa opción</hint>
          </input>
          <input appearance="denominador" ref="/ict4.1.4.a/meses/mes/cant_req_atencion">
            <label>Cant. de requerimientos de atención efectuados</label>
            <hint>Cantidad total de requerimientos de atención efectuados</hint>
          </input>
          <input appearance="porcentaje" ref="/ict4.1.4.a/meses/mes/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
