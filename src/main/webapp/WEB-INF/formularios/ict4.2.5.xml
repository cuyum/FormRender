<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.5-A Tasa de Entrega de Mensajes Cortos - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <ict4.2.5 id="ICT4.2.5">
          <trimestres jr:template="">
            <trimestre>
              <periodo_considerado/>
              <cantidad_mensajes_cortos/>
              <cantidad_total_mensajes/>
              <porcentaje/>
            </trimestre>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.5>
      </instance>
      <bind nodeset="/ict4.2.5/trimestres/trimestre/periodo_considerado" required="true()" type="select1"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.5/trimestres/trimestre/cantidad_mensajes_cortos" required="true()" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.5/trimestres/trimestre/cantidad_total_mensajes" required="true()" type="int"/>
      <bind constraint="porcentual= /ict4.2.5/trimestres/trimestre/cantidad_mensajes_cortos , /ict4.2.5/trimestres/trimestre/cantidad_total_mensajes ,100" nodeset="/ict4.2.5/trimestres/trimestre/porcentaje" readonly="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.5/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.5/trimestres">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.2.5/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.5/trimestres/trimestre">
          <label>Relación entre la Cant. de mensajes cortos entregados por el SMSC y la Cant. total de mensajes cortos recibidos por el SMSC</label>
          <select1 appearance="minimal" ref="/ict4.2.5/trimestres/trimestre/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>1º Trimestre</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>2º Trimestre</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>3º trimestre</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>4º trimestre</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <input ref="/ict4.2.5/trimestres/trimestre/cantidad_mensajes_cortos">
            <label>Cant. de mensajes cortos entregados por el SMSC</label>
            <hint>Cantidad de mensajes cortos entregados por el SMSC</hint>
          </input>
          <input ref="/ict4.2.5/trimestres/trimestre/cantidad_total_mensajes">
            <label>Cant. total de mensajes cortos recibidos por el SMSC</label>
            <hint>Cantidad total de mensajes cortos recibidos por el SMSC</hint>
          </input>
          <input ref="/ict4.2.5/trimestres/trimestre/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
