<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.3 - Redes e Infraestructura</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <D1.3 id="D1.3">
          <titulo/>
          <puntos jr:template="">
            <ubicacionOrigen>
              <provinciaOrigen/>
              <partidoOrigen/>
              <partidoOrigen_otro/>
              <localidadOrigen/>
              <localidadOrigen_otro/>
            </ubicacionOrigen>
            <equipamiento>
              <tecnologia/>
              <tecnologia_otro/>
              <equipCapacidadDisp/>
              <equipCapacidadUtilizada/>
            </equipamiento>
            <tipomedio>
              <medio/>
              <medio_otro/>
              <medioCapacidadDisp/>
              <medioCapacidadUtilizada/>
            </tipomedio>
            <tipotendido>
              <tendido/>
              <tendido_otro/>
              <tendidoCapacidadDisp/>
              <tendidoCapacidadUtilizada/>
            </tipotendido>
            <ubicacionDestino>
              <provinciaDestino/>
              <partidoDestino/>
              <partidoDestino_otro/>
              <localidadDestino/>
              <localidadDestino_otro/>
            </ubicacionDestino>
          </puntos>
          <meta>
            <instanceID/>
          </meta>
        </D1.3>
      </instance>
      <bind nodeset="/D1.3/titulo" readonly="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/D1.3/puntos/ubicacionOrigen/provinciaOrigen" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/D1.3/puntos/ubicacionOrigen/provinciaOrigen" nodeset="/D1.3/puntos/ubicacionOrigen/partidoOrigen" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/ubicacionOrigen/partidoOrigen_otro" relevant=" /D1.3/puntos/ubicacionOrigen/partidoOrigen =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/D1.3/puntos/ubicacionOrigen/partidoOrigen" nodeset="/D1.3/puntos/ubicacionOrigen/localidadOrigen" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/ubicacionOrigen/localidadOrigen_otro" relevant=" /D1.3/puntos/ubicacionOrigen/localidadOrigen =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/listas/tecnologia" nodeset="/D1.3/puntos/equipamiento/tecnologia" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/equipamiento/tecnologia_otro" relevant=" /D1.3/puntos/equipamiento/tecnologia =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=9999999" jr:constraintMsg="Ingrese entero entre 0 y 9999999" nodeset="/D1.3/puntos/equipamiento/equipCapacidadDisp" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=9999999" jr:constraintMsg="Ingrese entero entre 0 y 9999999" nodeset="/D1.3/puntos/equipamiento/equipCapacidadUtilizada" required="true()" type="int"/>
      <bind constraint="url=/servicios/rest/listas/medio" nodeset="/D1.3/puntos/tipomedio/medio" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/tipomedio/medio_otro" relevant=" /D1.3/puntos/tipomedio/medio =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=99999" jr:constraintMsg="Ingrese entero entre 0 y 99999" nodeset="/D1.3/puntos/tipomedio/medioCapacidadDisp" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999" jr:constraintMsg="Ingrese entero entre 0 y 99999" nodeset="/D1.3/puntos/tipomedio/medioCapacidadUtilizada" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind nodeset="/D1.3/puntos/tipotendido" relevant=" /D1.3/puntos/tipomedio/medio =FO or  /D1.3/puntos/tipomedio/medio =Otros"/>
      <bind constraint="url=/servicios/rest/listas/tendido" nodeset="/D1.3/puntos/tipotendido/tendido" relevant=" /D1.3/puntos/tipomedio/medio =FO or  /D1.3/puntos/tipomedio/medio =Otros" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/tipotendido/tendido_otro" relevant=" /D1.3/puntos/tipotendido/tendido =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999" jr:constraintMsg="Ingrese entero entre 0 y 999" nodeset="/D1.3/puntos/tipotendido/tendidoCapacidadDisp" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999" jr:constraintMsg="Ingrese entero entre 0 y 999" nodeset="/D1.3/puntos/tipotendido/tendidoCapacidadUtilizada" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/D1.3/puntos/ubicacionDestino/provinciaDestino" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/D1.3/puntos/ubicacionDestino/provinciaDestino" nodeset="/D1.3/puntos/ubicacionDestino/partidoDestino" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/ubicacionDestino/partidoDestino_otro" relevant=" /D1.3/puntos/ubicacionDestino/partidoDestino =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/D1.3/puntos/ubicacionDestino/partidoDestino" nodeset="/D1.3/puntos/ubicacionDestino/localidadDestino" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/ubicacionDestino/localidadDestino_otro" relevant=" /D1.3/puntos/ubicacionDestino/localidadDestino =9999" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.3/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input appearance="tooltip" ref="/D1.3/titulo">
      <hint>Se deben informar redes e infraestructura propia. Debe definir para cada uno de sus enlaces: punto de inicio, de fin, equipamiento, medio y tipo de tendido; de corresponder.</hint>
    </input>
    <group ref="/D1.3/puntos">
      <label></label>
      <repeat appearance="grilla" nodeset="/D1.3/puntos">
        <group appearance="col3 well-white" ref="/D1.3/puntos/ubicacionOrigen">
          <label>Ubicación Origen</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionOrigen/provinciaOrigen">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionOrigen/partidoOrigen">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/ubicacionOrigen/partidoOrigen_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionOrigen/localidadOrigen">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/ubicacionOrigen/localidadOrigen_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/equipamiento">
          <label>Equipamiento</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/equipamiento/tecnologia">
            <label>Tecnología</label>
            <hint>Elegir desde la tabla el tipo de tecnología implementada. En el caso de no figurar, elegir la opción &quot;otros&quot; y completar manualmente.</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            
          </select1>
          <input ref="/D1.3/puntos/equipamiento/tecnologia_otro">
            <label>Tec. Otros</label>
          </input>
          <input ref="/D1.3/puntos/equipamiento/equipCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Completar la capacidad máxima que es capaz de manejar el equipo en Mbps.</hint>
          </input>
          <input ref="/D1.3/puntos/equipamiento/equipCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Completar la capacidad utilizada [Mbps]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/tipomedio">
          <label>Medio</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/tipomedio/medio">
            <label>Tipo</label>
            <hint>Completar el medio de transmisión, elegir desde tabla. Para enlaces de FO, completar la cantidad de hilos instalados y la cantidad en uso.</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            
          </select1>
          <input ref="/D1.3/puntos/tipomedio/medio_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/D1.3/puntos/tipomedio/medioCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de pelos]</hint>
          </input>
          <input ref="/D1.3/puntos/tipomedio/medioCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de pelos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/tipotendido">
          <label>Tendido</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/tipotendido/tendido">
            <label>Tipo</label>
            <hint>Para el caso de FO, completar el tipo, la cantidad de ductos instalados y la cantidad de ductos en uso.</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            
          </select1>
          <input ref="/D1.3/puntos/tipotendido/tendido_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/D1.3/puntos/tipotendido/tendidoCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de ductos]</hint>
          </input>
          <input ref="/D1.3/puntos/tipotendido/tendidoCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de ductos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/ubicacionDestino">
          <label>Ubicación Destino</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionDestino/provinciaDestino">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionDestino/partidoDestino">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/ubicacionDestino/partidoDestino_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/D1.3/puntos/ubicacionDestino/localidadDestino">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/ubicacionDestino/localidadDestino_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
