<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.4.1-A Tasa de Cumplimiento de Tiempo de Transmisión - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <ict4.2.4.1 id="ICT4.2.4.1">
          <trimestres jr:template="">
            <relacion>
              <periodo_considerado/>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <hora_pico_inicio/>
              <hora_pico_fin/>
              <tipo_informacion/>
              <cant_paquetes/>
              <cant_total_paquetes_recibidos/>
              <porcentaje/>
            </relacion>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.4.1>
      </instance>
      <bind constraint="periodicidad=/servicios/rest/periodicidad and agrupador=1" nodeset="/ict4.2.4.1/trimestres/relacion/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/ict4.2.4.1/trimestres/relacion/provincia" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/ict4.2.4.1/trimestres/relacion/provincia" nodeset="/ict4.2.4.1/trimestres/relacion/partido" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.1/trimestres/relacion/partido_otro" relevant=" /ict4.2.4.1/trimestres/relacion/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/ict4.2.4.1/trimestres/relacion/partido" nodeset="/ict4.2.4.1/trimestres/relacion/localidad" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.1/trimestres/relacion/localidad_otro" relevant=" /ict4.2.4.1/trimestres/relacion/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/areas2 and depends=/ict4.2.4.1/trimestres/relacion/localidad and agrupador=2" nodeset="/ict4.2.4.1/trimestres/relacion/area" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.1/trimestres/relacion/area_otro" relevant=" /ict4.2.4.1/trimestres/relacion/area =9999" required="true()" type="string"/>
      <bind constraint="agrupador=3" nodeset="/ict4.2.4.1/trimestres/relacion/hora_pico_inicio" required="true()" type="time"/>
      <bind constraint="url=/servicios/rest/parametros/minuto_hora and hora_delta= /ict4.2.4.1/trimestres/relacion/hora_pico_inicio " nodeset="/ict4.2.4.1/trimestres/relacion/hora_pico_fin" readonly="true()" type="time"/>
      <bind constraint="agrupador=4" nodeset="/ict4.2.4.1/trimestres/relacion/tipo_informacion" required="true()" type="select1"/>
      <bind constraint=".&gt;=0 and totalizador" nodeset="/ict4.2.4.1/trimestres/relacion/cant_paquetes" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and totalizador" nodeset="/ict4.2.4.1/trimestres/relacion/cant_total_paquetes_recibidos" required="true()" type="int"/>
      <bind constraint="porcentual= /ict4.2.4.1/trimestres/relacion/cant_paquetes , /ict4.2.4.1/trimestres/relacion/cant_total_paquetes_recibidos ,100" nodeset="/ict4.2.4.1/trimestres/relacion/porcentaje" readonly="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.4.1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.4.1/trimestres">
      <label></label>
      <repeat appearance="grilla calculados" nodeset="/ict4.2.4.1/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.4.1/trimestres/relacion">
          <label>Relación entre la cantidad de paquetes recibidos en un tiempo T1 y el total de paquetes recibidos</label>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/partido">
            <label>Departamento / Partido</label>
            <hint>Departamento / Partido</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.1/trimestres/relacion/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.1/trimestres/relacion/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.1/trimestres/relacion/area_otro">
            <label>Área Otros</label>
            <hint>Área Otros</hint>
          </input>
          <input ref="/ict4.2.4.1/trimestres/relacion/hora_pico_inicio">
            <label>Hora Pico Inicio</label>
            <hint>Hora Pico Inicio</hint>
          </input>
          <input ref="/ict4.2.4.1/trimestres/relacion/hora_pico_fin">
            <label>Hora Pico Fin</label>
            <hint>Hora Pico Fin</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.1/trimestres/relacion/tipo_informacion">
            <label>Tipo de información</label>
            <hint>Tipo de información</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Audio Bidireccional</label>
              <value>Audio Bidireccional</value>
            </item>
            <item>
              <label>Audio</label>
              <value>Audio</value>
            </item>
            <item>
              <label>Video Bidireccional</label>
              <value>Video Bidireccional</value>
            </item>
            <item>
              <label>Video Unidireccional</label>
              <value>Video Unidireccional</value>
            </item>
            <item>
              <label>Datos</label>
              <value>Datos</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.1/trimestres/relacion/cant_paquetes">
            <label>Cant. de paquetes recibidos dentro de un tiempo (T1)</label>
            <hint>Cantidad de paquetes recibidos dentro de un tiempo (T1)</hint>
          </input>
          <input ref="/ict4.2.4.1/trimestres/relacion/cant_total_paquetes_recibidos">
            <label>Cant. total de paquetes recibidos</label>
            <hint>Cantidad total de paquetes recibidos</hint>
          </input>
          <input ref="/ict4.2.4.1/trimestres/relacion/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
