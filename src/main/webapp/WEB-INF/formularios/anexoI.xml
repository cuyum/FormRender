    </group>
    <group ref="/anexoI/Texto-5">
      <input ref="/anexoI/Texto-5/despiece">
        <label>Despiece:</label>
      </input>
    </group>
    <group ref="/anexoI/Texto-6">
      <input ref="/anexoI/Texto-6/tolerancia">
        <label>Tolerancia:</label>
      </input>
    </group>
    <group ref="/anexoI/suma">
      <label></label>
