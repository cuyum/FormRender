<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Anexo I</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <anexoI id="anexoI">
          <info>
            <titulo/>
          </info>
          <solicitud_gastos>
            <nro_solicitud/>
            <nombre_organismo_contratante/>
            <saf/>
            <cuit/>
            <expediente_nro/>
          </solicitud_gastos>
          <solicitud_gastos2>
            <actividad_interna/>
            <delegacion/>
            <unidad_solicitante/>
            <rubro_comercial/>
            <modalidad_compra_sugerida/>
            <fecha_inicio_sugerida/>
            <fecha_fin_vigencia_sugerida/>
            <clase/>
          </solicitud_gastos2>
          <solicitud_gastos3>
            <objeto/>
          </solicitud_gastos3>
          <solicitud_gastos4>
            <prioridad_justificacion_necesidad/>
          </solicitud_gastos4>
          <solicitud_gastos5>
            <fecha_entrega/>
            <lugar_entrega/>
          </solicitud_gastos5>
          <solicitud_gastos6>
            <jurisdiccion/>
            <subju/>
            <ent/>
            <programa/>
            <subprog/>
            <proy/>
            <activ/>
            <ob/>
            <descripcion_act_prog/>
            <ejerc_inc/>
            <ppr/>
            <ppa/>
            <spa/>
            <fuente_financiamiento/>
            <ub_geografica/>
            <mon/>
            <importe/>
          </solicitud_gastos6>
          <solicitud_gastos7>
            <causas_contratacion_directa/>
          </solicitud_gastos7>
          <bienes jr:template="">
            <bienes_y_servicios_solicitados>
              <codigo_de_catalogo/>
              <descripcion/>
              <unidad_de_medida/>
              <cantidad/>
              <importe_unitario/>
              <importe_total/>
            </bienes_y_servicios_solicitados>
          </bienes>
          <texto-1>
            <datos_servicio/>
          </texto-1>
          <Texto-2>
            <observaciones_propias/>
          </Texto-2>
          <Texto-3>
            <observaciones_catalogo/>
          </Texto-3>
          <Texto-4>
            <especificacion_tecnica/>
          </Texto-4>
          <Texto-5>
            <despiece/>
          </Texto-5>
          <Texto-6>
            <tolerancia/>
          </Texto-6>
          <suma jr:template="">
            <suma2>
              <fecha_entrega/>
              <descripcion/>
              <unidad_de_medida/>
              <cantidad/>
              <importe_unitario/>
            </suma2>
          </suma>
          <resultado_suma>
            <suma_de/>
            <total_suma/>
          </resultado_suma>
          <nota>
            <obsAnexoC/>
          </nota>
          <Texto-7>
            <solicitante/>
            <firma_y_aclaracion/>
          </Texto-7>
          <Texto-8>
            <fecha_autorizacion/>
            <firma_y_aclaracion_autorizacion/>
          </Texto-8>
          <meta>
            <instanceID/>
          </meta>
        </anexoI>
      </instance>
      <bind nodeset="/anexoI/info/titulo" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos/nro_solicitud" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos/nombre_organismo_contratante" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos/saf" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos/cuit" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos/expediente_nro" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/actividad_interna" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/delegacion" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/unidad_solicitante" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/rubro_comercial" required="true()" type="select1"/>
      <bind nodeset="/anexoI/solicitud_gastos2/modalidad_compra_sugerida" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/fecha_inicio_sugerida" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/fecha_fin_vigencia_sugerida" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos2/clase" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos3/objeto" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos4/prioridad_justificacion_necesidad" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos5/fecha_entrega" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos5/lugar_entrega" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/jurisdiccion" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/subju" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ent" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/programa" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/subprog" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/proy" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/activ" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ob" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/descripcion_act_prog" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ejerc_inc" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ppr" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ppa" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/spa" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/fuente_financiamiento" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/ub_geografica" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/mon" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos6/importe" required="true()" type="string"/>
      <bind nodeset="/anexoI/solicitud_gastos7/causas_contratacion_directa" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/codigo_de_catalogo" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/descripcion" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/unidad_de_medida" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/cantidad" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/importe_unitario" required="true()" type="string"/>
      <bind nodeset="/anexoI/bienes/bienes_y_servicios_solicitados/importe_total" required="true()" type="string"/>
      <bind nodeset="/anexoI/texto-1/datos_servicio" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-2/observaciones_propias" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-3/observaciones_catalogo" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-4/especificacion_tecnica" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-5/despiece" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-6/tolerancia" required="true()" type="string"/>
      <bind nodeset="/anexoI/suma/suma2/fecha_entrega" required="true()" type="string"/>
      <bind nodeset="/anexoI/suma/suma2/descripcion" required="true()" type="string"/>
      <bind nodeset="/anexoI/suma/suma2/unidad_de_medida" required="true()" type="string"/>
      <bind nodeset="/anexoI/suma/suma2/cantidad" required="true()" type="string"/>
      <bind nodeset="/anexoI/suma/suma2/importe_unitario" required="true()" type="string"/>
      <bind nodeset="/anexoI/resultado_suma/suma_de" required="true()" type="string"/>
      <bind nodeset="/anexoI/resultado_suma/total_suma" required="true()" type="string"/>
      <bind nodeset="/anexoI/nota/obsAnexoC" type="string"/>
      <bind nodeset="/anexoI/Texto-7/solicitante" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-7/firma_y_aclaracion" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-8/fecha_autorizacion" required="true()" type="string"/>
      <bind nodeset="/anexoI/Texto-8/firma_y_aclaracion_autorizacion" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/anexoI/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/anexoI/info">
        <label>FORMATO ESTANDAR DE SOLICITUD DE GASTOS</label>
    </group>
    <group appearance="col4 well-white" ref="/anexoI/solicitud_gastos">
      <label>SOLICITUD DE GASTOS</label>
      <input ref="/anexoI/solicitud_gastos/nro_solicitud">
        <label>Nº de Solicitud:</label>
      </input>
      <input ref="/anexoI/solicitud_gastos/nombre_organismo_contratante">
        <label>Nombre del organismo contratante</label>
      </input>
      <input ref="/anexoI/solicitud_gastos/saf">
        <label>SAF:</label>
      </input>
      <input ref="/anexoI/solicitud_gastos/cuit">
        <label>CUIT:</label>
      </input>
      <input ref="/anexoI/solicitud_gastos/expediente_nro">
        <label>EXPEDIENTE Nº:</label>
      </input>
    </group>
    <group ref="/anexoI/solicitud_gastos2">
      <input ref="/anexoI/solicitud_gastos2/actividad_interna">
        <label>ACTIVIDAD INTERNA:</label>
      </input>
