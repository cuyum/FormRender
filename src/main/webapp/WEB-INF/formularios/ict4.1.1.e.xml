<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.1.1-E Indicador de reclamos de usuarios - Cuadro E</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <ict4.1.1.e id="ICT4.1.1.e">
          <meses jr:template="">
            <mes>
              <periodo_considerado/>
              <canales/>
              <canales_otros/>
              <id_bocas/>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <tiempo_espera/>
              <personal_afectado_reclamos/>
              <personal_afectado_otras/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </ict4.1.1.e>
      </instance>
      <bind constraint="url=/servicios/rest/periodicidad and agrupador=1" nodeset="/ict4.1.1.e/meses/mes/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="agrupador=2" nodeset="/ict4.1.1.e/meses/mes/canales" required="true()" type="select1"/>
      <bind nodeset="/ict4.1.1.e/meses/mes/canales_otros" relevant=" /ict4.1.1.e/meses/mes/canales =Otros" required="true()" type="string"/>
      <bind nodeset="/ict4.1.1.e/meses/mes/id_bocas" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/ict4.1.1.e/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/ict4.1.1.e/meses/mes/provincia" nodeset="/ict4.1.1.e/meses/mes/partido" required="true()" type="select1"/>
      <bind nodeset="/ict4.1.1.e/meses/mes/partido_otro" relevant=" /ict4.1.1.e/meses/mes/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/ict4.1.1.e/meses/mes/partido" nodeset="/ict4.1.1.e/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/ict4.1.1.e/meses/mes/localidad_otro" relevant=" /ict4.1.1.e/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.1.e/meses/mes/tiempo_espera" required="true()" type="decimal"/>
      <bind constraint=".&gt;=0 and totalizador" nodeset="/ict4.1.1.e/meses/mes/personal_afectado_reclamos" required="true()" type="int"/>
      <bind constraint=".&gt;0 and totalizador" nodeset="/ict4.1.1.e/meses/mes/personal_afectado_otras" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.1.1.e/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.1.1.e/meses">
      <label></label>
      <repeat appearance="grilla sumarizada calculados" nodeset="/ict4.1.1.e/meses">
        <group appearance="col4 well-white" ref="/ict4.1.1.e/meses/mes">
          <label>Información sobre los Canales de Atención al Usuario</label>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/canales">
            <label>Canales de Atención al Usuario</label>
            <hint>Canales de Atención al Usuario</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Atención Telefónica</label>
              <value>Atencion_Telefonica</value>
            </item>
            <item>
              <label>Atención Personalizada - Sucursal</label>
              <value>Atencion_Personalizada_Sucursal</value>
            </item>
            <item>
              <label>Atención en Redes Sociales / Web / Mail / Postal u otros medios Escritos</label>
              <value>Atencion_en_Redes_Sociales</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/ict4.1.1.e/meses/mes/canales_otros">
            <label>Canales Otros</label>
            <hint>Canales de Atencion al usuario Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/id_bocas">
            <label>ID Bocas de atención</label>
            <hint>ID Bocas de atención</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Call Center 1</label>
              <value>Call Center 1</value>
            </item>
            <item>
              <label>Call Center 2</label>
              <value>Call Center 2</value>
            </item>
            <item>
              <label>Área 1</label>
              <value>Área 1</value>
            </item>
            <item>
              <label>Sucursal 1</label>
              <value>Sucursal 1</value>
            </item>
            <item>
              <label>Sucursal 2</label>
              <value>Sucursal 2</value>
            </item>
            <item>
              <label>Sucursal 3</label>
              <value>Sucursal 3</value>
            </item>
            <item>
              <label>Sucursal 4</label>
              <value>Sucursal 4</value>
            </item>
            <item>
              <label>Sucursal 5</label>
              <value>Sucursal 5</value>
            </item>
            <item>
              <label>Sucursal 6</label>
              <value>Sucursal 6</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/partido">
            <label>Departamento / Partido</label>
            <hint>Departamento / Partido</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.1.1.e/meses/mes/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.1.1.e/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.1.1.e/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <input ref="/ict4.1.1.e/meses/mes/tiempo_espera">
            <label>Espera promedio en atención al cliente</label>
            <hint>Tiempo de espera promedio en atención al cliente</hint>
          </input>
          <input ref="/ict4.1.1.e/meses/mes/personal_afectado_reclamos">
            <label>Pers. afectado a la recep. de Reclamos</label>
            <hint>Personal afectado a la recepción de Reclamos</hint>
          </input>
          <input ref="/ict4.1.1.e/meses/mes/personal_afectado_otras">
            <label>Personal afectado a otras tareas</label>
            <hint>Personal afectado a otras tareas</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
