      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/tierra_Fuego" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/tucuman" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C2.6/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input appearance="tooltip" ref="/C2.6/titulo">
      <hint>Toda la información económica-financiera debe ser completada en base a los mismos criterios utilizados en los EECC.</hint>
    </input>
    <group ref="/C2.6/meses">
      <label></label>
      <repeat nodeset="/C2.6/meses">
        <group appearance="col6 well-white variable" ref="/C2.6/meses/mes">
          <label>{title}</label>
          <input ref="/C2.6/meses/mes/caba">
