<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C2.6 - Ingresos por provincia</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <C2.6 id="C2.6">
          <titulo/>
          <meses jr:template="">
            <mes>
              <caba/>
              <buenos_Aires/>
              <catamarca/>
              <chaco/>
              <chubut/>
              <cordoba/>
              <corrientes/>
              <entre_rios/>
              <formosa/>
              <jujuy/>
              <la_Pampa/>
              <la_Rioja/>
              <mendoza/>
              <misiones/>
              <neuquen/>
              <rio_Negro/>
              <salta/>
              <san_Juan/>
              <san_Luis/>
              <santa_Cruz/>
              <santa_Fe/>
              <santiago_Estero/>
              <tierra_Fuego/>
              <tucuman/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C2.6>
      </instance>
      <bind nodeset="/C2.6/titulo" readonly="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/caba" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/buenos_Aires" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/catamarca" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/chaco" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/chubut" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/cordoba" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/corrientes" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/entre_rios" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/formosa" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/jujuy" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/la_Pampa" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/la_Rioja" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/mendoza" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/misiones" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/neuquen" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/rio_Negro" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/salta" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/san_Juan" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/san_Luis" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999999" jr:constraintMsg="Ingrese entero entre 0 y 999999999999" nodeset="/C2.6/meses/mes/santa_Cruz" required="true()" type="int"/>
