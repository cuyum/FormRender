<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.1.6-C Indicador sobre Reclamos de Facturación - Cuadro C</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <ict4.1.6.c id="ICT4.1.6.C">
          <meses jr:template="">
            <mes>
              <periodo_considerado/>
              <provincia/>
              <cant_fact_emitidas/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </ict4.1.6.c>
      </instance>
      <bind nodeset="/ict4.1.6.c/meses/mes/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/ict4.1.6.c/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.6.c/meses/mes/cant_fact_emitidas" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.1.6.c/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.1.6.c/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.1.6.c/meses">
        <group appearance="col3 well-white" ref="/ict4.1.6.c/meses/mes">
          <label>Facturas Emitidas por Provincia</label>
          <select1 appearance="minimal" ref="/ict4.1.6.c/meses/mes/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>Trimestre_1</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>Trimestre_2</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>Trimestre_3</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>Trimestre_4</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.1.6.c/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.1.6.c/meses/mes/cant_fact_emitidas">
            <label>Cant. de Facturas Emitidas</label>
            <hint>Cantidad de Facturas Emitidas</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
