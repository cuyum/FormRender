<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.2.1-A Eficiencia de llamada - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <ict4.2.2.1-a id="ICT4.2.2.1-A">
          <trimestres jr:template="">
            <trimestre>
              <periodo_considerado/>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <hora_pico_inicio/>
              <hora_pico_fin/>
              <cantidad_llamadas/>
              <cantidad_intentos/>
              <porcentaje/>
            </trimestre>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.2.1-a>
      </instance>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/ict4.2.2.1-a/trimestres/trimestre/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/ict4.2.2.1-a/trimestres/trimestre/provincia" nodeset="/ict4.2.2.1-a/trimestres/trimestre/partido" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/partido_otro" relevant=" /ict4.2.2.1-a/trimestres/trimestre/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/ict4.2.2.1-a/trimestres/trimestre/partido" nodeset="/ict4.2.2.1-a/trimestres/trimestre/localidad" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/localidad_otro" relevant=" /ict4.2.2.1-a/trimestres/trimestre/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/areas and depends=/ict4.2.2.1-a/trimestres/trimestre/localidad" nodeset="/ict4.2.2.1-a/trimestres/trimestre/area" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/area_otro" relevant=" /ict4.2.2.1-a/trimestres/trimestre/area =9999" required="true()" type="string"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/hora_pico_inicio" required="true()" type="string"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/hora_pico_fin" required="true()" type="string"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.2.1-a/trimestres/trimestre/cantidad_llamadas" required="true()" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.2.1-a/trimestres/trimestre/cantidad_intentos" required="true()" type="int"/>
      <bind nodeset="/ict4.2.2.1-a/trimestres/trimestre/porcentaje" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.2.1-a/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.2.1-a/trimestres">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.2.2.1-a/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.2.1-a/trimestres/trimestre">
          <label>Relación entre la cantidad de llamadas completadas y el total de intentos</label>
          <select1 appearance="minimal" ref="/ict4.2.2.1-a/trimestres/trimestre/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>1º Trimestre</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>2º Trimestre</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>3º trimestre</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>4º trimestre</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.2.1-a/trimestres/trimestre/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.2.1-a/trimestres/trimestre/partido">
            <label>Departamento / Partido</label>
            <hint>Departamento / Partido</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2.1-a/trimestres/trimestre/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.2.1-a/trimestres/trimestre/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2.1-a/trimestres/trimestre/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.2.1-a/trimestres/trimestre/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2.1-a/trimestres/trimestre/area_otro">
            <label>Área Otros</label>
            <hint>Área Otros</hint>
          </input>
          <input appearance="hour" ref="/ict4.2.2.1-a/trimestres/trimestre/hora_pico_inicio">
            <label>Hora Pico Inicio</label>
            <hint>Hora Pico Inicio</hint>
          </input>
          <input appearance="hour" ref="/ict4.2.2.1-a/trimestres/trimestre/hora_pico_fin">
            <label>Hora Pico Fin</label>
            <hint>Hora Pico Final</hint>
          </input>
          <input appearance="numerador" ref="/ict4.2.2.1-a/trimestres/trimestre/cantidad_llamadas">
            <label>Cant. de llamadas completadas</label>
            <hint>Cantidad de llamadas completadas originadas en el Área Local</hint>
          </input>
          <input appearance="denominador" ref="/ict4.2.2.1-a/trimestres/trimestre/cantidad_intentos">
            <label>Cant. de intentos de llamadas excluyendo las &quot;No Contesta u Ocupado&quot;</label>
            <hint>Cantidad de intentos de llamadas originadas en el Área Local excluyendo llamadas con retorno &quot;No Contesta u Ocupado&quot;</hint>
          </input>
          <input appearance="porcentaje" ref="/ict4.2.2.1-a/trimestres/trimestre/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
