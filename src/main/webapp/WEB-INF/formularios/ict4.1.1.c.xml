<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.1.1-C Indicador de reclamos de usuarios - Cuadro C</h:title>
    <model>
      <submission action="/api/deposition" method="form-data-post"/>
      <instance>
        <ict4.1.1.c id="ICT4.1.1.C">
          <meses jr:template="">
            <mes>
              <periodo_considerado/>
              <canales/>
              <canales_otros/>
              <cant_recl_ingresados/>
              <cant_total_contacto_rec/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </ict4.1.1.c>
      </instance>
      <bind nodeset="/ict4.1.1.c/meses/mes/periodo_considerado" required="true()" type="select1"/>
      <bind nodeset="/ict4.1.1.c/meses/mes/canales" required="true()" type="select1"/>
      <bind nodeset="/ict4.1.1.c/meses/mes/canales_otros" relevant=" /ict4.1.1.c/meses/mes/canales =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.1.c/meses/mes/cant_recl_ingresados" required="true()" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.1.1.c/meses/mes/cant_total_contacto_rec" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.1.1.c/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.1.1.c/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.1.1.c/meses">
        <group appearance="col3 well-white" ref="/ict4.1.1.c/meses/mes">
          <label>Canales de Atención al Usuario</label>
          <select1 appearance="minimal" ref="/ict4.1.1.c/meses/mes/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>Trimestre_1</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>Trimestre_2</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>Trimestre_3</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>Trimestre_4</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.1.1.c/meses/mes/canales">
            <label>Canales de Atención al Usuario</label>
            <hint>Canales de Atención al Usuario</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Atención Telefónica</label>
              <value>Atencion_Telefonica</value>
            </item>
            <item>
              <label>Atención Personalizada - Sucursal</label>
              <value>Atencion_Personalizada_Sucursal</value>
            </item>
            <item>
              <label>Atención en Redes Sociales / Web / Mail / Postal u otros medios Escritos</label>
              <value>Atencion_en_Redes_Sociales</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/ict4.1.1.c/meses/mes/canales_otros">
            <label>Canales Otros</label>
            <hint>Canales de Atencion al usuario Otros</hint>
          </input>
          <input ref="/ict4.1.1.c/meses/mes/cant_recl_ingresados">
            <label>Cant. de Reclamos ingresados</label>
            <hint>Cantidad de Reclamos ingresados</hint>
          </input>
          <input ref="/ict4.1.1.c/meses/mes/cant_total_contacto_rec">
            <label>Cant. total de Contactos recibidos</label>
            <hint>Cantidad total de Contactos recibidos</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
