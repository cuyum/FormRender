<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.2-A Accesibilidad del Servicio - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <ict4.2.2 id="ICT4.2.2">
          <trimestres jr:template="">
            <tasa>
              <periodo_considerado/>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <hora_pico_inicio/>
              <hora_pico_fin/>
              <cantidad_asignada_ex/>
              <cantidad_total_tent/>
              <porcentaje/>
            </tasa>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.2>
      </instance>
      <bind constraint="agrupador=1" nodeset="/ict4.2.2/trimestres/tasa/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/provincias" nodeset="/ict4.2.2/trimestres/tasa/provincia" required="true()" type="select1"/>
      <bind constraint="url=/servicios/rest/localizaciones/partidos and depends=/ict4.2.2/trimestres/tasa/provincia" nodeset="/ict4.2.2/trimestres/tasa/partido" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2/trimestres/tasa/partido_otro" relevant=" /ict4.2.2/trimestres/tasa/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/localidades and depends=/ict4.2.2/trimestres/tasa/partido" nodeset="/ict4.2.2/trimestres/tasa/localidad" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2/trimestres/tasa/localidad_otro" relevant=" /ict4.2.2/trimestres/tasa/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/servicios/rest/localizaciones/areas2 and depends=/ict4.2.2/trimestres/tasa/localidad and agrupador=2" nodeset="/ict4.2.2/trimestres/tasa/area" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.2/trimestres/tasa/area_otro" relevant=" /ict4.2.2/trimestres/tasa/area =9999" required="true()" type="string"/>
      <bind constraint="agrupador=3" nodeset="/ict4.2.2/trimestres/tasa/hora_pico_inicio" required="true()" type="time"/>
      <bind constraint="url=/servicios/rest/parametros/minuto_hora and hora_delta= /ict4.2.2/trimestres/tasa/hora_pico_inicio " nodeset="/ict4.2.2/trimestres/tasa/hora_pico_fin" readonly="true()" type="time"/>
      <bind constraint=".&gt;=0 and totalizador" nodeset="/ict4.2.2/trimestres/tasa/cantidad_asignada_ex" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and totalizador" nodeset="/ict4.2.2/trimestres/tasa/cantidad_total_tent" required="true()" type="int"/>
      <bind constraint="porcentual= /ict4.2.2/trimestres/tasa/cantidad_asignada_ex , /ict4.2.2/trimestres/tasa/cantidad_total_tent ,100" nodeset="/ict4.2.2/trimestres/tasa/porcentaje" readonly="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.2/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.2/trimestres">
      <label></label>
      <repeat appearance="grilla sumarizada calculado" nodeset="/ict4.2.2/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.2/trimestres/tasa">
          <label>Tasa de Asignación de Recursos (TAR)</label>
          <select1 appearance="minimal" ref="/ict4.2.2/trimestres/tasa/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>Trimestre_1</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>Trimestre_2</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>Trimestre_3</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>Trimestre_4</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.2/trimestres/tasa/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.2/trimestres/tasa/partido">
            <label>Departamento / Partido</label>
            <hint>Departamento / Partido</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2/trimestres/tasa/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.2/trimestres/tasa/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2/trimestres/tasa/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.2/trimestres/tasa/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.2/trimestres/tasa/area_otro">
            <label>Área Otros</label>
            <hint>Área Otros</hint>
          </input>
          <input ref="/ict4.2.2/trimestres/tasa/hora_pico_inicio">
            <label>Hora Pico Inicio</label>
            <hint>Hora Pico Inicio</hint>
          </input>
          <input ref="/ict4.2.2/trimestres/tasa/hora_pico_fin">
            <label>Hora Pico Fin</label>
            <hint>Hora Pico Final</hint>
          </input>
          <input ref="/ict4.2.2/trimestres/tasa/cantidad_asignada_ex">
            <label>Cant. de asignaciones exitosas de los recursos</label>
            <hint>Cantidad de asignaciones exitosas de los recursos necesarios para gestionar la comunicación</hint>
          </input>
          <input ref="/ict4.2.2/trimestres/tasa/cantidad_total_tent">
            <label>Cant. de tent. de asignación de recursos realizadas</label>
            <hint>Cantidad total de tentativas de asignación de recursos realizadas</hint>
          </input>
          <input ref="/ict4.2.2/trimestres/tasa/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
