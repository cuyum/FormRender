<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario 4.2.4.2-A Tasa de Pérdida de Paquetes - Cuadro A</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <ict4.2.4.2 id="ICT4.2.4.2">
          <trimestres jr:template="">
            <relacion>
              <periodo_considerado/>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <hora_pico_inicio/>
              <hora_pico_fin/>
              <tipo_informacion/>
              <cant_paquetes_perdidos/>
              <cant_total_paquetes_transmitidos/>
              <porcentaje/>
            </relacion>
          </trimestres>
          <meta>
            <instanceID/>
          </meta>
        </ict4.2.4.2>
      </instance>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/periodo_considerado" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/ict4.2.4.2/trimestres/relacion/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/ict4.2.4.2/trimestres/relacion/provincia" nodeset="/ict4.2.4.2/trimestres/relacion/partido" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/partido_otro" relevant=" /ict4.2.4.2/trimestres/relacion/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/ict4.2.4.2/trimestres/relacion/partido" nodeset="/ict4.2.4.2/trimestres/relacion/localidad" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/localidad_otro" relevant=" /ict4.2.4.2/trimestres/relacion/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/areas and depends=/ict4.2.4.2/trimestres/relacion/localidad" nodeset="/ict4.2.4.2/trimestres/relacion/area" required="true()" type="select1"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/area_otro" relevant=" /ict4.2.4.2/trimestres/relacion/area =9999" required="true()" type="string"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/hora_pico_inicio" required="true()" type="time"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/hora_pico_fin" required="true()" type="time"/>
      <bind nodeset="/ict4.2.4.2/trimestres/relacion/tipo_informacion" required="true()" type="select1"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.4.2/trimestres/relacion/cant_paquetes_perdidos" required="true()" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/ict4.2.4.2/trimestres/relacion/cant_total_paquetes_transmitidos" required="true()" type="int"/>
      <bind constraint="porcentual= /ict4.2.4.2/trimestres/relacion/cant_paquetes_perdidos , /ict4.2.4.2/trimestres/relacion/cant_total_paquetes_transmitidos ,100" nodeset="/ict4.2.4.2/trimestres/relacion/porcentaje" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/ict4.2.4.2/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/ict4.2.4.2/trimestres">
      <label></label>
      <repeat appearance="grilla" nodeset="/ict4.2.4.2/trimestres">
        <group appearance="col3 well-white" ref="/ict4.2.4.2/trimestres/relacion">
          <label>Relación entre la cantidad de paquetes perdidos y el total de paquetes transmitidos</label>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/periodo_considerado">
            <label>Período Considerado</label>
            <hint>Período Considerado</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Enero</label>
              <value>Enero</value>
            </item>
            <item>
              <label>Febrero</label>
              <value>Febrero</value>
            </item>
            <item>
              <label>Marzo</label>
              <value>Marzo</value>
            </item>
            <item>
              <label>Abril</label>
              <value>Abril</value>
            </item>
            <item>
              <label>Mayo</label>
              <value>Mayo</value>
            </item>
            <item>
              <label>Junio</label>
              <value>Junio</value>
            </item>
            <item>
              <label>Julio</label>
              <value>Julio</value>
            </item>
            <item>
              <label>Agosto</label>
              <value>Agosto</value>
            </item>
            <item>
              <label>Septiembre</label>
              <value>Septiembre</value>
            </item>
            <item>
              <label>Octubre</label>
              <value>Octubre</value>
            </item>
            <item>
              <label>Noviembre</label>
              <value>Noviembre</value>
            </item>
            <item>
              <label>Diciembre</label>
              <value>Diciembre</value>
            </item>
            <item>
              <label>1º Trimestre</label>
              <value>1º Trimestre</value>
            </item>
            <item>
              <label>2º Trimestre</label>
              <value>2º Trimestre</value>
            </item>
            <item>
              <label>3º trimestre</label>
              <value>3º trimestre</value>
            </item>
            <item>
              <label>4º trimestre</label>
              <value>4º trimestre</value>
            </item>
            <item>
              <label>Anual</label>
              <value>Anual</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/partido">
            <label>Departamento / Partido</label>
            <hint>Departamento / Partido</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.2/trimestres/relacion/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.2/trimestres/relacion/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.2/trimestres/relacion/area_otro">
            <label>Área Otros</label>
            <hint>Área Otros</hint>
          </input>
          <input ref="/ict4.2.4.2/trimestres/relacion/hora_pico_inicio">
            <label>Hora Pico Inicio</label>
            <hint>Hora Pico Inicio</hint>
          </input>
          <input ref="/ict4.2.4.2/trimestres/relacion/hora_pico_fin">
            <label>Hora Pico Fin</label>
            <hint>Hora Pico Fin</hint>
          </input>
          <select1 appearance="minimal" ref="/ict4.2.4.2/trimestres/relacion/tipo_informacion">
            <label>Tipo de información</label>
            <hint>Tipo de información</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Audio Bidireccional</label>
              <value>Audio Bidireccional</value>
            </item>
            <item>
              <label>Audio</label>
              <value>Audio</value>
            </item>
            <item>
              <label>Video Bidireccional</label>
              <value>Video Bidireccional</value>
            </item>
            <item>
              <label>Video Unidireccional</label>
              <value>Video Unidireccional</value>
            </item>
            <item>
              <label>Datos</label>
              <value>Datos</value>
            </item>
          </select1>
          <input ref="/ict4.2.4.2/trimestres/relacion/cant_paquetes_perdidos">
            <label>Cant. de paquetes perdidos</label>
            <hint>Cantidad de paquetes perdidos</hint>
          </input>
          <input ref="/ict4.2.4.2/trimestres/relacion/cant_total_paquetes_transmitidos">
            <label>Cant. de paquetes transmitidos</label>
            <hint>Cantidad total de paquetes transmitidos</hint>
          </input>
          <input ref="/ict4.2.4.2/trimestres/relacion/porcentaje">
            <label>Resultado (%)</label>
            <hint>Resultado del Indicador</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
