<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.46 - Gastos en Personal</h:title>
    <model>
      <instance>
        <C1.46 id="C1.46">
          <meses jr:template="">
            <mes>
              <gastos/>
              <cargas/>
              <terceros/>
              <otros/>
              <calculate/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.46>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.46/meses/mes/gastos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.46/meses/mes/cargas" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.46/meses/mes/terceros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.46/meses/mes/otros" required="true()" type="int"/>
      <bind calculate=" /C1.46/meses/mes/gastos + /C1.46/meses/mes/cargas + /C1.46/meses/mes/terceros + /C1.46/meses/mes/otros " nodeset="/C1.46/meses/mes/calculate" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.46/meses/mes/total" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.46/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.46/meses">
      <label></label>
      <repeat nodeset="/C1.46/meses">
        <group appearance="col3 well-white" ref="/C1.46/meses/mes">
          <label>{title}</label>
          <input ref="/C1.46/meses/mes/gastos">
            <label>Gastos en Personal (remuneraciones):</label>
            <hint>Gastos en Personal [en miles de pesos]</hint>
          </input>
          <input ref="/C1.46/meses/mes/cargas">
            <label>Cargas Soc. y otros costos laborales:</label>
            <hint>CS y Otros [en miles de pesos]</hint>
          </input>
          <input ref="/C1.46/meses/mes/terceros">
            <label>Materiales y trabajo de terceros:</label>
            <hint>Terceros [en miles de pesos]</hint>
          </input>
          <input ref="/C1.46/meses/mes/otros">
            <label>Otros:</label>
            <hint>Otros [en miles de pesos]</hint>
          </input>
          <input ref="/C1.46/meses/mes/total">
            <label>Total: <output value=" /C1.46/meses/mes/calculate "/></label><hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
