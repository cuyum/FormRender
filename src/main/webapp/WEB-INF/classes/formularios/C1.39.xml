<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.39 - Ingresos / larga distancia internacional horario reducido</h:title>
    <model>
      <instance>
        <C1.39 id="C1.39">
          <mes>
            <ddi>0</ddi>
            <operadora>0</operadora>
            <otros>0</otros>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.39>
      </instance>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.39/mes/ddi" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.39/mes/operadora" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.39/mes/otros" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.39/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col3 well-white" ref="/C1.39/mes">
      <label>Enero</label>
      <input ref="/C1.39/mes/ddi">
        <label>DDI</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.39/mes/operadora">
        <label>Operadora</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.39/mes/otros">
        <label>Otros</label>
        <hint>Ingrese número entero</hint>
      </input>
    </group>
  </h:body>
</h:html>
