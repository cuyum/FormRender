<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.17 - Calidad de servicio / facturación</h:title>
    <model>
      <instance>
        <C1.17 id="C1.17">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <contratos_vigentes/>
              <facturacion_papel/>
              <facturacion_web/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.17>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.17/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.17/meses/mes/provincia" nodeset="/C1.17/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.17/meses/mes/partido" nodeset="/C1.17/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/C1.17/meses/mes/localidad_otro" relevant=" /C1.17/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.17/meses/mes/localidad" nodeset="/C1.17/meses/mes/area" required="true()" type="select1"/>
      <bind nodeset="/C1.17/meses/mes/area_otro" relevant=" /C1.17/meses/mes/area =9999" required="true()" type="string"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.17/meses/mes/contratos_vigentes" required="true()" type="int"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.17/meses/mes/facturacion_papel" required="true()" type="int"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.17/meses/mes/facturacion_web" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.17/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.17/meses">
      <label></label>
      <repeat nodeset="/C1.17/meses">
        <group appearance="col4 well-white variable" ref="/C1.17/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/C1.17/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.17/meses/mes/partido">
            <label>Partido/Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.17/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <input ref="/C1.17/meses/mes/localidad_otro">
            <label>Especifique</label>
          </input>
          <select1 appearance="minimal" ref="/C1.17/meses/mes/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <input ref="/C1.17/meses/mes/area_otro">
            <label>Especifique</label>
          </input>
          <input ref="/C1.17/meses/mes/contratos_vigentes">
            <label>Contratos Vigente</label>
            <hint>Contratos Vigente</hint>
          </input>
          <input ref="/C1.17/meses/mes/facturacion_papel">
            <label>Facturación Papel</label>
            <hint>Facturación Papel</hint>
          </input>
          <input ref="/C1.17/meses/mes/facturacion_web">
            <label>Facturación Web</label>
            <hint>Facturación Web</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
