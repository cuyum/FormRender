<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.3 - Redes e Infraestructura</h:title>
    <model>
      <instance>
        <D1.3 id="D1.3">
          <puntos jr:template="">
            <lugar>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
            </lugar>
            <equipamiento>
              <tecnologia/>
              <equipamiento_tecn_otro/>
              <equipamiento_cap_disp/>
              <equipamiento_cap_util/>
            </equipamiento>
            <tipomedio>
              <medio/>
              <medio_tipo_otro/>
              <medio_cap_disp/>
              <medio_cap_util/>
            </tipomedio>
            <tipotendido>
              <tendido/>
              <tendido_tipo_otro/>
              <tendido_cap_disp/>
              <tendido_cap_util/>
            </tipotendido>
            <lugar1>
              <provincia1/>
              <partido1/>
              <localidad1/>
              <localidad1_otro/>
            </lugar1>
          </puntos>
          <meta>
            <instanceID/>
          </meta>
        </D1.3>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/D1.3/puntos/lugar/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/D1.3/puntos/lugar/provincia" nodeset="/D1.3/puntos/lugar/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/D1.3/puntos/lugar/partido" nodeset="/D1.3/puntos/lugar/localidad" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/lugar/localidad_otro" relevant=" /D1.3/puntos/lugar/localidad =9999" required="true()" type="string"/>
      <bind nodeset="/D1.3/puntos/equipamiento/tecnologia" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/equipamiento/equipamiento_tecn_otro" relevant=" /D1.3/puntos/equipamiento/tecnologia =Otros" required="true()" type="string"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/equipamiento/equipamiento_cap_disp" required="true()" type="decimal"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/equipamiento/equipamiento_cap_util" required="true()" type="decimal"/>
      <bind nodeset="/D1.3/puntos/tipomedio/medio" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/tipomedio/medio_tipo_otro" relevant=" /D1.3/puntos/tipomedio/medio =Otros" required="true()" type="string"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/tipomedio/medio_cap_disp" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/tipomedio/medio_cap_util" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind nodeset="/D1.3/puntos/tipotendido" relevant=" /D1.3/puntos/tipomedio/medio =FO or  /D1.3/puntos/tipomedio/medio =Otros"/>
      <bind nodeset="/D1.3/puntos/tipotendido/tendido" relevant=" /D1.3/puntos/tipomedio/medio =FO or  /D1.3/puntos/tipomedio/medio =Otros" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/tipotendido/tendido_tipo_otro" relevant=" /D1.3/puntos/tipotendido/tendido =Otros" required="true()" type="string"/>
      <bind constraint="mask=999" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/tipotendido/tendido_cap_disp" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint="mask=999" jr:constraintMsg="Datos no válidos" nodeset="/D1.3/puntos/tipotendido/tendido_cap_util" relevant=" /D1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/D1.3/puntos/lugar1/provincia1" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/D1.3/puntos/lugar1/provincia1" nodeset="/D1.3/puntos/lugar1/partido1" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/D1.3/puntos/lugar1/partido1" nodeset="/D1.3/puntos/lugar1/localidad1" required="true()" type="select1"/>
      <bind nodeset="/D1.3/puntos/lugar1/localidad1_otro" relevant=" /D1.3/puntos/lugar1/localidad1 =9999" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.3/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.3/puntos">
      <label></label>
      <repeat appearance="grilla" nodeset="/D1.3/puntos">
        <group appearance="col3 well-white" ref="/D1.3/puntos/lugar">
          <label>Lugar</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/lugar/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/equipamiento">
          <label>Equipamiento</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/equipamiento/tecnologia">
            <label>Tecnología</label>
            <hint>Tecnología</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>SDH</label>
              <value>SDH</value>
            </item>
            <item>
              <label>DWDM</label>
              <value>DWDM</value>
            </item>
            <item>
              <label>PDH</label>
              <value>PDH</value>
            </item>
            <item>
              <label>IP</label>
              <value>IP</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/equipamiento/equipamiento_tecn_otro">
            <label>Tec. Otros</label>
          </input>
          <input ref="/D1.3/puntos/equipamiento/equipamiento_cap_disp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [Mbps / xxE1]</hint>
          </input>
          <input ref="/D1.3/puntos/equipamiento/equipamiento_cap_util">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [Mbps / xxE1]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/tipomedio">
          <label>Medio</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/tipomedio/medio">
            <label>Tipo</label>
            <hint>Tipo</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>FO</label>
              <value>FO</value>
            </item>
            <item>
              <label>MW</label>
              <value>MW</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/tipomedio/medio_tipo_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/D1.3/puntos/tipomedio/medio_cap_disp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de pelos]</hint>
          </input>
          <input ref="/D1.3/puntos/tipomedio/medio_cap_util">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de pelos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/tipotendido">
          <label>Tendido</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/tipotendido/tendido">
            <label>Tipo</label>
            <hint>Tipo</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>AÉREO</label>
              <value>AEREO</value>
            </item>
            <item>
              <label>SUBTERRÁNEO</label>
              <value>SUBTERRANEO</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/tipotendido/tendido_tipo_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/D1.3/puntos/tipotendido/tendido_cap_disp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de ductos]</hint>
          </input>
          <input ref="/D1.3/puntos/tipotendido/tendido_cap_util">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de ductos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/D1.3/puntos/lugar1">
          <label>Lugar</label>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar1/provincia1">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar1/partido1">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.3/puntos/lugar1/localidad1">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.3/puntos/lugar1/localidad1_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
