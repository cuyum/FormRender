<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.3 - Redes e Infraestructura</h:title>
    <model>
      <instance>
        <C1.3 id="C1.3">
          <lugar>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
          </lugar>
          <equipamiento>
            <spinner/>
            <my_int1>0.00</my_int1>
            <my_int2/>
          </equipamiento>
          <medio>
            <spinner/>
            <my_int1/>
            <my_int2/>
          </medio>
          <tendido>
            <spinner1/>
            <my_int1/>
            <my_int2/>
          </tendido>
          <lugar1>
            <provincia1/>
            <partido1/>
            <localidad1/>
            <areas_local1/>
          </lugar1>
          <meta>
            <instanceID/>
          </meta>
        </C1.3>
      </instance>
      <bind constraint="url=/areas/provincias" jr:constraintMsg="true()" nodeset="/C1.3/lugar/provincia" required="true()" type="select1"/>
      <bind constraint="url=/areas/partidos and depends=/C1.3/lugar/provincia" jr:constraintMsg="true()" nodeset="/C1.3/lugar/partido" required="true()" type="select1"/>
      <bind constraint="url=/areas/localidades and depends=/C1.3/lugar/partido" jr:constraintMsg="true()" nodeset="/C1.3/lugar/localidad" required="true()" type="select1"/>
      <bind constraint="url=/areas/areas and depends=/C1.3/lugar/localidad" nodeset="/C1.3/lugar/areas_local" required="true()" type="select1"/>
      <bind nodeset="/C1.3/equipamiento/spinner" required="true()" type="select1"/>
      <bind constraint=".&lt;10.000.000,00 and .&gt;0.00" jr:constraintMsg="Debe ingresar número de hasta 7 dígitos y 2 decimales." nodeset="/C1.3/equipamiento/my_int1" required="true()" type="decimal"/>
      <bind constraint=".&lt;10.000.000,00 and .&gt;0.00" jr:constraintMsg="Debe ingresar número de hasta 7 dígitos y 2 decimales." nodeset="/C1.3/equipamiento/my_int2" required="true()" type="decimal"/>
      <bind nodeset="/C1.3/medio/spinner" required="true()" type="select1"/>
      <bind constraint=".&lt;100.000 and .&gt;0.00" jr:constraintMsg="Debe ingresar número de hasta 5 dígitos y 2 decimales." nodeset="/C1.3/medio/my_int1" required="true()" type="int"/>
      <bind constraint=".&lt;100.000 and .&gt;0.00" jr:constraintMsg="Debe ingresar número de hasta 5 dígitos y 2 decimales." nodeset="/C1.3/medio/my_int2" required="true()" type="int"/>
      <bind nodeset="/C1.3/tendido/spinner1" required="true()" type="select1"/>
      <bind constraint=".&lt;1000 and .&gt;0.00" jr:constraintMsg="Debe ingresar un número entero de hasta 3 dígitos" nodeset="/C1.3/tendido/my_int1" required="true()" type="int"/>
      <bind constraint=".&lt;1000 and .&gt;0.00" jr:constraintMsg="Debe ingresar un número entero de hasta 3 dígitos" nodeset="/C1.3/tendido/my_int2" required="true()" type="int"/>
      <bind jr:constraintMsg="true()" nodeset="/C1.3/lugar1/provincia1" required="true()" type="select1"/>
      <bind jr:constraintMsg="true()" nodeset="/C1.3/lugar1/partido1" required="true()" type="select1"/>
      <bind jr:constraintMsg="true()" nodeset="/C1.3/lugar1/localidad1" required="true()" type="select1"/>
      <bind nodeset="/C1.3/lugar1/areas_local1" required="true()" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.3/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well_white" ref="/C1.3/lugar">
      <label>Lugar</label>
      <select1 appearance="minimal" ref="/C1.3/lugar/provincia">
        <label>Provincia</label>
        <hint>Seleccione Provincia</hint>
        <item>
          <label></label>
          <value>id_provincia</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/partido">
        <label>Partido / Departamento</label>
        <hint>Seleccione Partido</hint>
        <item>
          <label></label>
          <value>id_dpto_partido</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/localidad">
        <label>Localidad</label>
        <hint>Seleccione Localidad</hint>
        <item>
          <label></label>
          <value>id_localidad</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/areas_local">
        <label>Área Local</label>
        <hint>Seleccione Área Local</hint>
        <item>
          <label></label>
          <value>id_area_local</value>
        </item>
      </select1>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/equipamiento">
      <label>Equipamiento</label>
      <select1 appearance="minimal" ref="/C1.3/equipamiento/spinner">
        <label>Tecnología</label>
        <hint>Seleccione Tecnología</hint>
        <item>
          <label>SDH</label>
          <value>a</value>
        </item>
        <item>
          <label>DWDM</label>
          <value>b</value>
        </item>
        <item>
          <label>PDH</label>
          <value>c</value>
        </item>
        <item>
          <label>IP</label>
          <value>d</value>
        </item>
        <item>
          <label>Otros</label>
          <value>e</value>
        </item>
      </select1>
      <input ref="/C1.3/equipamiento/my_int1">
        <label>Capacidad Disponible</label>
        <hint>9999999,99 [Mbps / xxE1]</hint>
      </input>
      <input ref="/C1.3/equipamiento/my_int2">
        <label>Capacidad Utilizada</label>
        <hint>9999999,99 [Mbps / xxE1]</hint>
      </input>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/medio">
      <label>Medio</label>
      <select1 appearance="minimal" ref="/C1.3/medio/spinner">
        <label>Tecnología</label>
        <hint>Seleccione Tecnología</hint>
        <item>
          <label>FO</label>
          <value>1</value>
        </item>
        <item>
          <label>MW</label>
          <value>2</value>
        </item>
        <item>
          <label>Otros</label>
          <value>3</value>
        </item>
      </select1>
      <input ref="/C1.3/medio/my_int1">
        <label>Capacidad Disponible</label>
        <hint>99.999 [cantidad de pelos]</hint>
      </input>
      <input ref="/C1.3/medio/my_int2">
        <label>Capacidad Utilizada</label>
        <hint>99.999 [cantidad de pelos]</hint>
      </input>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/tendido">
      <label>Tendido</label>
      <select1 appearance="minimal" ref="/C1.3/tendido/spinner1">
        <label>Tipo</label>
        <hint>Seleccione Tipo</hint>
        <item>
          <label>AÉREO</label>
          <value>1</value>
        </item>
        <item>
          <label>SUBTERRANEO</label>
          <value>2</value>
        </item>
        <item>
          <label>Otros</label>
          <value>3</value>
        </item>
      </select1>
      <input ref="/C1.3/tendido/my_int1">
        <label>Capacidad Disponible</label>
        <hint>999 [cantidad de ductos]</hint>
      </input>
      <input ref="/C1.3/tendido/my_int2">
        <label>Capacidad Utilizada</label>
        <hint>999 [cantidad de ductos]</hint>
      </input>
    </group>
    <group appearance="col4 well_white" ref="/C1.3/lugar1">
      <label>Lugar</label>
      <select1 appearance="minimal" ref="/C1.3/lugar1/provincia1">
        <label>Provincia</label>
        <hint>Seleccione Provincia</hint>
        <item>
          <label></label>
          <value>id_provincia</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/partido1">
        <label>Partido / Departamento</label>
        <hint>Seleccione Partido</hint>
        <item>
          <label></label>
          <value>id_dpto_partido</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/localidad1">
        <label>Localidad</label>
        <hint>Seleccione Localidad</hint>
        <item>
          <label></label>
          <value>id_localidad</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/areas_local1">
        <label>Área Local</label>
        <hint>Seleccione Área Local</hint>
        <item>
          <label></label>
          <value>id_area_local</value>
        </item>
      </select1>
    </group>
  </h:body>
</h:html>
