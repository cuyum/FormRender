<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.3 - Redes e Infraestructura</h:title>
    <model>
      <instance>
        <C1.3 id="C1.3">
          <lugar>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
          </lugar>
          <equipamiento>
            <equipamiento_tecn/>
            <equipamiento_cap_disp>0.00</equipamiento_cap_disp>
            <equipamiento_cap_util/>
          </equipamiento>
          <medio>
            <medio_tipo/>
            <medio_cap_disp/>
            <medio_cap_util/>
          </medio>
          <tendido>
            <tendido_tipo/>
            <tendido_cap_disp/>
            <tendido_cap_util/>
          </tendido>
          <lugar1>
            <provincia1/>
            <partido1/>
            <localidad1/>
            <areas_local1/>
          </lugar1>
          <meta>
            <instanceID/>
          </meta>
        </C1.3>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.3/lugar/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.3/lugar/provincia" nodeset="/C1.3/lugar/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.3/lugar/partido" nodeset="/C1.3/lugar/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.3/lugar/localidad" nodeset="/C1.3/lugar/areas_local" required="true()" type="select1"/>
      <bind nodeset="/C1.3/equipamiento/equipamiento_tecn" required="true()" type="select1"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/equipamiento/equipamiento_cap_disp" required="true()" type="decimal"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/equipamiento/equipamiento_cap_util" required="true()" type="decimal"/>
      <bind nodeset="/C1.3/medio/medio_tipo" required="true()" type="select1"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/medio/medio_cap_disp" required="true()" type="decimal"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/medio/medio_cap_util" required="true()" type="decimal"/>
      <bind nodeset="/C1.3/tendido/tendido_tipo" required="true()" type="select1"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/tendido/tendido_cap_disp" required="true()" type="decimal"/>
      <bind constraint="mask=9.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.3/tendido/tendido_cap_util" required="true()" type="decimal"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.3/lugar1/provincia1" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.3/lugar1/provincia1" nodeset="/C1.3/lugar1/partido1" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.3/lugar1/partido1" nodeset="/C1.3/lugar1/localidad1" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.3/lugar1/localidad1" nodeset="/C1.3/lugar1/areas_local1" required="true()" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.3/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well_white" ref="/C1.3/lugar">
      <label>Lugar</label>
      <select1 appearance="minimal" ref="/C1.3/lugar/provincia">
        <label>Provincia</label>
        <hint>Provincia</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/partido">
        <label>Partido / Departamento</label>
        <hint>Partido/Departamento</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/localidad">
        <label>Localidad</label>
        <hint>Localidad</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar/areas_local">
        <label>Área Local</label>
        <hint>Área Local</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/equipamiento">
      <label>Equipamiento</label>
      <select1 appearance="minimal" ref="/C1.3/equipamiento/equipamiento_tecn">
        <label>Tecnología</label>
        <hint>Tecnología</hint>
        <item>
          <label>Seleccione opcion</label>
          <value>0</value>
        </item>
        <item>
          <label>SDH</label>
          <value>a</value>
        </item>
        <item>
          <label>DWDM</label>
          <value>b</value>
        </item>
        <item>
          <label>PDH</label>
          <value>c</value>
        </item>
        <item>
          <label>IP</label>
          <value>d</value>
        </item>
        <item>
          <label>Otros</label>
          <value>e</value>
        </item>
      </select1>
      <input ref="/C1.3/equipamiento/equipamiento_cap_disp">
        <label>Capacidad Disponible</label>
        <hint>Capacidad Disponible [Mbps / xxE1]</hint>
      </input>
      <input ref="/C1.3/equipamiento/equipamiento_cap_util">
        <label>Capacidad Utilizada</label>
        <hint>Capacidad Utilizada [Mbps / xxE1]</hint>
      </input>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/medio">
      <label>Medio</label>
      <select1 appearance="minimal" ref="/C1.3/medio/medio_tipo">
        <label>Tipo</label>
        <hint>Tipo</hint>
        <item>
          <label>Seleccione opcion</label>
          <value>0</value>
        </item>
        <item>
          <label>FO</label>
          <value>1</value>
        </item>
        <item>
          <label>MW</label>
          <value>2</value>
        </item>
        <item>
          <label>Otros</label>
          <value>3</value>
        </item>
      </select1>
      <input ref="/C1.3/medio/medio_cap_disp">
        <label>Capacidad Disponible</label>
        <hint>Capacidad Disponible [Mbps / xxE1]</hint>
      </input>
      <input ref="/C1.3/medio/medio_cap_util">
        <label>Capacidad Utilizada</label>
        <hint>Capacidad Utilizada [Mbps / xxE1]</hint>
      </input>
    </group>
    <group appearance="col3 well_white" ref="/C1.3/tendido">
      <label>Tendido</label>
      <select1 appearance="minimal" ref="/C1.3/tendido/tendido_tipo">
        <label>Tipo</label>
        <hint>Tipo</hint>
        <item>
          <label>Seleccione opcion</label>
          <value>0</value>
        </item>
        <item>
          <label>AÉREO</label>
          <value>1</value>
        </item>
        <item>
          <label>SUBTERRANEO</label>
          <value>2</value>
        </item>
        <item>
          <label>Otros</label>
          <value>3</value>
        </item>
      </select1>
      <input ref="/C1.3/tendido/tendido_cap_disp">
        <label>Capacidad Disponible</label>
        <hint>Capacidad Disponible [Mbps / xxE1]</hint>
      </input>
      <input ref="/C1.3/tendido/tendido_cap_util">
        <label>Capacidad Utilizada</label>
        <hint>Capacidad Utilizada [Mbps / xxE1]</hint>
      </input>
    </group>
    <group appearance="col4 well_white" ref="/C1.3/lugar1">
      <label>Lugar</label>
      <select1 appearance="minimal" ref="/C1.3/lugar1/provincia1">
        <label>Provincia</label>
        <hint>Provincia</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/partido1">
        <label>Partido / Departamento</label>
        <hint>Partido/Departamento</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/localidad1">
        <label>Localidad</label>
        <hint>Localidad</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.3/lugar1/areas_local1">
        <label>Área Local</label>
        <hint>Área Local</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
    </group>
  </h:body>
</h:html>
