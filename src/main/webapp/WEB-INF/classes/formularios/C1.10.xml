<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.10 - Trafico III (cantidad de llamadas)</h:title>
    <model>
      <instance>
        <C1.10 id="C1.10">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <l_l>
              <l_hogar/>
              <l_empresa/>
              <l_gob/>
              <l_otros/>
            </l_l>
            <l_ldn>
              <ldn_hogar/>
              <ldn_empresa/>
              <ldn_gob/>
              <ldn_otros/>
            </l_ldn>
            <l_ldi>
              <ldi_hogar/>
              <ldi_empresa/>
              <ldi_gob/>
              <ldi_otros/>
            </l_ldi>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.10>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.10/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.10/mes/provincia" nodeset="/C1.10/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.10/mes/partido" nodeset="/C1.10/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.10/mes/localidad" nodeset="/C1.10/mes/areas_local" required="true()" type="select1"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_l/l_hogar" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_l/l_empresa" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_l/l_gob" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_l/l_otros" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldn/ldn_hogar" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldn/ldn_empresa" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldn/ldn_gob" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldn/ldn_otros" required="true()" type="decimal"/>
      <bind constraint="{u'default': u'mask=99999999,99'}" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldi/ldi_hogar" required="true()" type="decimal"/>
      <bind constraint="{u'default': u'mask=99999999,99'}" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldi/ldi_empresa" required="true()" type="decimal"/>
      <bind constraint="{u'default': u'mask=99999999,99'}" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldi/ldi_gob" required="true()" type="decimal"/>
      <bind constraint="{u'default': u'mask=99999999,99'}" jr:constraintMsg="Datos no válidos" nodeset="/C1.10/mes/l_ldi/ldi_otros" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.10/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.10/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.10/mes/provincia">
        <label>Provincia</label>
        <hint>Provincia</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.10/mes/partido">
        <label>Partido / Departamento</label>
        <hint>Partido/Departamento</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.10/mes/localidad">
        <label>Localidad</label>
        <hint>Localidad</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.10/mes/areas_local">
        <label>Área Local</label>
        <hint>Área Local</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <group appearance="col2 well-white" ref="/C1.10/mes/l_l">
        <label>L-L</label>
        <input ref="/C1.10/mes/l_l/l_hogar">
          <label>Hogar</label>
          <hint>Hogar [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_l/l_empresa">
          <label>Empresa</label>
          <hint>Empresa [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_l/l_gob">
          <label>Gob</label>
          <hint>Gob [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_l/l_otros">
          <label>Otros</label>
          <hint>Otros [en miles]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.10/mes/l_ldn">
        <label>L-LDN</label>
        <input ref="/C1.10/mes/l_ldn/ldn_hogar">
          <label>Hogar</label>
          <hint>Hogar [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldn/ldn_empresa">
          <label>Empresa</label>
          <hint>Empresa [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldn/ldn_gob">
          <label>Gob</label>
          <hint>Gob [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldn/ldn_otros">
          <label>Otros</label>
          <hint>Otros [en miles]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.10/mes/l_ldi">
        <label>L-LDI</label>
        <input ref="/C1.10/mes/l_ldi/ldi_hogar">
          <label>Hogar</label>
          <hint>Hogar [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldi/ldi_empresa">
          <label>Empresa</label>
          <hint>Empresa [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldi/ldi_gob">
          <label>Gob</label>
          <hint>Gob [en miles]</hint>
        </input>
        <input ref="/C1.10/mes/l_ldi/ldi_otros">
          <label>Otros</label>
          <hint>Otros [en miles]</hint>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
