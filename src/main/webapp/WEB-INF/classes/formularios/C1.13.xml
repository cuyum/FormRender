<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.13 - Tráfico III (en minutos)</h:title>
    <model>
      <instance>
        <C1.13 id="C1.13">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <l_l>
                <l_hogar/>
                <l_empresa/>
                <l_gob/>
                <l_otros/>
              </l_l>
              <l_ldn>
                <ldn_hogar/>
                <ldn_empresa/>
                <ldn_gob/>
                <ldn_otros/>
              </l_ldn>
              <l_ldi>
                <ldi_hogar/>
                <ldi_empresa/>
                <ldi_gob/>
                <ldi_otros/>
              </l_ldi>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.13>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.13/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.13/meses/mes/provincia" nodeset="/C1.13/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.13/meses/mes/partido" nodeset="/C1.13/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/C1.13/meses/mes/localidad_otro" relevant=" /C1.13/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.13/meses/mes/localidad" nodeset="/C1.13/meses/mes/area" required="true()" type="select1"/>
      <bind nodeset="/C1.13/meses/mes/area_otro" relevant=" /C1.13/meses/mes/area =9999" required="true()" type="string"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_l/l_hogar" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_l/l_empresa" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_l/l_gob" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_l/l_otros" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldn/ldn_hogar" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldn/ldn_empresa" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldn/ldn_gob" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldn/ldn_otros" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldi/ldi_hogar" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldi/ldi_empresa" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldi/ldi_gob" required="true()" type="decimal"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.13/meses/mes/l_ldi/ldi_otros" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.13/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.13/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/C1.13/meses">
        <group appearance="col4 well-white variable" ref="/C1.13/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/C1.13/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.13/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.13/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <input ref="/C1.13/meses/mes/localidad_otro">
            <label>Especifique</label>
          </input>
          <select1 appearance="minimal" ref="/C1.13/meses/mes/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <input ref="/C1.13/meses/mes/area_otro">
            <label>Especifique</label>
          </input>
          <group appearance="col2 well-white" ref="/C1.13/meses/mes/l_l">
            <label>L-L</label>
            <input ref="/C1.13/meses/mes/l_l/l_hogar">
              <label>Hogar</label>
              <hint>Hogar [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_l/l_empresa">
              <label>Empresa</label>
              <hint>Empresa [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_l/l_gob">
              <label>Gob</label>
              <hint>Gobierno [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_l/l_otros">
              <label>Otros</label>
              <hint>Otros [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.13/meses/mes/l_ldn">
            <label>L-LDN</label>
            <input ref="/C1.13/meses/mes/l_ldn/ldn_hogar">
              <label>Hogar</label>
              <hint>Hogar [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldn/ldn_empresa">
              <label>Empresa</label>
              <hint>Empresa [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldn/ldn_gob">
              <label>Gob</label>
              <hint>Gobierno [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldn/ldn_otros">
              <label>Otros</label>
              <hint>Otros [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.13/meses/mes/l_ldi">
            <label>L-LDI</label>
            <input ref="/C1.13/meses/mes/l_ldi/ldi_hogar">
              <label>Hogar</label>
              <hint>Hogar [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldi/ldi_empresa">
              <label>Empresa</label>
              <hint>Empresa [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldi/ldi_gob">
              <label>Gob</label>
              <hint>Gobierno [en miles]</hint>
            </input>
            <input ref="/C1.13/meses/mes/l_ldi/ldi_otros">
              <label>Otros</label>
              <hint>Otros [en miles]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
