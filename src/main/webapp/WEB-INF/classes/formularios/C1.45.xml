<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.45 - Ingresos interconexion a la red fija - LDI a vinculadas</h:title>
    <model>
      <instance>
        <C1.45 id="C1.45">
          <meses jr:template="">
            <mes>
              <arriendos/>
              <trafico/>
              <corresponsales_entrada/>
              <corresponsales_salida/>
              <total_neto/>
              <calculate3/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.45>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/arriendos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/trafico" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/corresponsales_entrada" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/corresponsales_salida" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/total_neto" required="true()" type="int"/>
      <bind calculate=" /C1.45/meses/mes/arriendos + /C1.45/meses/mes/trafico + /C1.45/meses/mes/corresponsales_entrada + /C1.45/meses/mes/corresponsales_salida + /C1.45/meses/mes/total_neto " nodeset="/C1.45/meses/mes/calculate3" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.45/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.45/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.45/meses">
      <label></label>
      <repeat nodeset="/C1.45/meses">
        <group appearance="col3 well-white variable" ref="/C1.45/meses/mes">
          <label>{title}</label>
          <input ref="/C1.45/meses/mes/arriendos">
            <label>Arriendos Circuitos LDI</label>
            <hint>Arriendos Circuitos LDI [en miles de pesos]</hint>
          </input>
          <input ref="/C1.45/meses/mes/trafico">
            <label>Tráfico LDI</label>
            <hint>Tráfico LDI [en miles de pesos]</hint>
          </input>
          <input ref="/C1.45/meses/mes/corresponsales_entrada">
            <label>Corresponsales de Entrada</label>
            <hint>Corresponsales de Entrada [en miles de pesos]</hint>
          </input>
          <input ref="/C1.45/meses/mes/corresponsales_salida">
            <label>Corresponsales de Salida</label>
            <hint>Corresponsales de Salida [en miles de pesos]</hint>
          </input>
          <input ref="/C1.45/meses/mes/total_neto">
            <label>Total Neto de Corresponsales de Salida</label>
            <hint>Total Neto de Corresponsales de Salida [en miles de pesos]</hint>
          </input>
          <input ref="/C1.45/meses/mes/total">
            <label>Total</label>
            <hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
