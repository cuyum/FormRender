<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.36 - Ingresos / larga distancia nacional horario normal</h:title>
    <model>
      <instance>
        <C1.36 id="C1.36">
          <mes>
            <clave1/>
            <clave2_3/>
            <clave4_5/>
            <clave6_12/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.36>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.36/mes/clave1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.36/mes/clave2_3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.36/mes/clave4_5" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.36/mes/clave6_12" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.36/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.36/mes">
      <label>Enero</label>
      <input ref="/C1.36/mes/clave1">
        <label>Clave 1</label>
        <hint>Clave 1 [en miles de pesos]</hint>
      </input>
      <input ref="/C1.36/mes/clave2_3">
        <label>Clave 2 y 3</label>
        <hint>Clave 2 y 3 [en miles de pesos]</hint>
      </input>
      <input ref="/C1.36/mes/clave4_5">
        <label>Clave 4 y 5</label>
        <hint>Clave 4 y 5 [en miles de pesos]</hint>
      </input>
      <input ref="/C1.36/mes/clave6_12">
        <label>Clave 6 a 12</label>
        <hint>Clave 6 a 12 [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
