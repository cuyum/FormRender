<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C.1.32.1 - Comercio Exterior</h:title>
    <model>
      <instance>
        <C.1.32.1 id="C_1_32_1">
          <nota/>
          <telecomunicaciones>
            <exportaciones1>0</exportaciones1>
            <importaciones1>0</importaciones1>
          </telecomunicaciones>
          <servicios>
            <exportaciones2>0</exportaciones2>
            <importaciones2>0</importaciones2>
          </servicios>
          <otros>
            <importaciones3>0</importaciones3>
            <importaciones4>0</importaciones4>
            <regalias>0</regalias>
          </otros>
          <totales>
            <calculate1/>
            <exportaciones/>
            <calculate2/>
            <importaciones/>
          </totales>
          <meta>
            <instanceID/>
          </meta>
        </C.1.32.1>
      </instance>
      <bind nodeset="/C.1.32.1/nota" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 8 dígitos" nodeset="/C.1.32.1/telecomunicaciones/exportaciones1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de hasta 8 dígitos" nodeset="/C.1.32.1/telecomunicaciones/importaciones1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 8 dígitos" nodeset="/C.1.32.1/servicios/exportaciones2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de hasta 8 dígitos" nodeset="/C.1.32.1/servicios/importaciones2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 8 dígitos" nodeset="/C.1.32.1/otros/importaciones3" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de hasta 8 dígitos" nodeset="/C.1.32.1/otros/importaciones4" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de hasta 8 dígitos" nodeset="/C.1.32.1/otros/regalias" type="int"/>
      <bind calculate=" /C.1.32.1/telecomunicaciones/exportaciones1 + /C.1.32.1/servicios/exportaciones2 " nodeset="/C.1.32.1/totales/calculate1" type="string"/>
      <bind nodeset="/C.1.32.1/totales/exportaciones" readonly="true()" type="string"/>
      <bind calculate=" /C.1.32.1/telecomunicaciones/importaciones1 + /C.1.32.1/servicios/importaciones2 + /C.1.32.1/otros/importaciones3 + /C.1.32.1/otros/importaciones4 " nodeset="/C.1.32.1/totales/calculate2" type="string"/>
      <bind nodeset="/C.1.32.1/totales/importaciones" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C.1.32.1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/C.1.32.1/nota">
      <label>Expresar en miles de pesos 8 dígitos</label>
    </input>
    <group ref="/C.1.32.1/telecomunicaciones">
      <label>Telecomunicaciones</label>
      <input ref="/C.1.32.1/telecomunicaciones/exportaciones1">
        <label>Exportaciones</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
      <input ref="/C.1.32.1/telecomunicaciones/importaciones1">
        <label>Importaciones</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
    </group>
    <group ref="/C.1.32.1/servicios">
      <label>Servicios</label>
      <input ref="/C.1.32.1/servicios/exportaciones2">
        <label>Exportaciones</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
      <input ref="/C.1.32.1/servicios/importaciones2">
        <label>Importaciones</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
    </group>
    <group ref="/C.1.32.1/otros">
      <label>Otros</label>
      <input ref="/C.1.32.1/otros/importaciones3">
        <label>Importaciones Bienes de Capital</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
      <input ref="/C.1.32.1/otros/importaciones4">
        <label>Importaciones Partes y Piezas para Bienes de Capital</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
      <input ref="/C.1.32.1/otros/regalias">
        <label>Regalías y Derechos de Licencia</label>
        <hint>En miles de pesos de 8 dígitos</hint>
      </input>
    </group>
    <group ref="/C.1.32.1/totales">
      <label>Totales</label>
      <input ref="/C.1.32.1/totales/exportaciones">
        <label>Exportaciones: $ <output value=" /C.1.32.1/totales/calculate1 "/></label></input>
      <input ref="/C.1.32.1/totales/importaciones">
        <label>Importaciones: $ <output value=" /C.1.32.1/totales/calculate2 "/></label></input>
    </group>
  </h:body>
</h:html>
