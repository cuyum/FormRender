<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.20 - Calidad de servicio / atencion telefónica</h:title>
    <model>
      <itext>
        <translation lang="Spanish">
          <text id="/C1.20/mes/partido3:hint">
            <value>Seleccione Partido</value>
          </text>
          <text id="/C1.20/mes/provincia3:hint">
            <value>Seleccione Provincia</value>
          </text>
          <text id="/C1.20/mes/personal:hint">
            <value>Ingrese número entero</value>
          </text>
          <text id="/C1.20/mes/espera_prom:hint">
            <value>Ingrese número entero</value>
          </text>
          <text id="/C1.20/mes/areas_local3:hint">
            <value>Seleccione Área Local</value>
          </text>
          <text id="/C1.20/mes/localidad3:hint">
            <value>Seleccione Localidad</value>
          </text>
        </translation>
      </itext>
      <instance>
        <C1.20 id="C1.20">
          <mes>
            <provincia3/>
            <partido3/>
            <localidad3/>
            <areas_local3/>
            <personal/>
            <espera_prom/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.20>
      </instance>
      <bind constraint="url=/areas/provincias" jr:constraintMsg="true()" nodeset="/C1.20/mes/provincia3" required="true()" type="select1"/>
      <bind constraint="url=/areas/partidos and depends=/C1.20/mes/provincia" jr:constraintMsg="true()" nodeset="/C1.20/mes/partido3" required="true()" type="select1"/>
      <bind constraint="url=/areas/localidades and depends=/C1.20/mes/partido" jr:constraintMsg="true()" nodeset="/C1.20/mes/localidad3" required="true()" type="select1"/>
      <bind constraint="url=/areas/areas and depends=/C1.20/mes/localidad" nodeset="/C1.20/mes/areas_local3" required="true()" type="select1"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.20/mes/personal" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.20/mes/espera_prom" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.20/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.20/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.20/mes/provincia3">
        <label>Provincia</label>
        <hint ref="jr:itext('/C1.20/mes/provincia3:hint')"/>
        <item>
          <label></label>
          <value>id_provincia</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/partido3">
        <label>Partido</label>
        <hint ref="jr:itext('/C1.20/mes/partido3:hint')"/>
        <item>
          <label></label>
          <value>id_dpto_partido</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/localidad3">
        <label>Localidad</label>
        <hint ref="jr:itext('/C1.20/mes/localidad3:hint')"/>
        <item>
          <label></label>
          <value>id_localidad</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/areas_local3">
        <label>Área Local</label>
        <hint ref="jr:itext('/C1.20/mes/areas_local3:hint')"/>
        <item>
          <label></label>
          <value>id_area_local</value>
        </item>
      </select1>
      <input ref="/C1.20/mes/personal">
        <label>Personal</label>
        <hint ref="jr:itext('/C1.20/mes/personal:hint')"/>
      </input>
      <input ref="/C1.20/mes/espera_prom">
        <label>Espera Promedio</label>
        <hint ref="jr:itext('/C1.20/mes/espera_prom:hint')"/>
      </input>
    </group>
  </h:body>
</h:html>
