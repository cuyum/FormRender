<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.20 - Calidad de servicio / atencion telefónica</h:title>
    <model>
      <itext>
        <translation lang="Spanish">
          <text id="/C1.20/mes/areas_local:hint">
            <value>Seleccione Área Local</value>
          </text>
          <text id="/C1.20/mes/personal:hint">
            <value>Ingrese número entero</value>
          </text>
          <text id="/C1.20/mes/localidad:hint">
            <value>Seleccione Localidad</value>
          </text>
          <text id="/C1.20/mes/provincia:hint">
            <value>Seleccione Provincia</value>
          </text>
          <text id="/C1.20/mes/partido:hint">
            <value>Seleccione Partido</value>
          </text>
          <text id="/C1.20/mes/espera_prom:hint">
            <value>Ingrese número entero</value>
          </text>
        </translation>
      </itext>
      <instance>
        <C1.20 id="C1.20">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <personal/>
            <espera_prom/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.20>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.20/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.20/mes/provincia" nodeset="/C1.20/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.20/mes/partido" nodeset="/C1.20/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.20/mes/localidad" nodeset="/C1.20/mes/areas_local" required="true()" type="select1"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.20/mes/personal" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.20/mes/espera_prom" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.20/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.20/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.20/mes/provincia">
        <label>Provincia</label>
        <hint ref="jr:itext('/C1.20/mes/provincia:hint')"/>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/partido">
        <label>Partido</label>
        <hint ref="jr:itext('/C1.20/mes/partido:hint')"/>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/localidad">
        <label>Localidad</label>
        <hint ref="jr:itext('/C1.20/mes/localidad:hint')"/>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.20/mes/areas_local">
        <label>Área Local</label>
        <hint ref="jr:itext('/C1.20/mes/areas_local:hint')"/>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <input ref="/C1.20/mes/personal">
        <label>Personal</label>
        <hint ref="jr:itext('/C1.20/mes/personal:hint')"/>
      </input>
      <input ref="/C1.20/mes/espera_prom">
        <label>Espera Promedio</label>
        <hint ref="jr:itext('/C1.20/mes/espera_prom:hint')"/>
      </input>
    </group>
  </h:body>
</h:html>
