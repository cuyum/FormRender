<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario F1.11 - Calidad de servicio / Facturación</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <F1.11 id="F1.11">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <contratosVigentes/>
              <facturacionPapel/>
              <facturacionWeb/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </F1.11>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/F1.11/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/F1.11/meses/mes/provincia" nodeset="/F1.11/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/F1.11/meses/mes/partido" nodeset="/F1.11/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/F1.11/meses/mes/localidad_otro" relevant=" /F1.11/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.11/meses/mes/contratosVigentes" required="true()" type="int"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.11/meses/mes/facturacionPapel" required="true()" type="int"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.11/meses/mes/facturacionWeb" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/F1.11/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/F1.11/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/F1.11/meses">
        <group appearance="col4 well-white variable" ref="/F1.11/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/F1.11/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.11/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.11/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/F1.11/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <input ref="/F1.11/meses/mes/contratosVigentes">
            <label>Contratos Vigentes</label>
            <hint>Contratos Vigentes</hint>
          </input>
          <input ref="/F1.11/meses/mes/facturacionPapel">
            <label>Facturación Papel</label>
            <hint>Facturación Papel</hint>
          </input>
          <input ref="/F1.11/meses/mes/facturacionWeb">
            <label>Facturación Web</label>
            <hint>Facturación Web</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
