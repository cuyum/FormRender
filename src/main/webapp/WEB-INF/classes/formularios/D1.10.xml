<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.10 - Tráfico V (llamadas)</h:title>
    <model>
      <instance>
        <D1.10 id="D1.10">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <l_l>
                <l_e/>
                <l_s/>
              </l_l>
              <l_ldn>
                <ldn_e/>
                <ldn_s/>
              </l_ldn>
              <l_ldi>
                <ldi_e/>
                <ldi_s/>
              </l_ldi>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </D1.10>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/D1.10/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/D1.10/meses/mes/provincia" nodeset="/D1.10/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/D1.10/meses/mes/partido" nodeset="/D1.10/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/D1.10/meses/mes/localidad_otro" relevant=" /D1.10/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_l/l_e" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_l/l_s" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_ldn/ldn_e" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_ldn/ldn_s" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_ldi/ldi_e" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.10/meses/mes/l_ldi/ldi_s" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.10/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.10/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/D1.10/meses">
        <group appearance="col3 well-white variable" ref="/D1.10/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/D1.10/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.10/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.10/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.10/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <group appearance="col2 well-white" ref="/D1.10/meses/mes/l_l">
            <label>L-L</label>
            <input ref="/D1.10/meses/mes/l_l/l_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/D1.10/meses/mes/l_l/l_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.10/meses/mes/l_ldn">
            <label>L-LDN</label>
            <input ref="/D1.10/meses/mes/l_ldn/ldn_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/D1.10/meses/mes/l_ldn/ldn_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.10/meses/mes/l_ldi">
            <label>L-LDI</label>
            <input ref="/D1.10/meses/mes/l_ldi/ldi_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/D1.10/meses/mes/l_ldi/ldi_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
