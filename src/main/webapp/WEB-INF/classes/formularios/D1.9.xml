<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.9 - Tráfico IV (minutos)</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <D1.9 id="D1.9">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <prepago>
                <prepago_hp/>
                <prepago_hnp/>
              </prepago>
              <pospago>
                <pospago_hp/>
                <pospago_hnp/>
              </pospago>
              <mixto>
                <mixto_hp/>
                <mixto_hnp/>
              </mixto>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </D1.9>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/D1.9/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/D1.9/meses/mes/provincia" nodeset="/D1.9/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/D1.9/meses/mes/partido" nodeset="/D1.9/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/D1.9/meses/mes/localidad_otro" relevant=" /D1.9/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/prepago/prepago_hp" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/prepago/prepago_hnp" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/pospago/pospago_hp" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/pospago/pospago_hnp" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/mixto/mixto_hp" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.9/meses/mes/mixto/mixto_hnp" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.9/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.9/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/D1.9/meses">
        <group appearance="col3 well-white variable" ref="/D1.9/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/D1.9/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.9/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.9/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/D1.9/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <group appearance="col2 well-white" ref="/D1.9/meses/mes/prepago">
            <label>Prepago</label>
            <input ref="/D1.9/meses/mes/prepago/prepago_hp">
              <label>HP</label>
              <hint>HP [en miles]</hint>
            </input>
            <input ref="/D1.9/meses/mes/prepago/prepago_hnp">
              <label>HNP</label>
              <hint>HNP [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.9/meses/mes/pospago">
            <label>Pospago</label>
            <input ref="/D1.9/meses/mes/pospago/pospago_hp">
              <label>HP</label>
              <hint>HP [en miles]</hint>
            </input>
            <input ref="/D1.9/meses/mes/pospago/pospago_hnp">
              <label>HNP</label>
              <hint>HNP [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.9/meses/mes/mixto">
            <label>Mixto</label>
            <input ref="/D1.9/meses/mes/mixto/mixto_hp">
              <label>HP</label>
              <hint>HP [en miles]</hint>
            </input>
            <input ref="/D1.9/meses/mes/mixto/mixto_hnp">
              <label>HNP</label>
              <hint>HNP [en miles]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
