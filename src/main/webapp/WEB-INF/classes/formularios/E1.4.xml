<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario E1.4 - Accesos</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <E1.4 id="E1.4">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
              <disponibles/>
              <operativos/>
              <prepago/>
              <pospago/>
              <mixto/>
              <altas/>
              <bajas/>
              <terminalesID/>
              <terminalesRTPC/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </E1.4>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/E1.4/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/E1.4/meses/mes/provincia" nodeset="/E1.4/meses/mes/partido" required="true()" type="select1"/>
      <bind nodeset="/E1.4/meses/mes/partido_otro" relevant=" /E1.4/meses/mes/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/E1.4/meses/mes/partido" nodeset="/E1.4/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/E1.4/meses/mes/localidad_otro" relevant=" /E1.4/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/disponibles" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/operativos" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/prepago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/pospago" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/mixto" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/altas" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/bajas" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/terminalesID" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999999" jr:constraintMsg="Ingrese entero entre 0 y 99.999.999" nodeset="/E1.4/meses/mes/terminalesRTPC" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/E1.4/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/E1.4/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/E1.4/meses">
        <group appearance="col4 well-white variable" ref="/E1.4/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/E1.4/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/E1.4/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.4/meses/mes/partido_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/E1.4/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.4/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <input ref="/E1.4/meses/mes/disponibles">
            <label>Disponibles</label>
            <hint>Disponibles</hint>
          </input>
          <input ref="/E1.4/meses/mes/operativos">
            <label>Operativos</label>
            <hint>Operativos</hint>
          </input>
          <input ref="/E1.4/meses/mes/prepago">
            <label>Prepago</label>
            <hint>Prepago</hint>
          </input>
          <input ref="/E1.4/meses/mes/pospago">
            <label>Pospago</label>
            <hint>Pospago</hint>
          </input>
          <input ref="/E1.4/meses/mes/mixto">
            <label>Mixto</label>
            <hint>Mixto</hint>
          </input>
          <input ref="/E1.4/meses/mes/altas">
            <label>Altas</label>
            <hint>Altas</hint>
          </input>
          <input ref="/E1.4/meses/mes/bajas">
            <label>Bajas</label>
            <hint>Bajas</hint>
          </input>
          <input ref="/E1.4/meses/mes/terminalesID">
            <label>Terminales con ID</label>
            <hint>Terminales con ID</hint>
          </input>
          <input ref="/E1.4/meses/mes/terminalesRTPC">
            <label>Terminales con acceso a la RTPC</label>
            <hint>Terminales con acceso a la RTPC</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
