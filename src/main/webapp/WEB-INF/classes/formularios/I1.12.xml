<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario I1.12 - Gastos y Costos</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <I1.12 id="I1.12">
          <meses jr:template="">
            <mes>
              <interconexion/>
              <otrosExplotacion/>
              <otrosComerciales/>
              <otrosFinancieros/>
              <otrosAdminist/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </I1.12>
      </instance>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/interconexion" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/otrosExplotacion" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/otrosComerciales" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/otrosFinancieros" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/otrosAdminist" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/I1.12/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/I1.12/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/I1.12/meses">
      <label></label>
      <repeat nodeset="/I1.12/meses">
        <group appearance="col4 well-white variable" ref="/I1.12/meses/mes">
          <label>{title}</label>
          <input ref="/I1.12/meses/mes/interconexion">
            <label>Interconexi贸n</label>
            <hint>Interconexi贸n [en miles de pesos]</hint>
          </input>
          <input ref="/I1.12/meses/mes/otrosExplotacion">
            <label>Otros Gastos de Explotaci贸n</label>
            <hint>Otros Gastos de Explotaci贸n [en miles de pesos]</hint>
          </input>
          <input ref="/I1.12/meses/mes/otrosComerciales">
            <label>Otros Comerciales</label>
            <hint>Otros Comerciales [en miles de pesos]</hint>
          </input>
          <input ref="/I1.12/meses/mes/otrosFinancieros">
            <label>Otros Financieros</label>
            <hint>Otros Financieros [en miles de pesos]</hint>
          </input>
          <input ref="/I1.12/meses/mes/otrosAdminist">
            <label>Otros Administrativos</label>
            <hint>Otros Administrativos [en miles de pesos]</hint>
          </input>
          <input ref="/I1.12/meses/mes/total">
            <label>Total</label>
            <hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
