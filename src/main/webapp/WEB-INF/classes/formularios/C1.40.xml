<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C.1.40 - Ingresos interconexion a la red fija - Local 3</h:title>
    <model>
      <instance>
        <C1.40 id="C1.40">
          <mes>
            <local1>0</local1>
            <local2>0</local2>
            <local3>0</local3>
            <local4>0</local4>
            <local5>0</local5>
            <local6>0</local6>
            <local7>0</local7>
            <local8>0</local8>
            <calculate1/>
            <local9/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.40>
      </instance>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local1" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local2" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local3" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local4" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local5" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local6" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local7" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C1.40/mes/local8" required="true()" type="int"/>
      <bind calculate=" /C1.40/mes/local1 + /C1.40/mes/local2 + /C1.40/mes/local3 + /C1.40/mes/local4 + /C1.40/mes/local5 + /C1.40/mes/local6 + /C1.40/mes/local7 + /C1.40/mes/local8 " nodeset="/C1.40/mes/calculate1" type="string"/>
      <bind nodeset="/C1.40/mes/local9" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.40/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col3" ref="/C1.40/mes">
      <label>Mes</label>
      <input ref="/C1.40/mes/local1">
        <label>Origen y Terminacion Local Fijo</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local2">
        <label>Origen y Terminacion Móvil</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local3">
        <label>Puertos</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local4">
        <label>Coubicacion</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local5">
        <label>Tránsito Local</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local6">
        <label>Otras Facilidades Esenciales</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local7">
        <label>Facturación y Cobranzas</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local8">
        <label>Otros</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C1.40/mes/local9">
        <label>Total <output value=" /C1.40/mes/calculate1 "/></label></input>
    </group>
  </h:body>
</h:html>
