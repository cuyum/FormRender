<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.37 - Ingresos / larga distancia nacional horario reducido</h:title>
    <model>
      <instance>
        <C1.37 id="C1.37">
          <mes>
            <clave1>0</clave1>
            <clave2_3>0</clave2_3>
            <clave4_5>0</clave4_5>
            <clave6_12>0</clave6_12>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.37>
      </instance>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.37/mes/clave1" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.37/mes/clave2_3" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.37/mes/clave4_5" required="true()" type="int"/>
      <bind constraint=".&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero hasta 12 dígitos y mayor a 0" nodeset="/C1.37/mes/clave6_12" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.37/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.37/mes">
      <label>Enero</label>
      <input ref="/C1.37/mes/clave1">
        <label>Clave 1</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.37/mes/clave2_3">
        <label>Clave 2 y 3</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.37/mes/clave4_5">
        <label>Clave 4 y 5</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.37/mes/clave6_12">
        <label>Clave 6 a 12</label>
        <hint>Ingrese número entero</hint>
      </input>
    </group>
  </h:body>
</h:html>
