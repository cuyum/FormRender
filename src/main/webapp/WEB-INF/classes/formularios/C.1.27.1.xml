      <hint>Ingrese numero entero</hint>
    </input>
    <input ref="/C.1.27.1/cargas">
      <label>Cargas Sociales y Otros Costos Laborales</label>
      <hint>Ingrese numero entero</hint>
    </input>
    <input ref="/C.1.27.1/materiales">
      <label>Materiales y Trabajo de Terceros para Operaciones</label>
      <hint>Ingrese numero entero</hint>
    </input>
    <input ref="/C.1.27.1/otros">
      <label>Otros</label>
      <hint>Ingrese numero entero</hint>
    </input>
    <input ref="/C.1.27.1/calculate_test_output">
      <label>Total: $ <output value=" /C.1.27.1/calculate "/></label></input>
