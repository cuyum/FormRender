<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C.1.27 - GASTOS EN PERSONAL</h:title>
    <model>
      <instance>
        <C.1.27.1 id="C_1_27">
          <nota/>
          <gastos>0</gastos>
          <cargas>0</cargas>
          <materiales>0</materiales>
          <otros>0</otros>
          <calculate/>
          <calculate_test_output/>
          <meta>
            <instanceID/>
          </meta>
        </C.1.27.1>
      </instance>
      <bind nodeset="/C.1.27.1/nota" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.27.1/gastos" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.27.1/cargas" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.27.1/materiales" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C.1.27.1/otros" required="true()" type="int"/>
      <bind calculate=" /C.1.27.1/gastos + /C.1.27.1/cargas + /C.1.27.1/materiales + /C.1.27.1/otros " nodeset="/C.1.27.1/calculate" type="string"/>
      <bind nodeset="/C.1.27.1/calculate_test_output" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C.1.27.1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/C.1.27.1/nota">
      <label>Expresar en miles de pesos 12 d√≠gitos</label>
    </input>
    <input ref="/C.1.27.1/gastos">
