<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.49 - Personal ocupado</h:title>
    <model>
      <instance>
        <C1.49 id="C1.49">
          <mes>
            <hombres/>
            <mujeres/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.49>
      </instance>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.49/mes/hombres" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.49/mes/mujeres" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.49/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col1 well-white" ref="/C1.49/mes">
      <label>Enero</label>
      <input ref="/C1.49/mes/hombres">
        <label>Hombres</label>
        <hint>Hombres</hint>
      </input>
      <input ref="/C1.49/mes/mujeres">
        <label>Mujeres</label>
        <hint>Mujeres</hint>
      </input>
    </group>
  </h:body>
</h:html>
