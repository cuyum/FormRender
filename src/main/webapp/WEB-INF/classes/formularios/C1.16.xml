<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C1.16</h:title>
    <model>
      <instance>
        <C1.16 id="C1.16">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <TRP/>
            <TRCP/>
            <TRR/>
            <ROAC/>
            <RSCP/>
            <TCF/>
            <DPOSS/>
            <TFRP/>
            <DRF/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.16>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.16/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.16/mes/provincia" nodeset="/C1.16/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.16/mes/partido" nodeset="/C1.16/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.16/mes/localidad" nodeset="/C1.16/mes/areas_local" required="true()" type="select1"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/TRP" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/TRCP" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/TRR" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/ROAC" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/RSCP" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/TCF" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/DPOSS" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/TFRP" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.16/mes/DRF" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.16/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.16/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.16/mes/provincia">
        <label>Provincia</label>
        <hint>Seleccione Provincia</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.16/mes/partido">
        <label>Partido / Departamento</label>
        <hint>Seleccione Partido</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.16/mes/localidad">
        <label>Localidad</label>
        <hint>Seleccione Localidad</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.16/mes/areas_local">
        <label>Área Local</label>
        <hint>Seleccione Área Local</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <input ref="/C1.16/mes/TRP">
        <label>TRP</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/TRCP">
        <label>TRCP</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/TRR">
        <label>TRR</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/ROAC">
        <label>ROAC</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/RSCP">
        <label>RSCP</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/TCF">
        <label>TCF</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/DPOSS">
        <label>DPOSS</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/TFRP">
        <label>TFRP</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.16/mes/DRF">
        <label>DRF</label>
        <hint>Ingrese número entero</hint>
      </input>
    </group>
  </h:body>
</h:html>
