            <itemset nodeset="instance('localidades')/root/item[provincia= /C.1.1/datos/areas1/provincia  and partido= /C.1.1/datos/areas1/partido ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <input ref="/C.1.1/datos/areas1/localidad_otro">
            <label ref="jr:itext('/C.1.1/datos/areas1/localidad_otro:label')"/>
          </input>
          <select1 appearance="minimal" ref="/C.1.1/datos/areas1/areas_local">
            <label ref="jr:itext('/C.1.1/datos/areas1/areas_local:label')"/>
            <hint ref="jr:itext('/C.1.1/datos/areas1/areas_local:hint')"/>
            <itemset nodeset="instance('areas')/root/item[provincia= /C.1.1/datos/areas1/provincia ]">
