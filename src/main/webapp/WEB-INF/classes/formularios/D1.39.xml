<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.39 - Otros Gastos</h:title>
    <model>
      <instance>
        <D1.39 id="D1.39">
          <meses jr:template="">
            <mes>
              <id/>
              <impuestos/>
              <amortBienesUso/>
              <amortIntangibles/>
              <publicidad/>
              <costoEquipos/>
              <otrosComerciales/>
              <otrosFinancieros/>
              <otrosAdminist/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </D1.39>
      </instance>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/id" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/impuestos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/amortBienesUso" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/amortIntangibles" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/publicidad" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/costoEquipos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/otrosComerciales" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/otrosFinancieros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/otrosAdminist" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.39/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.39/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.39/meses">
      <label></label>
      <repeat nodeset="/D1.39/meses">
        <group appearance="col4 well-white variable" ref="/D1.39/meses/mes">
          <label>{title}</label>
          <input ref="/D1.39/meses/mes/id">
            <label>I &amp; D</label>
            <hint>I &amp; D [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/impuestos">
            <label>lmpuestos</label>
            <hint>lmp [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/amortBienesUso">
            <label>Amortización de Bienes de Uso</label>
            <hint>Amortiz. de Bienes de uso [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/amortIntangibles">
            <label>Amortización de Intangibles</label>
            <hint>Amortización de Intangibles [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/publicidad">
            <label>Publicidad</label>
            <hint>Publicidad [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/costoEquipos">
            <label>Costo de Equipos</label>
            <hint>Costo Equipos [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/otrosComerciales">
            <label>Otros Comerciales</label>
            <hint>Otros Comerciales [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/otrosFinancieros">
            <label>Otros Financieros</label>
            <hint>Otros Financieros [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/otrosAdminist">
            <label>Otros Administrativos</label>
            <hint>Otros Administrativos [en miles de pesos]</hint>
          </input>
          <input ref="/D1.39/meses/mes/total">
            <label>Total</label>
            <hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
