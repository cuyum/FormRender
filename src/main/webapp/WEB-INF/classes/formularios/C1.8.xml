  <h:body>
    <group ref="/C1.8/datos">
      <label></label>
      <repeat nodeset="/C1.8/datos">
        <group appearance="col4" ref="/C1.8/datos/mes">
          <label>Mes</label>
          <select1 appearance="minimal" ref="/C1.8/datos/mes/provincia4">
            <label>Provincia/Región</label>
            <item>
              <label ref="jr:itext('/C1.8/datos/mes/provincia4/buenosAires:label')"/>
              <value>buenosAires</value>
            </item>
            <item>
              <label ref="jr:itext('/C1.8/datos/mes/provincia4/catamarca:label')"/>
              <value>catamarca</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.8/datos/mes/partido4">
            <label>Partido/Dpto</label>
            <itemset nodeset="instance('partidos')/root/item[provincia= /C1.8/datos/mes/provincia4 ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <select1 appearance="minimal" ref="/C1.8/datos/mes/localidad4">
            <label>Localidad</label>
            <itemset nodeset="instance('localidades')/root/item[provincia= /C1.8/datos/mes/provincia4  and partido= /C1.8/datos/mes/partido4 ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <input ref="/C1.8/datos/mes/localidad_otro4">
            <label>Por favor especifique</label>
          </input>
          <select1 appearance="minimal" ref="/C1.8/datos/mes/areas_local4">
            <label>Área Local</label>
            <hint>Seleccione Área Local</hint>
            <itemset nodeset="instance('areas')/root/item[provincia= /C1.8/datos/mes/provincia4 ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <input ref="/C1.8/datos/mes/abono_1">
            <label>Abono Local</label>
            <hint>Ingrese número entero</hint>
          </input>
          <input ref="/C1.8/datos/mes/abono_2">
            <label>Abono LDN/LDI</label>
            <hint>Ingrese número entero</hint>
          </input>
          <input ref="/C1.8/datos/mes/abono_3">
            <label>Abono LDI</label>
            <hint>Ingrese número entero</hint>
          </input>
          <input ref="/C1.8/datos/mes/suscrip_serv_adic">
            <label>Suscripción Servicios Adicionales</label>
            <hint>Ingrese número entero</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
