<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario E1.3 - Redes e Infraestructura</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <E1.3 id="E1.3">
          <puntos jr:template="">
            <ubicacionOrigen>
              <provincia/>
              <partido/>
              <partido_otro/>
              <localidad/>
              <localidad_otro/>
            </ubicacionOrigen>
            <equipamiento>
              <tecnologia/>
              <tecnologia_otro/>
              <equipCapacidadDisp/>
              <equipCapacidadUtilizada/>
            </equipamiento>
            <tipomedio>
              <medio/>
              <medio_otro/>
              <medioCapacidadDisp/>
              <medioCapacidadUtilizada/>
            </tipomedio>
            <tipotendido>
              <tendido/>
              <tendido_otro/>
              <tendidoCapacidadDisp/>
              <tendidoCapacidadUtilizada/>
            </tipotendido>
            <ubicacionDestino>
              <provinciaDestino/>
              <partidoDestino/>
              <partidoDestino_otro/>
              <localidadDestino/>
              <localidadDestino_otro/>
            </ubicacionDestino>
          </puntos>
          <meta>
            <instanceID/>
          </meta>
        </E1.3>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/E1.3/puntos/ubicacionOrigen/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/E1.3/puntos/ubicacionOrigen/provincia" nodeset="/E1.3/puntos/ubicacionOrigen/partido" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/ubicacionOrigen/partido_otro" relevant=" /E1.3/puntos/ubicacionOrigen/partido =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/E1.3/puntos/ubicacionOrigen/partido" nodeset="/E1.3/puntos/ubicacionOrigen/localidad" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/ubicacionOrigen/localidad_otro" relevant=" /E1.3/puntos/ubicacionOrigen/localidad =9999" required="true()" type="string"/>
      <bind nodeset="/E1.3/puntos/equipamiento/tecnologia" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/equipamiento/tecnologia_otro" relevant=" /E1.3/puntos/equipamiento/tecnologia =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=9999999.99" jr:constraintMsg="Ingrese decimal entre 0 y 9.999.999,99" nodeset="/E1.3/puntos/equipamiento/equipCapacidadDisp" required="true()" type="decimal"/>
      <bind constraint=".&gt;=0 and .&lt;=9999999.99" jr:constraintMsg="Ingrese decimal entre 0 y 9.999.999,99" nodeset="/E1.3/puntos/equipamiento/equipCapacidadUtilizada" required="true()" type="decimal"/>
      <bind nodeset="/E1.3/puntos/tipomedio/medio" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/tipomedio/medio_otro" relevant=" /E1.3/puntos/tipomedio/medio =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=99999" jr:constraintMsg="Ingrese entero entre 0 y 99.999" nodeset="/E1.3/puntos/tipomedio/medioCapacidadDisp" relevant=" /E1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=99999" jr:constraintMsg="Ingrese entero entre 0 y 99.999" nodeset="/E1.3/puntos/tipomedio/medioCapacidadUtilizada" relevant=" /E1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind nodeset="/E1.3/puntos/tipotendido" relevant=" /E1.3/puntos/tipomedio/medio =FO or  /E1.3/puntos/tipomedio/medio =Otros"/>
      <bind nodeset="/E1.3/puntos/tipotendido/tendido" relevant=" /E1.3/puntos/tipomedio/medio =FO or  /E1.3/puntos/tipomedio/medio =Otros" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/tipotendido/tendido_otro" relevant=" /E1.3/puntos/tipotendido/tendido =Otros" required="true()" type="string"/>
      <bind constraint=".&gt;=0 and .&lt;=999" jr:constraintMsg="Ingrese entero entre 0 y 999" nodeset="/E1.3/puntos/tipotendido/tendidoCapacidadDisp" relevant=" /E1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999" jr:constraintMsg="Ingrese entero entre 0 y 999" nodeset="/E1.3/puntos/tipotendido/tendidoCapacidadUtilizada" relevant=" /E1.3/puntos/tipomedio/medio =FO" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/E1.3/puntos/ubicacionDestino/provinciaDestino" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/E1.3/puntos/ubicacionDestino/provinciaDestino" nodeset="/E1.3/puntos/ubicacionDestino/partidoDestino" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/ubicacionDestino/partidoDestino_otro" relevant=" /E1.3/puntos/ubicacionDestino/partidoDestino =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/E1.3/puntos/ubicacionDestino/partidoDestino" nodeset="/E1.3/puntos/ubicacionDestino/localidadDestino" required="true()" type="select1"/>
      <bind nodeset="/E1.3/puntos/ubicacionDestino/localidadDestino_otro" relevant=" /E1.3/puntos/ubicacionDestino/localidadDestino =9999" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/E1.3/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/E1.3/puntos">
      <label></label>
      <repeat appearance="grilla" nodeset="/E1.3/puntos">
        <group appearance="col3 well-white" ref="/E1.3/puntos/ubicacionOrigen">
          <label>Unicación Origen</label>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionOrigen/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionOrigen/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/ubicacionOrigen/partido_otro">
            <label>Paartido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionOrigen/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/ubicacionOrigen/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/E1.3/puntos/equipamiento">
          <label>Equipamiento</label>
          <select1 appearance="minimal" ref="/E1.3/puntos/equipamiento/tecnologia">
            <label>Tecnología</label>
            <hint>Tecnología</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>SDH</label>
              <value>SDH</value>
            </item>
            <item>
              <label>DWDM</label>
              <value>DWDM</value>
            </item>
            <item>
              <label>PDH</label>
              <value>PDH</value>
            </item>
            <item>
              <label>IP</label>
              <value>IP</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/equipamiento/tecnologia_otro">
            <label>Tec. Otros</label>
          </input>
          <input ref="/E1.3/puntos/equipamiento/equipCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [Mbps / xxE1]</hint>
          </input>
          <input ref="/E1.3/puntos/equipamiento/equipCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [Mbps / xxE1]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/E1.3/puntos/tipomedio">
          <label>Medio</label>
          <select1 appearance="minimal" ref="/E1.3/puntos/tipomedio/medio">
            <label>Tipo</label>
            <hint>Tipo</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>FO</label>
              <value>FO</value>
            </item>
            <item>
              <label>MW</label>
              <value>MW</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/tipomedio/medio_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/E1.3/puntos/tipomedio/medioCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de pelos]</hint>
          </input>
          <input ref="/E1.3/puntos/tipomedio/medioCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de pelos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/E1.3/puntos/tipotendido">
          <label>Tendido</label>
          <select1 appearance="minimal" ref="/E1.3/puntos/tipotendido/tendido">
            <label>Tipo</label>
            <hint>Tipo</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>AÉREO</label>
              <value>AEREO</value>
            </item>
            <item>
              <label>SUBTERRÁNEO</label>
              <value>SUBTERRANEO</value>
            </item>
            <item>
              <label>Otros</label>
              <value>Otros</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/tipotendido/tendido_otro">
            <label>Tipo Otros</label>
          </input>
          <input ref="/E1.3/puntos/tipotendido/tendidoCapacidadDisp">
            <label>Capacidad Disponible</label>
            <hint>Capacidad Disponible [cantidad de ductos]</hint>
          </input>
          <input ref="/E1.3/puntos/tipotendido/tendidoCapacidadUtilizada">
            <label>Capacidad Utilizada</label>
            <hint>Capacidad Utilizada [cantidad de ductos]</hint>
          </input>
        </group>
        <group appearance="col3 well-white" ref="/E1.3/puntos/ubicacionDestino">
          <label>Ubicación Destino</label>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionDestino/provinciaDestino">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionDestino/partidoDestino">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/ubicacionDestino/partidoDestino_otro">
            <label>Partido Otros</label>
            <hint>Partido Otros</hint>
          </input>
          <select1 appearance="minimal" ref="/E1.3/puntos/ubicacionDestino/localidadDestino">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/E1.3/puntos/ubicacionDestino/localidadDestino_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
