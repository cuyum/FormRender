<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.44 - Rentas</h:title>
    <model>
      <instance>
        <D1.44 id="D1.44">
          <rentas>
            <remision/>
            <distribucion/>
          </rentas>
          <meta>
            <instanceID/>
          </meta>
        </D1.44>
      </instance>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.44/rentas/remision" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.44/rentas/distribucion" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.44/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col1 well-white" ref="/D1.44/rentas">
      <label>Rentas</label>
      <input ref="/D1.44/rentas/remision">
        <label>Remisión de Utilidades</label>
        <hint>Remisión de Utilidades [en miles de pesos]</hint>
      </input>
      <input ref="/D1.44/rentas/distribucion">
        <label>Distribución de Dividendos</label>
        <hint>Distribución de Dividendos [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
