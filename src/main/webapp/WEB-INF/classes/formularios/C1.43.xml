<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.43 - Ingresos interconexion a la red fija - local a vinculadas</h:title>
    <model>
      <instance>
        <C1.43 id="C1.43">
          <Enero>
            <orterfijo/>
            <ortermovil/>
            <puertos/>
            <coubic/>
            <transito/>
            <facilidades/>
            <facturacion/>
            <otros/>
            <calculate1/>
            <total/>
          </Enero>
          <meta>
            <instanceID/>
          </meta>
        </C1.43>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/orterfijo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/ortermovil" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/puertos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/coubic" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/transito" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/facilidades" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/facturacion" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/otros" required="true()" type="int"/>
      <bind calculate=" /C1.43/Enero/orterfijo + /C1.43/Enero/ortermovil + /C1.43/Enero/puertos + /C1.43/Enero/coubic + /C1.43/Enero/transito + /C1.43/Enero/facilidades + /C1.43/Enero/facturacion + /C1.43/Enero/otros " nodeset="/C1.43/Enero/calculate1" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.43/Enero/total" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.43/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col3 well-white" ref="/C1.43/Enero">
      <label>Enero</label>
      <input ref="/C1.43/Enero/orterfijo">
        <label>Origen y Terminacion Local Fijo:</label>
        <hint>Origen y Terminacion Local Fijo [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/ortermovil">
        <label>Origen y Terminacion Móvil:</label>
        <hint>Origen y Terminacion Móvil [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/puertos">
        <label>Puertos:</label>
        <hint>Puertos [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/coubic">
        <label>Coubicacion:</label>
        <hint>Coubicacion [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/transito">
        <label>Tránsito Local:</label>
        <hint>Tránsito Local [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/facilidades">
        <label>Otras Facilidades Esenciales:</label>
        <hint>Otras Facilidades Esenciales [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/facturacion">
        <label>Facturación y Cobranzas:</label>
        <hint>Facturación y Cobranzas [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/otros">
        <label>Otros:</label>
        <hint>Otros [en miles de pesos]</hint>
      </input>
      <input ref="/C1.43/Enero/total">
        <label>Total: <output value=" /C1.43/Enero/calculate1 "/></label><hint>Total [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
