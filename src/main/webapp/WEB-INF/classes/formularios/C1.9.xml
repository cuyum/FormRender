<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.9 - Tráfico I (llamadas)</h:title>
    <model>
      <instance>
        <C1.9 id="C1.9">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <area/>
              <l_l>
                <l_e/>
                <l_s/>
              </l_l>
              <l_ldn>
                <ldn_e/>
                <ldn_s/>
              </l_ldn>
              <l_ldi>
                <ldi_e/>
                <ldi_s/>
              </l_ldi>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.9>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.9/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.9/meses/mes/provincia" nodeset="/C1.9/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.9/meses/mes/partido" nodeset="/C1.9/meses/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.9/meses/mes/localidad" nodeset="/C1.9/meses/mes/area" required="true()" type="select1"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_l/l_e" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_l/l_s" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_ldn/ldn_e" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_ldn/ldn_s" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_ldi/ldi_e" required="true()" type="decimal"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/meses/mes/l_ldi/ldi_s" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.9/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.9/meses">
      <label></label>
      <repeat nodeset="/C1.9/meses">
        <group appearance="col4 well-white variable" ref="/C1.9/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/C1.9/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.9/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.9/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.9/meses/mes/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opcion</label>
              <value>0</value>
            </item>
          </select1>
          <group appearance="col2 well-white" ref="/C1.9/meses/mes/l_l">
            <label>L-L</label>
            <input ref="/C1.9/meses/mes/l_l/l_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/C1.9/meses/mes/l_l/l_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.9/meses/mes/l_ldn">
            <label>L-LDN</label>
            <input ref="/C1.9/meses/mes/l_ldn/ldn_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/C1.9/meses/mes/l_ldn/ldn_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.9/meses/mes/l_ldi">
            <label>L-LDI</label>
            <input ref="/C1.9/meses/mes/l_ldi/ldi_e">
              <label>Entrante</label>
              <hint>Entrante [en miles]</hint>
            </input>
            <input ref="/C1.9/meses/mes/l_ldi/ldi_s">
              <label>Saliente</label>
              <hint>Saliente [en miles]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
