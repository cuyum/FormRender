<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.9 - Tráfico I (llamadas)</h:title>
    <model>
      <instance>
        <C1.9 id="C1.9">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <l_l>
              <e_e1/>
              <s_s1/>
            </l_l>
            <l_ldn>
              <e_e2/>
              <s_s2/>
            </l_ldn>
            <l_ldi>
              <e_e3/>
              <s_s3/>
            </l_ldi>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.9>
      </instance>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/provincias" nodeset="/C1.9/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/partidos and depends=/C1.9/mes/provincia" nodeset="/C1.9/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/localidades and depends=/C1.9/mes/partido" nodeset="/C1.9/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=http://54.232.16.128:8080/localizacion/rest/localizaciones/areas and depends=/C1.9/mes/localidad" nodeset="/C1.9/mes/areas_local" required="true()" type="select1"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_l/e_e1" required="true()" type="string"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_l/s_s1" required="true()" type="string"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_ldn/e_e2" required="true()" type="string"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_ldn/s_s2" required="true()" type="string"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_ldi/e_e3" required="true()" type="string"/>
      <bind constraint="mask=99.999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.9/mes/l_ldi/s_s3" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.9/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.9/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.9/mes/provincia">
        <label>Provincia</label>
        <hint>Provincia</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.9/mes/partido">
        <label>Partido / Departamento</label>
        <hint>Partido/Departamento</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.9/mes/localidad">
        <label>Localidad</label>
        <hint>Localidad</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.9/mes/areas_local">
        <label>Área Local</label>
        <hint>Área Local</hint>
        <item>
          <label>Seleccione una opcion</label>
          <value>0</value>
        </item>
      </select1>
      <group appearance="col2 well-white" ref="/C1.9/mes/l_l">
        <label>L-L</label>
        <input ref="/C1.9/mes/l_l/e_e1">
          <label>Entrante</label>
          <hint>E</hint>
        </input>
        <input ref="/C1.9/mes/l_l/s_s1">
          <label>Saliente</label>
          <hint>S</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.9/mes/l_ldn">
        <label>L-LDN</label>
        <input ref="/C1.9/mes/l_ldn/e_e2">
          <label>Entrante</label>
          <hint>E</hint>
        </input>
        <input ref="/C1.9/mes/l_ldn/s_s2">
          <label>Saliente</label>
          <hint>S</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.9/mes/l_ldi">
        <label>L-LDI</label>
        <input ref="/C1.9/mes/l_ldi/e_e3">
          <label>Entrante</label>
          <hint>E</hint>
        </input>
        <input ref="/C1.9/mes/l_ldi/s_s3">
          <label>Saliente</label>
          <hint>S</hint>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
