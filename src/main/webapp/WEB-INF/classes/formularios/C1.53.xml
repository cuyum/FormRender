<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.53 - Financiamiento SU / Subsidios</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <C1.53 id="C1.53">
          <financ>
            <financiamientoSu/>
            <subsidiosTasaInteres/>
            <subsidiosImpositivos/>
            <subsidiosOtros/>
          </financ>
          <meta>
            <instanceID/>
          </meta>
        </C1.53>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.53/financ/financiamientoSu" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.53/financ/subsidiosTasaInteres" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.53/financ/subsidiosImpositivos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.53/financ/subsidiosOtros" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.53/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.53/financ">
      <label>Financiamiento</label>
      <input ref="/C1.53/financ/financiamientoSu">
        <label>Financiamiento con SU</label>
        <hint>Financiamiento con SU [en miles de pesos]</hint>
      </input>
      <input ref="/C1.53/financ/subsidiosTasaInteres">
        <label>Subsidios tasa de Interés</label>
        <hint>Subsidios tasa de Interés [en miles de pesos]</hint>
      </input>
      <input ref="/C1.53/financ/subsidiosImpositivos">
        <label>Subsidios Impositivos</label>
        <hint>Subsidios Impositivos [en miles de pesos]</hint>
      </input>
      <input ref="/C1.53/financ/subsidiosOtros">
        <label>Otros Subsidios</label>
        <hint>Otros Subsidios [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
