<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario H2.5 - Ingresos</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <H2.5 id="H2.5">
          <ingresos>
            <anio/>
            <abono/>
          </ingresos>
          <trafico>
            <urbano/>
            <interurbano/>
            <internacional/>
          </trafico>
          <tot>
            <total/>
          </tot>
          <meta>
            <instanceID/>
          </meta>
        </H2.5>
      </instance>
      <bind nodeset="/H2.5/ingresos/anio" required="true()" type="select1"/>
      <bind constraint="mask=9.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/H2.5/ingresos/abono" required="true()" type="int"/>
      <bind constraint="mask=9.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/H2.5/trafico/urbano" required="true()" type="int"/>
      <bind constraint="mask=9.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/H2.5/trafico/interurbano" required="true()" type="int"/>
      <bind constraint="mask=9.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/H2.5/trafico/internacional" required="true()" type="int"/>
      <bind constraint="mask=9.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/H2.5/tot/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/H2.5/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/H2.5/ingresos">
      <label>Ingresos</label>
      <select1 appearance="minimal" ref="/H2.5/ingresos/anio">
        <label>Año</label>
        <hint>Año</hint>
        <item>
          <label>Seleccione una opción</label>
          <value>-1</value>
        </item>
        <item>
          <label>2010</label>
          <value>2010</value>
        </item>
        <item>
          <label>2011</label>
          <value>2011</value>
        </item>
        <item>
          <label>2012</label>
          <value>2012</value>
        </item>
        <item>
          <label>2013</label>
          <value>2013</value>
        </item>
        <item>
          <label>2014</label>
          <value>2014</value>
        </item>
        <item>
          <label>2015</label>
          <value>2015</value>
        </item>
        <item>
          <label>2016</label>
          <value>2016</value>
        </item>
        <item>
          <label>2017</label>
          <value>2017</value>
        </item>
        <item>
          <label>2018</label>
          <value>2018</value>
        </item>
        <item>
          <label>2019</label>
          <value>2019</value>
        </item>
        <item>
          <label>2020</label>
          <value>2020</value>
        </item>
      </select1>
      <input ref="/H2.5/ingresos/abono">
        <label>Abono</label>
        <hint>Abono</hint>
      </input>
    </group>
    <group appearance="col4 well-white" ref="/H2.5/trafico">
      <label>Tráfico</label>
      <input ref="/H2.5/trafico/urbano">
        <label>Urbano</label>
        <hint>Urbano</hint>
      </input>
      <input ref="/H2.5/trafico/interurbano">
        <label>Interurbano</label>
        <hint>Interurbano</hint>
      </input>
      <input ref="/H2.5/trafico/internacional">
        <label>Internacional</label>
        <hint>Internacional</hint>
      </input>
    </group>
    <group appearance="col4 well-white" ref="/H2.5/tot">
      <input ref="/H2.5/tot/total">
        <label>Total</label>
        <hint>Total</hint>
      </input>
    </group>
  </h:body>
</h:html>
