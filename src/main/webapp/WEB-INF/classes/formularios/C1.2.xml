  <h:body>
    <group ref="/C1.2/datos">
      <label></label>
      <repeat nodeset="/C1.2/datos">
        <group appearance="col4" ref="/C1.2/datos/interconexion">
          <label ref="jr:itext('/C1.2/datos/interconexion:label')"/>
          <select1 appearance="minimal" ref="/C1.2/datos/interconexion/provincia">
            <label ref="jr:itext('/C1.2/datos/interconexion/provincia:label')"/>
            <item>
              <label ref="jr:itext('/C1.2/datos/interconexion/provincia/buenosAires:label')"/>
              <value>buenosAires</value>
            </item>
            <item>
              <label ref="jr:itext('/C1.2/datos/interconexion/provincia/catamarca:label')"/>
              <value>catamarca</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.2/datos/interconexion/partido">
            <label ref="jr:itext('/C1.2/datos/interconexion/partido:label')"/>
            <itemset nodeset="instance('partidos')/root/item[provincia= /C1.2/datos/interconexion/provincia ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <select1 appearance="minimal" ref="/C1.2/datos/interconexion/localidad">
            <label ref="jr:itext('/C1.2/datos/interconexion/localidad:label')"/>
            <itemset nodeset="instance('localidades')/root/item[provincia= /C1.2/datos/interconexion/provincia  and partido= /C1.2/datos/interconexion/partido ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <input ref="/C1.2/datos/interconexion/localidad_otro">
            <label ref="jr:itext('/C1.2/datos/interconexion/localidad_otro:label')"/>
          </input>
          <select1 appearance="minimal" ref="/C1.2/datos/interconexion/areas_local">
            <label ref="jr:itext('/C1.2/datos/interconexion/areas_local:label')"/>
            <hint ref="jr:itext('/C1.2/datos/interconexion/areas_local:hint')"/>
            <itemset nodeset="instance('areas')/root/item[provincia= /C1.2/datos/interconexion/provincia ]">
              <value ref="name"/>
              <label ref="jr:itext(itextId)"/>
            </itemset>
          </select1>
          <input ref="/C1.2/datos/interconexion/cuit_prestador">
            <label ref="jr:itext('/C1.2/datos/interconexion/cuit_prestador:label')"/>
            <hint ref="jr:itext('/C1.2/datos/interconexion/cuit_prestador:hint')"/>
          </input>
