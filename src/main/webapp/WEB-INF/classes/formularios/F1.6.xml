<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario F1.6 - Clientes Servicio y tecnología</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <F1.6 id="F1.6">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <servicio/>
              <servicio_otro/>
              <acceso/>
              <accesoBandaAncha/>
              <acceso_otro/>
              <accesoBandaAnchayOtros/>
              <accesoBandaAncha_otro/>
              <cuentas/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </F1.6>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/F1.6/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/F1.6/meses/mes/provincia" nodeset="/F1.6/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/F1.6/meses/mes/partido" nodeset="/F1.6/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/F1.6/meses/mes/localidad_otro" relevant=" /F1.6/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind nodeset="/F1.6/meses/mes/servicio" required="true()" type="select1"/>
      <bind nodeset="/F1.6/meses/mes/servicio_otro" relevant=" /F1.6/meses/mes/servicio =OTROS" required="true()" type="string"/>
      <bind nodeset="/F1.6/meses/mes/acceso" relevant=" /F1.6/meses/mes/servicio =610 or  /F1.6/meses/mes/servicio =611 or  /F1.6/meses/mes/servicio =612 or  /F1.6/meses/mes/servicio =NRO_GEOGRAFICO or  /F1.6/meses/mes/servicio =OTROS" required="true()" type="select1"/>
      <bind nodeset="/F1.6/meses/mes/accesoBandaAncha" relevant=" /F1.6/meses/mes/servicio =BANDA_ANCHA" type="select1"/>
      <bind nodeset="/F1.6/meses/mes/acceso_otro" relevant=" /F1.6/meses/mes/accesoBandaAncha =OTROS" required="true()" type="string"/>
      <bind nodeset="/F1.6/meses/mes/accesoBandaAnchayOtros" relevant=" /F1.6/meses/mes/servicio =OTROS" type="select1"/>
      <bind nodeset="/F1.6/meses/mes/accesoBandaAncha_otro" relevant=" /F1.6/meses/mes/accesoBandaAnchayOtros =OTROS" required="true()" type="string"/>
      <bind constraint="mask=99.999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.6/meses/mes/cuentas" relevant=" /F1.6/meses/mes/servicio =OTROS" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/F1.6/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/F1.6/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/F1.6/meses">
        <group appearance="col3 well-white variable" ref="/F1.6/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/F1.6/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
          </input>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/servicio">
            <label>Servicio</label>
            <hint>Servicio</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>610</label>
              <value>610</value>
            </item>
            <item>
              <label>611</label>
              <value>611</value>
            </item>
            <item>
              <label>612</label>
              <value>612</value>
            </item>
            <item>
              <label>Nº GEOGRAFICO</label>
              <value>NRO_GEOGRAFICO</value>
            </item>
            <item>
              <label>OTROS</label>
              <value>OTROS</value>
            </item>
            <item>
              <label>BANDA ANCHA</label>
              <value>BANDA_ANCHA</value>
            </item>
          </select1>
          <input ref="/F1.6/meses/mes/servicio_otro">
            <label>Servicio Otros</label>
          </input>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/acceso">
            <label>Accesos</label>
            <hint>Accesos</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>DIAL UP</label>
              <value>DIAL_UP</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/accesoBandaAncha">
            <label>Accesos</label>
            <hint>Accesos</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>ADSL</label>
              <value>ADSL</value>
            </item>
            <item>
              <label>CABLEMODEN</label>
              <value>CABLEMODEN</value>
            </item>
            <item>
              <label>WIRELESS</label>
              <value>WIRELESS</value>
            </item>
            <item>
              <label>CELULAR</label>
              <value>CELULAR</value>
            </item>
            <item>
              <label>SATELITAL</label>
              <value>SATELITAL</value>
            </item>
            <item>
              <label>BPL</label>
              <value>BPL</value>
            </item>
            <item>
              <label>WIMAX</label>
              <value>WIMAX</value>
            </item>
            <item>
              <label>FIBRA OPTICA</label>
              <value>FIBRA_OPTICA</value>
            </item>
            <item>
              <label>OTROS</label>
              <value>OTROS</value>
            </item>
          </select1>
          <input ref="/F1.6/meses/mes/acceso_otro">
            <label>Acceso Otros</label>
          </input>
          <select1 appearance="minimal" ref="/F1.6/meses/mes/accesoBandaAnchayOtros">
            <label>Accesos</label>
            <hint>Accesos</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>DIAL UP</label>
              <value>DIAL_UP</value>
            </item>
            <item>
              <label>OTROS</label>
              <value>OTROS</value>
            </item>
          </select1>
          <input ref="/F1.6/meses/mes/accesoBandaAncha_otro">
            <label>Acceso Otros</label>
          </input>
          <input ref="/F1.6/meses/mes/cuentas">
            <label>Cuentas</label>
            <hint>Cuentas</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
