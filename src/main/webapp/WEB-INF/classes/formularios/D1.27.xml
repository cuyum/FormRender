<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.27 - Tarifas / Planes pospago - mixto</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <D1.27 id="D1.27">
          <tarifasPlanes jr:template="">
            <caracteristicas>
              <plan/>
              <modalid/>
              <segment/>
            </caracteristicas>
            <cargos>
              <abono/>
              <gastos/>
              <total/>
            </cargos>
            <servicios>
              <minutos/>
              <sms/>
              <mms/>
              <serviciosAdic/>
            </servicios>
            <precios>
              <minuto>
                <minutoOn/>
                <minutoOff/>
              </minuto>
              <sms>
                <smsOn/>
                <smsOff/>
              </sms>
              <mms>
                <mmsOn/>
                <mmsOff/>
              </mms>
              <datos>
                <datosOn/>
                <datosOff/>
              </datos>
            </precios>
            <otrosImp>
              <otros>
                <otrosOtros/>
              </otros>
              <impuestos>
                <porcentaje/>
              </impuestos>
            </otrosImp>
          </tarifasPlanes>
          <meta>
            <instanceID/>
          </meta>
        </D1.27>
      </instance>
      <bind nodeset="/D1.27/tarifasPlanes/caracteristicas/plan" required="true()" type="string"/>
      <bind nodeset="/D1.27/tarifasPlanes/caracteristicas/modalid" required="true()" type="select1"/>
      <bind nodeset="/D1.27/tarifasPlanes/caracteristicas/segment" required="true()" type="select1"/>
      <bind constraint="mask=9.999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/cargos/abono" type="int"/>
      <bind constraint="mask=9.999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/cargos/gastos" type="int"/>
      <bind constraint="mask=99.999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/cargos/total" type="int"/>
      <bind nodeset="/D1.27/tarifasPlanes/servicios/minutos" required="true()" type="string"/>
      <bind nodeset="/D1.27/tarifasPlanes/servicios/sms" required="true()" type="string"/>
      <bind nodeset="/D1.27/tarifasPlanes/servicios/mms" required="true()" type="string"/>
      <bind nodeset="/D1.27/tarifasPlanes/servicios/serviciosAdic" type="string"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/minuto/minutoOn" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/minuto/minutoOff" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/sms/smsOn" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/sms/smsOff" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/mms/mmsOn" required="true()" type="int"/>
      <bind constraint="mask=99.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/mms/mmsOff" required="true()" type="int"/>
      <bind jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/datos/datosOn" required="true()" type="int"/>
      <bind jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/precios/datos/datosOff" required="true()" type="int"/>
      <bind nodeset="/D1.27/tarifasPlanes/otrosImp/otros/otrosOtros" required="true()" type="string"/>
      <bind constraint="mask=999,99" jr:constraintMsg="Datos no válidos" nodeset="/D1.27/tarifasPlanes/otrosImp/impuestos/porcentaje" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.27/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.27/tarifasPlanes">
      <label></label>
      <repeat appearance="grilla" nodeset="/D1.27/tarifasPlanes">
        <group appearance="col3 well-white" ref="/D1.27/tarifasPlanes/caracteristicas">
          <label>Características Generales</label>
          <input ref="/D1.27/tarifasPlanes/caracteristicas/plan">
            <label>Nombre del Plan</label>
            <hint>Nombre del Plan</hint>
          </input>
          <select1 appearance="minimal" ref="/D1.27/tarifasPlanes/caracteristicas/modalid">
            <label>Modalidad</label>
            <hint>Modalidad</hint>
            <item>
              <label>Seleccione opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>Opción 1</label>
              <value>opcion1</value>
            </item>
            <item>
              <label>Opción 2</label>
              <value>opcion2</value>
            </item>
            <item>
              <label>Opción 3</label>
              <value>opcion3</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/D1.27/tarifasPlanes/caracteristicas/segment">
            <label>Segmento</label>
            <hint>Segmento</hint>
            <item>
              <label>Seleccione opción</label>
              <value>-1</value>
            </item>
            <item>
              <label>HOGARES</label>
              <value>HOGARES</value>
            </item>
            <item>
              <label>COMERCIAL</label>
              <value>COMERCIAL</value>
            </item>
            <item>
              <label>JUBILADOS</label>
              <value>JUBILADOS</value>
            </item>
            <item>
              <label>GOBIERNO</label>
              <value>GOBIERNO</value>
            </item>
            <item>
              <label>PROFESIONALES</label>
              <value>PROFESIONALES</value>
            </item>
            <item>
              <label>OTROS</label>
              <value>OTROS</value>
            </item>
          </select1>
        </group>
        <group appearance="col3 well-white" ref="/D1.27/tarifasPlanes/cargos">
          <label>Cargos Fijos Mensuales</label>
          <input ref="/D1.27/tarifasPlanes/cargos/abono">
            <label>Abono</label>
            <hint>Abono</hint>
          </input>
          <input ref="/D1.27/tarifasPlanes/cargos/gastos">
            <label>Gastos Administrativos</label>
            <hint>Gastos Administrativos</hint>
          </input>
          <input ref="/D1.27/tarifasPlanes/cargos/total">
            <label>Total</label>
            <hint>Total</hint>
          </input>
        </group>
        <group appearance="col4 well-white" ref="/D1.27/tarifasPlanes/servicios">
          <label>Servicios Inluidos en Abono</label>
          <input ref="/D1.27/tarifasPlanes/servicios/minutos">
            <label>Minutos</label>
            <hint>Minutos</hint>
          </input>
          <input ref="/D1.27/tarifasPlanes/servicios/sms">
            <label>SMS</label>
            <hint>SMS</hint>
          </input>
          <input ref="/D1.27/tarifasPlanes/servicios/mms">
            <label>MMS</label>
            <hint>MMS</hint>
          </input>
          <input ref="/D1.27/tarifasPlanes/servicios/serviciosAdic">
            <label>Servicios Adicionales</label>
            <hint>Servicios Adicionales</hint>
          </input>
        </group>
        <group appearance="col1 well-white" ref="/D1.27/tarifasPlanes/precios">
          <label>Precios</label>
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/precios/minuto">
            <label>Minuto Local</label>
            <input ref="/D1.27/tarifasPlanes/precios/minuto/minutoOn">
              <label>ON NET</label>
              <hint>ON NET</hint>
            </input>
            <input ref="/D1.27/tarifasPlanes/precios/minuto/minutoOff">
              <label>OFF NET</label>
              <hint>OFF NET</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/precios/sms">
            <label>SMS</label>
            <input ref="/D1.27/tarifasPlanes/precios/sms/smsOn">
              <label>ON NET</label>
              <hint>ON NET</hint>
            </input>
            <input ref="/D1.27/tarifasPlanes/precios/sms/smsOff">
              <label>OFF NET</label>
              <hint>OFF NET</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/precios/mms">
            <label>MMS</label>
            <input ref="/D1.27/tarifasPlanes/precios/mms/mmsOn">
              <label>ON NET</label>
              <hint>ON NET</hint>
            </input>
            <input ref="/D1.27/tarifasPlanes/precios/mms/mmsOff">
              <label>OFF NET</label>
              <hint>OFF NET</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/precios/datos">
            <label>Datos</label>
            <input ref="/D1.27/tarifasPlanes/precios/datos/datosOn">
              <label>ON NET</label>
              <hint>ON NET</hint>
            </input>
            <input ref="/D1.27/tarifasPlanes/precios/datos/datosOff">
              <label>OFF NET</label>
              <hint>OFF NET</hint>
            </input>
          </group>
        </group>
        <group appearance="col1 well-white" ref="/D1.27/tarifasPlanes/otrosImp">
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/otrosImp/otros">
            <label>Otros</label>
            <input ref="/D1.27/tarifasPlanes/otrosImp/otros/otrosOtros">
              <label>Otros</label>
              <hint>Otros</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.27/tarifasPlanes/otrosImp/impuestos">
            <label>Impuestos</label>
            <input ref="/D1.27/tarifasPlanes/otrosImp/impuestos/porcentaje">
              <label>Porcentaje</label>
              <hint>Porcentaje</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
