<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C1.15 - Tráfico hacia móviles (llamadas)</h:title>
    <model>
      <instance>
        <C1.15 id="C1.15">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <l_l>
              <saliente_1/>
              <operador_1/>
            </l_l>
            <l_ldn>
              <saliente_2/>
              <operador_2/>
            </l_ldn>
            <l_ldi>
              <saliente_3/>
              <operador_3/>
            </l_ldi>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.15>
      </instance>
      <bind constraint="true()" nodeset="/C1.15/mes/provincia" required="true()" type="select1"/>
      <bind constraint="true()" nodeset="/C1.15/mes/partido" required="true()" type="select1"/>
      <bind constraint="true()" nodeset="/C1.15/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/C1.15/mes/areas_local" required="true()" type="select1"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_l/saliente_1" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_l/operador_1" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_ldn/saliente_2" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_ldn/operador_2" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_ldi/saliente_3" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.15/mes/l_ldi/operador_3" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.15/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
