<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.50 - Inversiones</h:title>
    <model>
      <instance>
        <C1.50 id="C1.50">
          <mes>
            <redes_de_acceso>
              <acceso_fijo>0</acceso_fijo>
              <acceso_movil>0</acceso_movil>
            </redes_de_acceso>
            <transporte>
              <transporte_optico>0</transporte_optico>
              <transporte_cobre>0</transporte_cobre>
              <transporte_radioelectrico_radioenlaces>0</transporte_radioelectrico_radioenlaces>
              <transporte_radioelectrico_satelital>0</transporte_radioelectrico_satelital>
            </transporte>
            <obra_civil>
              <equipamiento_asociado_redes>0</equipamiento_asociado_redes>
              <obra_civil_redes_inalambricas>0</obra_civil_redes_inalambricas>
              <obra_civil_redes_acceso>0</obra_civil_redes_acceso>
              <obra_civil_plantel_exterior>0</obra_civil_plantel_exterior>
            </obra_civil>
            <equipamiento>
              <equipamiento_redes>0</equipamiento_redes>
              <equipamiento_instalaciones>0</equipamiento_instalaciones>
            </equipamiento>
            <software>
              <software_administrar_red>0</software_administrar_red>
              <software_monitoreo_control>0</software_monitoreo_control>
              <software_mantenimiento>0</software_mantenimiento>
              <software_nodo>0</software_nodo>
            </software>
            <licencias>
              <licencias_software>0</licencias_software>
              <licencias_hardware>0</licencias_hardware>
              <licencias_regulatorias>0</licencias_regulatorias>
            </licencias>
            <infraestructura_administrativa_comercial>0</infraestructura_administrativa_comercial>
            <desarrollo_su>0</desarrollo_su>
            <otros_otros>0</otros_otros>
            <total_total>0</total_total>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.50>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/redes_de_acceso/acceso_fijo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/redes_de_acceso/acceso_movil" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/transporte/transporte_optico" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/transporte/transporte_cobre" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/transporte/transporte_radioelectrico_radioenlaces" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/transporte/transporte_radioelectrico_satelital" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/obra_civil/equipamiento_asociado_redes" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/obra_civil/obra_civil_redes_inalambricas" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/obra_civil/obra_civil_redes_acceso" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/obra_civil/obra_civil_plantel_exterior" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/equipamiento/equipamiento_redes" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/equipamiento/equipamiento_instalaciones" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/software/software_administrar_red" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/software/software_monitoreo_control" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/software/software_mantenimiento" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/software/software_nodo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/licencias/licencias_software" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/licencias/licencias_hardware" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C1.50/mes/licencias/licencias_regulatorias" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/infraestructura_administrativa_comercial" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/mes/desarrollo_su" required="true()" type="int"/>
      <bind nodeset="/C1.50/mes/otros_otros" required="true()" type="int"/>
      <bind nodeset="/C1.50/mes/total_total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.50/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col2 well-white" ref="/C1.50/mes">
      <label>Enero</label>
      <group ref="/C1.50/mes/redes_de_acceso">
        <label>Redes de Acceso</label>
        <input ref="/C1.50/mes/redes_de_acceso/acceso_fijo">
          <label>Fijo:</label>
          <hint>Acceso Fijo [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/redes_de_acceso/acceso_movil">
          <label>Móvil:</label>
          <hint>Acceso Móvil [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.50/mes/transporte">
        <label>Transporte</label>
        <input ref="/C1.50/mes/transporte/transporte_optico">
          <label>Óptico:</label>
          <hint>Transporte Óptico [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/transporte/transporte_cobre">
          <label>Cobre:</label>
          <hint>Transporte Cobre [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/transporte/transporte_radioelectrico_radioenlaces">
          <label>Radioeléctrico Radioenlaces:</label>
          <hint>Transporte Radioeléctrico. Radioenlaces [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/transporte/transporte_radioelectrico_satelital">
          <label>Radioeléctrico Satelital:</label>
          <hint>Transporte Radioeléctrico. Satelital [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.50/mes/obra_civil">
        <label>Obra Civil</label>
        <input ref="/C1.50/mes/obra_civil/equipamiento_asociado_redes">
          <label>Redes de Transporte:</label>
          <hint>Obra Civil. Redes de Transporte [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/obra_civil/obra_civil_redes_inalambricas">
          <label>Redes Inalámbricas:</label>
          <hint>Obra Civil. Redes Inalámbricas [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/obra_civil/obra_civil_redes_acceso">
          <label>Redes de Acceso:</label>
          <hint>Obra Civil. Redes de Acceso [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/obra_civil/obra_civil_plantel_exterior">
          <label>Plantel Exterior:</label>
          <hint>Obra Civil. Plantel Exterior [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="well-white" ref="/C1.50/mes/equipamiento">
        <label>Equipamiento</label>
        <input ref="/C1.50/mes/equipamiento/equipamiento_redes">
          <label>Redes de Transporte Acceso fijo y móvil:</label>
          <hint>Equipamiento asociado a las redes de transporte y acceso fijo y móvil [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/equipamiento/equipamiento_instalaciones">
          <label>Instalaciones de los Clientes</label>
          <hint>Equipamiento en Instalaciones de los Clientes [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.50/mes/software">
        <label>Software</label>
        <input ref="/C1.50/mes/software/software_administrar_red">
          <label>Administración la Red:</label>
          <hint>Software para administracion de red [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/software/software_monitoreo_control">
          <label>Monitoreo y Control:</label>
          <hint>Software para Monitoreo y control [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/software/software_mantenimiento">
          <label>Mantenimiento:</label>
          <hint>Software para mantenimiento [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/software/software_nodo">
          <label>Nodo o Datacenter de Alojamiento:</label>
          <hint>Software para NODO O DATACENTER DE ALOJAMIENTO [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.50/mes/licencias">
        <label>Licencias</label>
        <input ref="/C1.50/mes/licencias/licencias_software">
          <label>Software:</label>
          <hint>Licencias Software [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/licencias/licencias_hardware">
          <label>Hardware:</label>
          <hint>Licencias Hardware [en miles de pesos]</hint>
        </input>
        <input ref="/C1.50/mes/licencias/licencias_regulatorias">
          <label>Regulatorias:</label>
          <hint>Licencias Regulatorias [en miles de pesos]</hint>
        </input>
      </group>
      <input ref="/C1.50/mes/infraestructura_administrativa_comercial">
        <label>Infraestructura Administrativa/Comercial:</label>
        <hint>Infraestructura administrativa/comercial [en miles de pesos]</hint>
      </input>
      <input ref="/C1.50/mes/desarrollo_su">
        <label>Desarrollo del Servicio Universal:</label>
        <hint>Desarrollo del Servicio Universal, conf Pto 10.1 f) Anexo I Dtoo. 764/2000 [en miles de pesos]</hint>
      </input>
      <input ref="/C1.50/mes/otros_otros">
        <label>Otros</label>
      </input>
      <input ref="/C1.50/mes/total_total">
        <label>Total</label>
      </input>
    </group>
  </h:body>
</h:html>
