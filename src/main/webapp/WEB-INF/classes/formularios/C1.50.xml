<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.50 - Inversiones</h:title>
    <model>
      <instance>
        <C1.50 id="C1.50">
          <meses jr:template="">
            <mes>
              <redes_de_acceso>
                <acceso_fijo/>
                <acceso_movil/>
              </redes_de_acceso>
              <transporte>
                <transporte_optico/>
                <transporte_cobre/>
                <transporte_radioelectrico_radioenlaces/>
                <transporte_radioelectrico_satelital/>
              </transporte>
              <obra_civil>
                <ocivil_transporte/>
                <ocivil_inalambricas/>
                <ocivil_acceso/>
                <ocivil_plantel/>
              </obra_civil>
              <equipamiento>
                <e_redes/>
                <e_instalaciones/>
              </equipamiento>
              <software>
                <s_administracion/>
                <s_monitoreo/>
                <s_mantenimiento/>
                <s_nodo/>
              </software>
              <licencias>
                <l_software/>
                <l_hardware/>
                <l_regulatorias/>
              </licencias>
              <infraestructura/>
              <desarrollo/>
              <otros_otros/>
              <calculate1/>
              <total_total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.50>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/redes_de_acceso/acceso_fijo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/redes_de_acceso/acceso_movil" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/transporte/transporte_optico" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/transporte/transporte_cobre" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/transporte/transporte_radioelectrico_radioenlaces" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/transporte/transporte_radioelectrico_satelital" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/obra_civil/ocivil_transporte" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/obra_civil/ocivil_inalambricas" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/obra_civil/ocivil_acceso" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/obra_civil/ocivil_plantel" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/equipamiento/e_redes" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/equipamiento/e_instalaciones" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/software/s_administracion" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/software/s_monitoreo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/software/s_mantenimiento" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/software/s_nodo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/licencias/l_software" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/licencias/l_hardware" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Debe ingresar un numero entero hasta 12 digitos y mayor a 0" nodeset="/C1.50/meses/mes/licencias/l_regulatorias" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/infraestructura" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/desarrollo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.50/meses/mes/otros_otros" required="true()" type="int"/>
      <bind calculate=" /C1.50/meses/mes/redes_de_acceso/acceso_fijo + /C1.50/meses/mes/redes_de_acceso/acceso_movil + /C1.50/meses/mes/transporte/transporte_optico + /C1.50/meses/mes/transporte/transporte_cobre + /C1.50/meses/mes/transporte/transporte_radioelectrico_radioenlaces + /C1.50/meses/mes/transporte/transporte_radioelectrico_satelital + /C1.50/meses/mes/obra_civil/ocivil_transporte + /C1.50/meses/mes/obra_civil/ocivil_inalambricas + /C1.50/meses/mes/obra_civil/ocivil_acceso + /C1.50/meses/mes/obra_civil/ocivil_plantel + /C1.50/meses/mes/equipamiento/e_redes + /C1.50/meses/mes/equipamiento/e_instalaciones + /C1.50/meses/mes/software/s_administracion + /C1.50/meses/mes/software/s_monitoreo + /C1.50/meses/mes/software/s_mantenimiento + /C1.50/meses/mes/software/s_nodo + /C1.50/meses/mes/licencias/l_software + /C1.50/meses/mes/licencias/l_hardware + /C1.50/meses/mes/licencias/l_regulatorias + /C1.50/meses/mes/infraestructura + /C1.50/meses/mes/desarrollo + /C1.50/meses/mes/otros_otros " nodeset="/C1.50/meses/mes/calculate1" type="string"/>
      <bind nodeset="/C1.50/meses/mes/total_total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.50/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.50/meses">
      <label></label>
      <repeat nodeset="/C1.50/meses">
        <group appearance="col1 well-white" ref="/C1.50/meses/mes">
          <label>{title}</label>
          <group appearance="col1 well-white" ref="/C1.50/meses/mes/redes_de_acceso">
            <label>Redes de Acceso</label>
            <input ref="/C1.50/meses/mes/redes_de_acceso/acceso_fijo">
              <label>Fijo:</label>
              <hint>Acceso Fijo [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/redes_de_acceso/acceso_movil">
              <label>Móvil:</label>
              <hint>Acceso Móvil [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/transporte">
            <label>Transporte</label>
            <input ref="/C1.50/meses/mes/transporte/transporte_optico">
              <label>Óptico:</label>
              <hint>Transporte Óptico [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporte_cobre">
              <label>Cobre:</label>
              <hint>Transporte Cobre [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporte_radioelectrico_radioenlaces">
              <label>Radioeléctrico Radioenlaces:</label>
              <hint>Transporte Radioeléctrico. Radioenlaces [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/transporte/transporte_radioelectrico_satelital">
              <label>Radioeléctrico Satelital:</label>
              <hint>Transporte Radioeléctrico. Satelital [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/obra_civil">
            <label>Obra Civil</label>
            <input ref="/C1.50/meses/mes/obra_civil/ocivil_transporte">
              <label>Redes de Transporte:</label>
              <hint>Obra Civil. Redes de Transporte [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivil_inalambricas">
              <label>Redes Inalámbricas:</label>
              <hint>Obra Civil. Redes Inalámbricas [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivil_acceso">
              <label>Redes de Acceso:</label>
              <hint>Obra Civil. Redes de Acceso [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/obra_civil/ocivil_plantel">
              <label>Plantel Exterior:</label>
              <hint>Obra Civil. Plantel Exterior [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col1 well-white" ref="/C1.50/meses/mes/equipamiento">
            <label>Equipamiento</label>
            <input ref="/C1.50/meses/mes/equipamiento/e_redes">
              <label>Redes de Transporte Acceso fijo y móvil:</label>
              <hint>Equipamiento asociado a las redes de transporte y acceso fijo y móvil [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/equipamiento/e_instalaciones">
              <label>Instalaciones de los Clientes</label>
              <hint>Equipamiento en Instalaciones de los Clientes [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/software">
            <label>Software</label>
            <input ref="/C1.50/meses/mes/software/s_administracion">
              <label>Administración la Red:</label>
              <hint>Software para administracion de red [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/s_monitoreo">
              <label>Monitoreo y Control:</label>
              <hint>Software para Monitoreo y control [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/s_mantenimiento">
              <label>Mantenimiento:</label>
              <hint>Software para mantenimiento [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/software/s_nodo">
              <label>Nodo o Datacenter de Alojamiento:</label>
              <hint>Software para Nodo o Datacenter de Alojamiento [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.50/meses/mes/licencias">
            <label>Licencias</label>
            <input ref="/C1.50/meses/mes/licencias/l_software">
              <label>Software:</label>
              <hint>Licencias Software [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/licencias/l_hardware">
              <label>Hardware:</label>
              <hint>Licencias Hardware [en miles de pesos]</hint>
            </input>
            <input ref="/C1.50/meses/mes/licencias/l_regulatorias">
              <label>Regulatorias:</label>
              <hint>Licencias Regulatorias [en miles de pesos]</hint>
            </input>
          </group>
          <input ref="/C1.50/meses/mes/infraestructura">
            <label>Infraestructura Administrativa/Comercial:</label>
            <hint>Infraestructura administrativa/comercial [en miles de pesos]</hint>
          </input>
          <input ref="/C1.50/meses/mes/desarrollo">
            <label>Desarrollo del Servicio Universal:</label>
            <hint>Desarrollo del Servicio Universal, conf Pto 10.1 f) Anexo I Dtoo. 764/2000 [en miles de pesos]</hint>
          </input>
          <input ref="/C1.50/meses/mes/otros_otros">
            <label>Otros</label>
            <hint>Otros</hint>
          </input>
          <input ref="/C1.50/meses/mes/total_total">
            <label>Total <output value=" /C1.50/meses/mes/calculate1 "/></label></input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
