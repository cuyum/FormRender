<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario F1.13 - Calidad de servicio / Atención personalizada</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <F1.13 id="F1.13">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <centrosAtencion/>
              <personal/>
              <esperaPromedio/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </F1.13>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/F1.13/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/F1.13/meses/mes/provincia" nodeset="/F1.13/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/F1.13/meses/mes/partido" nodeset="/F1.13/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/F1.13/meses/mes/localidad_otro" relevant=" /F1.13/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="mask=999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.13/meses/mes/centrosAtencion" required="true()" type="int"/>
      <bind constraint="mask=999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.13/meses/mes/personal" required="true()" type="int"/>
      <bind constraint="mask=999.999" jr:constraintMsg="Datos no válidos" nodeset="/F1.13/meses/mes/esperaPromedio" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/F1.13/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/F1.13/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/F1.13/meses">
        <group appearance="col4 well-white variable" ref="/F1.13/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/F1.13/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.13/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/F1.13/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/F1.13/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
            <hint>Localidad Otros</hint>
          </input>
          <input ref="/F1.13/meses/mes/centrosAtencion">
            <label>Centros de Atención</label>
            <hint>Centros de Atención</hint>
          </input>
          <input ref="/F1.13/meses/mes/personal">
            <label>Personal</label>
            <hint>Personal</hint>
          </input>
          <input ref="/F1.13/meses/mes/esperaPromedio">
            <label>Espera Promedio</label>
            <hint>Espera Promedio</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
