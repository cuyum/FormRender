<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>C1.14 - Tráfico hacia móviles (minutos)</h:title>
    <model>
      <instance>
        <C1.14 id="C1.14">
          <meses jr:template="">
            <mes>
              <provincia/>
              <partido/>
              <localidad/>
              <localidad_otro/>
              <area/>
              <area_otro/>
              <l_l>
                <l_salientes1/>
                <l_operador1/>
                <l_salientes2/>
                <l_operador2/>
                <l_salientes3/>
                <l_operador3/>
                <l_salientes4/>
                <l_operador4/>
              </l_l>
              <l_ldn>
                <ldn_salientes1/>
                <ldn_operador1/>
                <ldn_salientes2/>
                <ldn_operador2/>
                <ldn_salientes3/>
                <ldn_operador3/>
                <ldn_salientes4/>
                <ldn_operador4/>
              </l_ldn>
              <l_ldi>
                <ldi_salientes1/>
                <ldi_operador1/>
                <ldi_salientes2/>
                <ldi_operador2/>
                <ldi_salientes3/>
                <ldi_operador3/>
                <ldi_salientes4/>
                <ldi_operador4/>
              </l_ldi>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.14>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/provincias" nodeset="/C1.14/meses/mes/provincia" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/partidos and depends=/C1.14/meses/mes/provincia" nodeset="/C1.14/meses/mes/partido" required="true()" type="select1"/>
      <bind constraint="url=/localizacion/rest/localizaciones/localidades and depends=/C1.14/meses/mes/partido" nodeset="/C1.14/meses/mes/localidad" required="true()" type="select1"/>
      <bind nodeset="/C1.14/meses/mes/localidad_otro" relevant=" /C1.14/meses/mes/localidad =9999" required="true()" type="string"/>
      <bind constraint="url=/localizacion/rest/localizaciones/areas and depends=/C1.14/meses/mes/localidad" nodeset="/C1.14/meses/mes/area" required="true()" type="select1"/>
      <bind nodeset="/C1.14/meses/mes/area_otro" relevant=" /C1.14/meses/mes/area =9999" required="true()" type="string"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_l/l_salientes1" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_l/l_operador1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_l/l_salientes2" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_l/l_operador2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_l/l_salientes3" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_l/l_operador3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_l/l_salientes4" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_l/l_operador4" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldn/ldn_salientes1" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldn/ldn_operador1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldn/ldn_salientes2" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldn/ldn_operador2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldn/ldn_salientes3" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldn/ldn_operador3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldn/ldn_salientes4" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldn/ldn_operador4" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldi/ldi_salientes1" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldi/ldi_operador1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldi/ldi_salientes2" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldi/ldi_operador2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldi/ldi_salientes3" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldi/ldi_operador3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999,9" jr:constraintMsg="Datos no válidos" nodeset="/C1.14/meses/mes/l_ldi/ldi_salientes4" required="true()" type="decimal"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.14/meses/mes/l_ldi/ldi_operador4" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.14/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.14/meses">
      <label></label>
      <repeat appearance="grilla" nodeset="/C1.14/meses">
        <group appearance="col4 well-white variable" ref="/C1.14/meses/mes">
          <label>{title}</label>
          <select1 appearance="minimal" ref="/C1.14/meses/mes/provincia">
            <label>Provincia</label>
            <hint>Provincia</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.14/meses/mes/partido">
            <label>Partido / Departamento</label>
            <hint>Partido/Departamento</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <select1 appearance="minimal" ref="/C1.14/meses/mes/localidad">
            <label>Localidad</label>
            <hint>Localidad</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/C1.14/meses/mes/localidad_otro">
            <label>Localidad Otros</label>
          </input>
          <select1 appearance="minimal" ref="/C1.14/meses/mes/area">
            <label>Área Local</label>
            <hint>Área Local</hint>
            <item>
              <label>Seleccione una opción</label>
              <value>-1</value>
            </item>
          </select1>
          <input ref="/C1.14/meses/mes/area_otro">
            <label>Área Otros</label>
          </input>
          <group appearance="col2 well-white" ref="/C1.14/meses/mes/l_l">
            <label>L-L</label>
            <input ref="/C1.14/meses/mes/l_l/l_salientes1">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_operador1">
              <label>Operador 1</label>
              <hint>Operador 1</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_salientes2">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_operador2">
              <label>Operador 2</label>
              <hint>Operador 2</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_salientes3">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_operador3">
              <label>Operador 3</label>
              <hint>Operador 3</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_salientes4">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_l/l_operador4">
              <label>Operador Otros</label>
              <hint>Operador Otros</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.14/meses/mes/l_ldn">
            <label>L-LDN</label>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_salientes1">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_operador1">
              <label>Operador 1</label>
              <hint>Operador 1</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_salientes2">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_operador2">
              <label>Operador 2</label>
              <hint>Operador 2</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_salientes3">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_operador3">
              <label>Operador 3</label>
              <hint>Operador 3</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_salientes4">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldn/ldn_operador4">
              <label>Operador Otros</label>
              <hint>Operador Otros</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.14/meses/mes/l_ldi">
            <label>L-LDI</label>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_salientes1">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_operador1">
              <label>Operador 1</label>
              <hint>Operador 1</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_salientes2">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_operador2">
              <label>Operador 2</label>
              <hint>Operador 2</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_salientes3">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_operador3">
              <label>Operador 3</label>
              <hint>Operador 3</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_salientes4">
              <label>Salientes</label>
              <hint>Salientes [en miles]</hint>
            </input>
            <input ref="/C1.14/meses/mes/l_ldi/ldi_operador4">
              <label>Operador Otros</label>
              <hint>Operador Otros</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
