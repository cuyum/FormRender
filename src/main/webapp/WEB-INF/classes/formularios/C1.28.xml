<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C.1.28 - Ingresos</h:title>
    <model>
      <instance>
        <C1.28 id="C1.28">
          <mes>
            <stl>0</stl>
            <ldn>0</ldn>
            <ldi>0</ldi>
            <lineas>0</lineas>
            <otros>0</otros>
            <calculate1/>
            <total/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.28>
      </instance>
      <bind constraint=".&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 8 dígitos" nodeset="/C1.28/mes/stl" required="true()" type="int"/>
      <bind constraint=".&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 8 dígitos" nodeset="/C1.28/mes/ldn" required="true()" type="int"/>
      <bind constraint=".&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 8 dígitos" nodeset="/C1.28/mes/ldi" required="true()" type="int"/>
      <bind constraint=".&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 8 dígitos" nodeset="/C1.28/mes/lineas" required="true()" type="int"/>
      <bind constraint=".&lt;100000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero hasta 8 dígitos" nodeset="/C1.28/mes/otros" required="true()" type="int"/>
      <bind calculate=" /C1.28/mes/stl + /C1.28/mes/ldn + /C1.28/mes/ldi + /C1.28/mes/lineas + /C1.28/mes/otros " nodeset="/C1.28/mes/calculate1" type="string"/>
      <bind nodeset="/C1.28/mes/total" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.28/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.28/mes">
      <label>Enero</label>
      <input ref="/C1.28/mes/stl">
        <label>STL</label>
        <hint>8 dígitos</hint>
      </input>
      <input ref="/C1.28/mes/ldn">
        <label>LDN</label>
        <hint>8 dígitos</hint>
      </input>
      <input ref="/C1.28/mes/ldi">
        <label>LDI</label>
        <hint>8 dígitos</hint>
      </input>
      <input ref="/C1.28/mes/lineas">
        <label>Líneas Dierctas</label>
        <hint>8 dígitos</hint>
      </input>
      <input ref="/C1.28/mes/otros">
        <label>Otros</label>
        <hint>8 dígitos</hint>
      </input>
      <input ref="/C1.28/mes/total">
        <label>Total <output value=" /C1.28/mes/calculate1 "/></label></input>
    </group>
  </h:body>
</h:html>
