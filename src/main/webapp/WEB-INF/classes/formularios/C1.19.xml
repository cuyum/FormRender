<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.19 - Calidad de servicio / atención personalizada</h:title>
    <model>
      <instance>
        <C1.19 id="C1.19">
          <mes>
            <provincia2/>
            <partido2/>
            <localidad2/>
            <areas_local2/>
            <centro_at/>
            <personal/>
            <espera_prom/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.19>
      </instance>
      <bind constraint="url=/areas/provincias" jr:constraintMsg="true()" nodeset="/C1.19/mes/provincia2" required="true()" type="select1"/>
      <bind constraint="url=/areas/partidos and depends=/C1.19/mes/provincia" jr:constraintMsg="true()" nodeset="/C1.19/mes/partido2" required="true()" type="select1"/>
      <bind constraint="url=/areas/localidades and depends=/C1.19/mes/partido" jr:constraintMsg="true()" nodeset="/C1.19/mes/localidad2" required="true()" type="select1"/>
      <bind constraint="url=/areas/areas and depends=/C1.19/mes/localidad" nodeset="/C1.19/mes/areas_local2" required="true()" type="select1"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.19/mes/centro_at" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.19/mes/personal" required="true()" type="int"/>
      <bind constraint=".&lt;1000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de hasta 6 dígitos y mayor a 0" nodeset="/C1.19/mes/espera_prom" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.19/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.19/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.19/mes/provincia2">
        <label>Provincia</label>
        <hint>Seleccione Provincia</hint>
        <item>
          <label></label>
          <value>id_provincia</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.19/mes/partido2">
        <label>Partido</label>
        <hint>Seleccione Partido</hint>
        <item>
          <label></label>
          <value>id_dpto_partido</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.19/mes/localidad2">
        <label>Localidad</label>
        <hint>Seleccione Localidad</hint>
        <item>
          <label></label>
          <value>id_localidad</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.19/mes/areas_local2">
        <label>Área Local</label>
        <hint>Seleccione Área Local</hint>
        <item>
          <label></label>
          <value>id_area_local</value>
        </item>
      </select1>
      <input ref="/C1.19/mes/centro_at">
        <label>Centros de Atención</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.19/mes/personal">
        <label>Personal</label>
        <hint>Ingrese número entero</hint>
      </input>
      <input ref="/C1.19/mes/espera_prom">
        <label>Espera Promedio</label>
        <hint>Ingrese número entero</hint>
      </input>
    </group>
  </h:body>
</h:html>
