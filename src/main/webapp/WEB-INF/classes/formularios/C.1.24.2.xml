<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C.1.24 - INGRESOS INTERCONEXION A LA RED FIJA (LARGA DISTANCIA NACIONAL A 3)</h:title>
    <model>
      <instance>
        <C.1.24_2 id="C_1_24_2">
          <nota/>
          <mes>
            <nacional1>0</nacional1>
            <nacional2>0</nacional2>
            <nacional3>0</nacional3>
            <calculate2/>
            <nacional4/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C.1.24_2>
      </instance>
      <bind nodeset="/C.1.24_2/nota" readonly="true()" type="string"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.24_2/mes/nacional1" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.24_2/mes/nacional2" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un numero entero de 12 dígitos" nodeset="/C.1.24_2/mes/nacional3" required="true()" type="int"/>
      <bind calculate=" /C.1.24_2/mes/nacional1 + /C.1.24_2/mes/nacional2 + /C.1.24_2/mes/nacional3 " nodeset="/C.1.24_2/mes/calculate2" type="string"/>
      <bind nodeset="/C.1.24_2/mes/nacional4" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C.1.24_2/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/C.1.24_2/nota">
      <label>Expresar en miles de pesos 12 dígitos</label>
    </input>
    <group appearance="Col4" ref="/C.1.24_2/mes">
      <label>Mes</label>
      <input ref="/C.1.24_2/mes/nacional1">
        <label>Arriendo de Circuitos LDN</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C.1.24_2/mes/nacional2">
        <label>Transporte LDN</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C.1.24_2/mes/nacional3">
        <label>Otros</label>
        <hint>Miles de pesos 12 dígitos</hint>
      </input>
      <input ref="/C.1.24_2/mes/nacional4">
        <label>TOTAL: $ <output value=" /C.1.24_2/mes/calculate2 "/></label></input>
    </group>
  </h:body>
</h:html>
