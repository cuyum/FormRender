<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.51 - Comercio exterior</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <C1.51 id="C1.51">
          <meses jr:template="">
            <mes>
              <telecomunicExpo/>
              <telecomunicImpo/>
              <serviciosExpo/>
              <serviciosImpo/>
              <importacBienesCapital/>
              <importacionesPiezas/>
              <regalias/>
              <total>
                <totalExpo/>
                <totalImpo/>
              </total>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.51>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/telecomunicExpo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/telecomunicImpo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/serviciosExpo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/serviciosImpo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/importacBienesCapital" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/importacionesPiezas" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/regalias" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/total/totalExpo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/total/totalImpo" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.51/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.51/meses">
      <label></label>
      <repeat nodeset="/C1.51/meses">
        <group appearance="col4 well-white variable" ref="/C1.51/meses/mes">
          <label>{title}</label>
          <input ref="/C1.51/meses/mes/telecomunicExpo">
            <label>Telecomunicaciones Expo.</label>
            <hint>Expo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/telecomunicImpo">
            <label>Telecomunicaciones Impo.</label>
            <hint>Impo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/serviciosExpo">
            <label>Servicios Expo.</label>
            <hint>Expo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/serviciosImpo">
            <label>Servicios Impo.</label>
            <hint>Impo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/importacBienesCapital">
            <label>Importaciones Bienes Capital</label>
            <hint>Importaciones de Bienes de Capital [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/importacionesPiezas">
            <label>Importaciones de Piezas</label>
            <hint>Importaciones de Piezas [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/regalias">
            <label>Regalías</label>
            <hint>Regalías [en miles de pesos]</hint>
          </input>
          <group appearance="col4 well-white" ref="/C1.51/meses/mes/total">
            <input ref="/C1.51/meses/mes/total/totalExpo">
              <label>Totales expo.</label>
              <hint>Expo [en miles de pesos]</hint>
            </input>
            <input ref="/C1.51/meses/mes/total/totalImpo">
              <label>Totales impo.</label>
              <hint>Impo [en miles de pesos]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
