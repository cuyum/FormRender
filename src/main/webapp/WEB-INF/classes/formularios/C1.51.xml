<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.51 - Comercio exterior</h:title>
    <model>
      <instance>
        <C1.51 id="C1.51">
          <meses jr:template="">
            <mes>
              <tele_expo/>
              <tele_impo/>
              <serv_expo/>
              <serv_impo/>
              <impo_bienes/>
              <impo_piezas/>
              <regalias/>
              <total>
                <calculate1/>
                <calculate2/>
                <total1/>
                <total2/>
              </total>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.51>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/tele_expo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/tele_impo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/serv_expo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/serv_impo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/impo_bienes" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/impo_piezas" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.51/meses/mes/regalias" type="int"/>
      <bind calculate=" /C1.51/meses/mes/tele_expo + /C1.51/meses/mes/serv_expo " nodeset="/C1.51/meses/mes/total/calculate1" type="string"/>
      <bind calculate=" /C1.51/meses/mes/tele_impo + /C1.51/meses/mes/serv_impo + /C1.51/meses/mes/impo_bienes + /C1.51/meses/mes/impo_piezas " nodeset="/C1.51/meses/mes/total/calculate2" type="string"/>
      <bind constraint="mask=999.999.999.999" nodeset="/C1.51/meses/mes/total/total1" readonly="true()" type="string"/>
      <bind constraint="mask=999.999.999.999" nodeset="/C1.51/meses/mes/total/total2" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.51/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.51/meses">
      <label></label>
      <repeat nodeset="/C1.51/meses">
        <group appearance="col4 well-white" ref="/C1.51/meses/mes">
          <label>{title}</label>
          <input ref="/C1.51/meses/mes/tele_expo">
            <label>Telecomunicaciones Expo.:</label>
            <hint>Expo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/tele_impo">
            <label>Telecomunicaciones Impo.:</label>
            <hint>Impo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/serv_expo">
            <label>Servicios Expo.:</label>
            <hint>Expo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/serv_impo">
            <label>Servicios Impo.:</label>
            <hint>Impo [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/impo_bienes">
            <label>Importaciones Bienes Capital:</label>
            <hint>Importaciones de Bienes de Capital [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/impo_piezas">
            <label>Importaciones de Piezas:</label>
            <hint>Importaciones de Piezas [en miles de pesos]</hint>
          </input>
          <input ref="/C1.51/meses/mes/regalias">
            <label>Regalías:</label>
            <hint>Regalías [en miles de pesos]</hint>
          </input>
          <group appearance="col1 well-white" ref="/C1.51/meses/mes/total">
            <label>Total</label>
            <input ref="/C1.51/meses/mes/total/total1">
              <label>Totales expo.: <output value=" /C1.51/meses/mes/total/calculate1 "/></label><hint>Expo [en miles de pesos]</hint>
            </input>
            <input ref="/C1.51/meses/mes/total/total2">
              <label>Totales impo.: <output value=" /C1.51/meses/mes/total/calculate2 "/></label><hint>Impo [en miles de pesos]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
