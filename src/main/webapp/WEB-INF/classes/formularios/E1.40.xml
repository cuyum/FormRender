<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario E1.40 - Comercio exterior</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <E1.40 id="E1.40">
          <importaciones>
            <importacionTelecomunic/>
            <importacionServicios/>
            <importacionBienesCapital/>
            <importacionPiezas/>
            <importacionTotal/>
          </importaciones>
          <exportaciones>
            <exportacionTelecomunic/>
            <exportacionServicios/>
            <exportacionTotal/>
          </exportaciones>
          <grupoRegalias>
            <regalias/>
          </grupoRegalias>
          <meta>
            <instanceID/>
          </meta>
        </E1.40>
      </instance>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/importaciones/importacionTelecomunic" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/importaciones/importacionServicios" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/importaciones/importacionBienesCapital" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/importaciones/importacionPiezas" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/importaciones/importacionTotal" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/exportaciones/exportacionTelecomunic" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/exportaciones/exportacionServicios" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/exportaciones/exportacionTotal" required="true()" type="int"/>
      <bind constraint=".&gt;=0 and .&lt;=999999999" jr:constraintMsg="Ingrese entero entre 0 y 999.999.999" nodeset="/E1.40/grupoRegalias/regalias" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/E1.40/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/E1.40/importaciones">
      <label>Importaciones</label>
      <input ref="/E1.40/importaciones/importacionTelecomunic">
        <label>Telecomunicaciones</label>
        <hint>Telecomunicaciones [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/importaciones/importacionServicios">
        <label>Servicios</label>
        <hint>Servicios [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/importaciones/importacionBienesCapital">
        <label>Bienes de Capital</label>
        <hint>Bienes de Capital [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/importaciones/importacionPiezas">
        <label>Piezas</label>
        <hint>Piezas [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/importaciones/importacionTotal">
        <label>Total</label>
        <hint>Total [en miles de pesos]</hint>
      </input>
    </group>
    <group appearance="col4 well-white" ref="/E1.40/exportaciones">
      <label>Exportaciones</label>
      <input ref="/E1.40/exportaciones/exportacionTelecomunic">
        <label>Telecomunicaciones</label>
        <hint>Telecomunicaciones [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/exportaciones/exportacionServicios">
        <label>Servicios</label>
        <hint>Servicios [en miles de pesos]</hint>
      </input>
      <input ref="/E1.40/exportaciones/exportacionTotal">
        <label>Total</label>
        <hint>Total [en miles de pesos]</hint>
      </input>
    </group>
    <group appearance="col4 well-white" ref="/E1.40/grupoRegalias">
      <label>Regalías</label>
      <input ref="/E1.40/grupoRegalias/regalias">
        <label>Regalías</label>
        <hint>Regalías [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
