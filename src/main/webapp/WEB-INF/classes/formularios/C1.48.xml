<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.48 - Costos de interconexión</h:title>
    <model>
      <instance>
        <C1.48 id="C1.48">
          <mes>
            <red_movil>
              <prove1/>
              <costo1>0</costo1>
              <prove2/>
              <costo2>0</costo2>
              <prove3/>
              <costo3>0</costo3>
              <prove_otros/>
              <costo_otros>0</costo_otros>
            </red_movil>
            <red_fija>
              <prove1_1/>
              <costo1_1>0</costo1_1>
              <prove2_1/>
              <costo2_1>0</costo2_1>
              <prove_otros_1/>
              <costo_otros_1>0</costo_otros_1>
              <mantenimiento/>
              <red_movil_otros>0</red_movil_otros>
            </red_fija>
            <otros>
              <mantenimiento1>0</mantenimiento1>
              <otros1/>
            </otros>
            <total1>
              <calculate1/>
              <total_red/>
            </total1>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.48>
      </instance>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_movil/prove1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_movil/costo1" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_movil/prove2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_movil/costo2" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_movil/prove3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_movil/costo3" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_movil/prove_otros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_movil/costo_otros" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_fija/prove1_1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_fija/costo1_1" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_fija/prove2_1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_fija/costo2_1" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_fija/prove_otros_1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_fija/costo_otros_1" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{2}[0-9]{8}[0-9]$') and type=cuit" jr:constraintMsg="11 dígitos sin guiones ni espacios" nodeset="/C1.48/mes/red_fija/mantenimiento" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/red_fija/red_movil_otros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/otros/mantenimiento1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/otros/otros1" required="true()" type="int"/>
      <bind calculate=" /C1.48/mes/red_movil/costo1 + /C1.48/mes/red_movil/costo2 + /C1.48/mes/red_movil/costo3 + /C1.48/mes/red_movil/costo_otros + /C1.48/mes/red_fija/costo1_1 + /C1.48/mes/red_fija/costo2_1 + /C1.48/mes/red_fija/costo_otros_1 + /C1.48/mes/red_fija/red_movil_otros + /C1.48/mes/otros/mantenimiento1 + /C1.48/mes/otros/otros1 " nodeset="/C1.48/mes/total1/calculate1" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/mes/total1/total_red" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.48/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col1 well-white" ref="/C1.48/mes">
      <label>Enero</label>
      <group appearance="col2 well-white" ref="/C1.48/mes/red_movil">
        <label>Costo interconexión a red móvil</label>
        <input ref="/C1.48/mes/red_movil/prove1">
          <label>Proveedor</label>
          <hint>Prove 1</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/costo1">
          <label>Costo</label>
          <hint>Costo 1 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/prove2">
          <label>Proveedor</label>
          <hint>Prove 2</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/costo2">
          <label>Costo</label>
          <hint>Costo 2 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/prove3">
          <label>Proveedor</label>
          <hint>Prove 3</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/costo3">
          <label>Costo</label>
          <hint>Costo 3 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/prove_otros">
          <label>Proveedor Otros</label>
          <hint>Prove Otros</hint>
        </input>
        <input ref="/C1.48/mes/red_movil/costo_otros">
          <label>Costo Otros</label>
          <hint>Costo Otros [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.48/mes/red_fija">
        <label>Costo interconexión a red fija</label>
        <input ref="/C1.48/mes/red_fija/prove1_1">
          <label>Proveedor</label>
          <hint>Prove 1</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/costo1_1">
          <label>Costo</label>
          <hint>Costo 1 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/prove2_1">
          <label>Proveedor</label>
          <hint>Prove 2</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/costo2_1">
          <label>Costo</label>
          <hint>Costo 2 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/prove_otros_1">
          <label>Proveedor</label>
          <hint>Prove 3</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/costo_otros_1">
          <label>Costo</label>
          <hint>Costo 3 [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/mantenimiento">
          <label>Proveedor Otros</label>
          <hint>Prove Otros</hint>
        </input>
        <input ref="/C1.48/mes/red_fija/red_movil_otros">
          <label>Costo Otros</label>
          <hint>Costo Otros [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.48/mes/otros">
        <label>Otros</label>
        <input ref="/C1.48/mes/otros/mantenimiento1">
          <label>Mantenimiento</label>
          <hint>Mantenimiento de Redes [en miles de pesos]</hint>
        </input>
        <input ref="/C1.48/mes/otros/otros1">
          <label>Otros</label>
          <hint>Otros [en miles de pesos]</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.48/mes/total1">
        <label>Total</label>
        <input ref="/C1.48/mes/total1/total_red">
          <label>Total <output value=" /C1.48/mes/total1/calculate1 "/></label><hint>Total [en miles de pesos]</hint>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
