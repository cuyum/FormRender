<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.48 - Costos de interconexión</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <C1.48 id="C1.48">
          <meses jr:template="">
            <mes>
              <redMovil>
                <redMovilPrestador1/>
                <redMovilCosto1/>
                <redMovilPrestador2/>
                <redMovilCosto2/>
                <redMovilPrestador3/>
                <redMovilCcosto3/>
                <redMovilPrestadorOtros/>
                <redMovilCostoOtros/>
              </redMovil>
              <redFija>
                <redFijaPrestador1/>
                <redFijaCosto1/>
                <redFijaPrestador2/>
                <redFijaCosto2/>
                <redFijaPrestador3/>
                <redFijaCosto3/>
                <redFijaPrestadorOtros/>
                <redFijaCostoOtros/>
              </redFija>
              <otros>
                <mantenimiento/>
                <otros/>
              </otros>
              <grupoTotal>
                <total/>
              </grupoTotal>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.48>
      </instance>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilPrestador1" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilCosto1" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilPrestador2" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilCosto2" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilPrestador3" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilCcosto3" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.48/meses/mes/redMovil/redMovilPrestadorOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redMovil/redMovilCostoOtros" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaPrestador1" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaCosto1" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaPrestador2" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaCosto2" required="true()" type="int"/>
      <bind constraint="url=/localizacion/rest/localizaciones/prestadores and cuit" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaPrestador3" required="true()" type="select1"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaCosto3" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/C1.48/meses/mes/redFija/redFijaPrestadorOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/redFija/redFijaCostoOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/otros/mantenimiento" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/otros/otros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.48/meses/mes/grupoTotal/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.48/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.48/meses">
      <label></label>
      <repeat nodeset="/C1.48/meses">
        <group appearance="col1 well-white variable" ref="/C1.48/meses/mes">
          <label>{title}</label>
          <group appearance="col2 well-white" ref="/C1.48/meses/mes/redMovil">
            <label>Costo interconexión a red móvil</label>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redMovil/redMovilPrestador1">
              <label>Prestador</label>
              <hint>Prestador 1</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redMovil/redMovilCosto1">
              <label>Costo</label>
              <hint>Costo 1 [en miles de pesos]</hint>
            </input>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redMovil/redMovilPrestador2">
              <label>Prestador</label>
              <hint>Prestador 2</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redMovil/redMovilCosto2">
              <label>Costo</label>
              <hint>Costo 2 [en miles de pesos]</hint>
            </input>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redMovil/redMovilPrestador3">
              <label>Prestador</label>
              <hint>Prestador 3</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redMovil/redMovilCcosto3">
              <label>Costo</label>
              <hint>Costo 3 [en miles de pesos]</hint>
            </input>
            <input ref="/C1.48/meses/mes/redMovil/redMovilPrestadorOtros">
              <label>Prestador Otros</label>
              <hint>Prestador Otros</hint>
            </input>
            <input ref="/C1.48/meses/mes/redMovil/redMovilCostoOtros">
              <label>Costo Otros</label>
              <hint>Costo Otros [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.48/meses/mes/redFija">
            <label>Costo interconexión a red fija</label>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redFija/redFijaPrestador1">
              <label>Prestador</label>
              <hint>Prestador 1</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redFija/redFijaCosto1">
              <label>Costo</label>
              <hint>Costo 1 [en miles de pesos]</hint>
            </input>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redFija/redFijaPrestador2">
              <label>Prestador</label>
              <hint>Prestador 2</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redFija/redFijaCosto2">
              <label>Costo</label>
              <hint>Costo 2 [en miles de pesos]</hint>
            </input>
            <select1 appearance="minimal" ref="/C1.48/meses/mes/redFija/redFijaPrestador3">
              <label>Prestador</label>
              <hint>Prestador 3</hint>
              <item>
                <label>Seleccione una opción</label>
                <value>-1</value>
              </item>
            </select1>
            <input ref="/C1.48/meses/mes/redFija/redFijaCosto3">
              <label>Costo</label>
              <hint>Costo 3 [en miles de pesos]</hint>
            </input>
            <input ref="/C1.48/meses/mes/redFija/redFijaPrestadorOtros">
              <label>Prestador Otros</label>
              <hint>Prestador Otros</hint>
            </input>
            <input ref="/C1.48/meses/mes/redFija/redFijaCostoOtros">
              <label>Costo Otros</label>
              <hint>Costo Otros [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.48/meses/mes/otros">
            <input ref="/C1.48/meses/mes/otros/mantenimiento">
              <label>Mantenimiento de Redes</label>
              <hint>Mantenimiento de Redes [en miles de pesos]</hint>
            </input>
            <input ref="/C1.48/meses/mes/otros/otros">
              <label>Otros</label>
              <hint>Otros [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/C1.48/meses/mes/grupoTotal">
            <input ref="/C1.48/meses/mes/grupoTotal/total">
              <label>Total</label>
              <hint>Total [en miles de pesos]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
