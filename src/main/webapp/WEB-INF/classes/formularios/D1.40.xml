<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.40 - Costos de interconexión</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <D1.40 id="D1.40">
          <meses jr:template="">
            <mes>
              <redMovil>
                <redMovilProve1/>
                <redMovilCosto1/>
                <redMovilProve2/>
                <redMovilCosto2/>
                <redMovilProve3/>
                <redMovilCosto3/>
                <redMovilProveOtros/>
                <redMovilCostoOtros/>
              </redMovil>
              <redFija>
                <redFijaProve1/>
                <redFijaCosto1/>
                <redFijaProve2/>
                <redFijaCosto2/>
                <redFijaProve3/>
                <redFijaCosto3/>
                <redFijaProveOtros/>
                <redFijaCostoOtros/>
              </redFija>
              <otros>
                <mantenimiento/>
                <otros/>
              </otros>
              <gtotal>
                <total/>
              </gtotal>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </D1.40>
      </instance>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redMovil/redMovilProve1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redMovil/redMovilCosto1" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redMovil/redMovilProve2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redMovil/redMovilCosto2" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redMovil/redMovilProve3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redMovil/redMovilCosto3" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redMovil/redMovilProveOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redMovil/redMovilCostoOtros" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redFija/redFijaProve1" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redFija/redFijaCosto1" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redFija/redFijaProve2" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redFija/redFijaCosto2" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redFija/redFijaProve3" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redFija/redFijaCosto3" required="true()" type="int"/>
      <bind constraint="cuit" jr:constraintMsg="CUIT no válido" nodeset="/D1.40/meses/mes/redFija/redFijaProveOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/redFija/redFijaCostoOtros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/otros/mantenimiento" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/otros/otros" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.40/meses/mes/gtotal/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.40/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/D1.40/meses">
      <label></label>
      <repeat nodeset="/D1.40/meses">
        <group appearance="col1 well-white variable" ref="/D1.40/meses/mes">
          <label>{title}</label>
          <group appearance="col2 well-white" ref="/D1.40/meses/mes/redMovil">
            <label>Costo interconexión a red móvil</label>
            <input ref="/D1.40/meses/mes/redMovil/redMovilProve1">
              <label>Proveedor</label>
              <hint>Proveedor 1</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilCosto1">
              <label>Costo</label>
              <hint>Costo 1 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilProve2">
              <label>Proveedor</label>
              <hint>Proveedor 2</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilCosto2">
              <label>Costo</label>
              <hint>Costo 2 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilProve3">
              <label>Proveedor</label>
              <hint>Proveedor 3</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilCosto3">
              <label>Costo</label>
              <hint>Costo 3 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilProveOtros">
              <label>Proveedor Otros</label>
              <hint>Proveedor Otros</hint>
            </input>
            <input ref="/D1.40/meses/mes/redMovil/redMovilCostoOtros">
              <label>Otros Costos</label>
              <hint>Otros Costos [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.40/meses/mes/redFija">
            <label>Costo interconexión a red fija</label>
            <input ref="/D1.40/meses/mes/redFija/redFijaProve1">
              <label>Proveedor</label>
              <hint>Proveedor 1</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaCosto1">
              <label>Costo</label>
              <hint>Costo 1 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaProve2">
              <label>Proveedor</label>
              <hint>Proveedor 2</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaCosto2">
              <label>Costo</label>
              <hint>Costo 2 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaProve3">
              <label>Proveedor</label>
              <hint>Proveedor 3</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaCosto3">
              <label>Costo</label>
              <hint>Costo 3 [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaProveOtros">
              <label>Proveedor Otros</label>
              <hint>Proveedor Otros</hint>
            </input>
            <input ref="/D1.40/meses/mes/redFija/redFijaCostoOtros">
              <label>Otros Costos</label>
              <hint>Otros Costos [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.40/meses/mes/otros">
            <input ref="/D1.40/meses/mes/otros/mantenimiento">
              <label>Mantenimiento</label>
              <hint>Mantenimiento de Redes [en miles de pesos]</hint>
            </input>
            <input ref="/D1.40/meses/mes/otros/otros">
              <label>Otros</label>
              <hint>Otros [en miles de pesos]</hint>
            </input>
          </group>
          <group appearance="col2 well-white" ref="/D1.40/meses/mes/gtotal">
            <input ref="/D1.40/meses/mes/gtotal/total">
              <label>Total</label>
              <hint>Total [en miles de pesos]</hint>
            </input>
          </group>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
