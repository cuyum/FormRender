<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.44 - Ingresos interconexion a la red fija - LDN a vinculadas</h:title>
    <model>
      <instance>
        <C1.44 id="C1.44">
          <meses jr:template="">
            <mes>
              <arriendo/>
              <transporte/>
              <otros/>
              <calculate2/>
              <total/>
            </mes>
          </meses>
          <meta>
            <instanceID/>
          </meta>
        </C1.44>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.44/meses/mes/arriendo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.44/meses/mes/transporte" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.44/meses/mes/otros" required="true()" type="int"/>
      <bind calculate=" /C1.44/meses/mes/arriendo + /C1.44/meses/mes/transporte + /C1.44/meses/mes/otros " nodeset="/C1.44/meses/mes/calculate2" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.44/meses/mes/total" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.44/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/C1.44/meses">
      <label></label>
      <repeat nodeset="/C1.44/meses">
        <group appearance="col4 well-white variable" ref="/C1.44/meses/mes">
          <label>{title}</label>
          <input ref="/C1.44/meses/mes/arriendo">
            <label>Arriendo de Circuitos LDN</label>
            <hint>Arriendo de Circuitos LDN [en miles de pesos]</hint>
          </input>
          <input ref="/C1.44/meses/mes/transporte">
            <label>Transporte LDN</label>
            <hint>Transporte LDN [en miles de pesos]</hint>
          </input>
          <input ref="/C1.44/meses/mes/otros">
            <label>Otros</label>
            <hint>Otros [en miles de pesos]</hint>
          </input>
          <input ref="/C1.44/meses/mes/total">
            <label>Total</label>
            <hint>Total [en miles de pesos]</hint>
          </input>
        </group>
      </repeat>
    </group>
  </h:body>
</h:html>
