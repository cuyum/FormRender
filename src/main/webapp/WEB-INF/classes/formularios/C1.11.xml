<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.11 - Tráfico III en cantidad de llamadas</h:title>
    <model>
      <instance>
        <C1.11 id="C1.11">
          <mes>
            <provincia/>
            <partido/>
            <localidad/>
            <areas_local/>
            <l_l>
              <hn_1/>
              <hr_1/>
            </l_l>
            <l_ldn>
              <hn_2/>
              <hr_2/>
            </l_ldn>
            <l_ldi>
              <hn_3/>
              <hr_3/>
            </l_ldi>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.11>
      </instance>
      <bind constraint="{u'default': u'url=/areas/provincias'}" nodeset="/C1.11/mes/provincia" required="true()" type="select1"/>
      <bind constraint="{u'default': u'url=/areas/partidos and depends=/C1.11/mes/provincia'}" nodeset="/C1.11/mes/partido" required="true()" type="select1"/>
      <bind constraint="{u'default': u'url=/areas/localidades and depends=/C1.11/mes/partido'}" nodeset="/C1.11/mes/localidad" required="true()" type="select1"/>
      <bind constraint="url=/areas/areas and depends=/C1.11/mes/localidad" nodeset="/C1.11/mes/areas_local" required="true()" type="select1"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_l/hn_1" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_l/hr_1" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_ldn/hn_2" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_ldn/hr_2" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_ldi/hn_3" required="true()" type="int"/>
      <bind constraint="{u'default': u'.&lt;1000000 and .&gt;0'}" jr:constraintMsg="Debe ingresar un número entero hasta 6 dígitos y mayor a 0" nodeset="/C1.11/mes/l_ldi/hr_3" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.11/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.11/mes">
      <label>Enero</label>
      <select1 appearance="minimal" ref="/C1.11/mes/provincia">
        <label>Provincia</label>
        <hint>Seleccione Provincia</hint>
        <item>
          <label></label>
          <value>id_provincia</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.11/mes/partido">
        <label>Partido / Departamento</label>
        <hint>Seleccione Partido</hint>
        <item>
          <label></label>
          <value>id_dpto_partido</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.11/mes/localidad">
        <label>Localidad</label>
        <hint>Seleccione Localidad</hint>
        <item>
          <label></label>
          <value>id_localidad</value>
        </item>
      </select1>
      <select1 appearance="minimal" ref="/C1.11/mes/areas_local">
        <label>Área Local</label>
        <hint>Seleccione Área Local</hint>
        <item>
          <label></label>
          <value>id_area_local</value>
        </item>
      </select1>
      <group appearance="col2 well-white" ref="/C1.11/mes/l_l">
        <label>L-L</label>
        <input ref="/C1.11/mes/l_l/hn_1">
          <label>HN</label>
          <hint>Ingrese número entero</hint>
        </input>
        <input ref="/C1.11/mes/l_l/hr_1">
          <label>HR</label>
          <hint>Ingrese número entero</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.11/mes/l_ldn">
        <label>L-LDN</label>
        <input ref="/C1.11/mes/l_ldn/hn_2">
          <label>HN</label>
          <hint>Ingrese número entero</hint>
        </input>
        <input ref="/C1.11/mes/l_ldn/hr_2">
          <label>HR</label>
          <hint>Ingrese número entero</hint>
        </input>
      </group>
      <group appearance="col2 well-white" ref="/C1.11/mes/l_ldi">
        <label>L-LDI</label>
        <input ref="/C1.11/mes/l_ldi/hn_3">
          <label>HN</label>
          <hint>Ingrese número entero</hint>
        </input>
        <input ref="/C1.11/mes/l_ldi/hr_3">
          <label>HR</label>
          <hint>Ingrese número entero</hint>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
