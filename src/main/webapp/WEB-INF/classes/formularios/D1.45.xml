<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.45 - Financiamiento SU / Subsidios</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <D1.45 id="D1.45">
          <financiamiento>
            <financiamientoSU/>
            <subsidiosTasaInteres/>
            <subsidiosImpositivos/>
            <subsidiosOtros/>
          </financiamiento>
          <meta>
            <instanceID/>
          </meta>
        </D1.45>
      </instance>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.45/financiamiento/financiamientoSU" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.45/financiamiento/subsidiosTasaInteres" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.45/financiamiento/subsidiosImpositivos" required="true()" type="int"/>
      <bind constraint="mask=999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/D1.45/financiamiento/subsidiosOtros" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.45/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/D1.45/financiamiento">
      <label>Financiamiento</label>
      <input ref="/D1.45/financiamiento/financiamientoSU">
        <label>Financiamiento con SU</label>
        <hint>Financiamiento con SU [en miles de pesos]</hint>
      </input>
      <input ref="/D1.45/financiamiento/subsidiosTasaInteres">
        <label>Subsidios tasa de Interés</label>
        <hint>Subsidios tasa de Interés [en miles de pesos]</hint>
      </input>
      <input ref="/D1.45/financiamiento/subsidiosImpositivos">
        <label>Subsidios impositivos</label>
        <hint>Subsidios Impositivos [en miles de pesos]</hint>
      </input>
      <input ref="/D1.45/financiamiento/subsidiosOtros">
        <label>Otros Subsidios</label>
        <hint>Otros Subsidios [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
