<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.41 - Ingresos interconexion a la red fija - larga distancia nacional a 3</h:title>
    <model>
      <instance>
        <C1.41 id="C1.41">
          <mes>
            <arriendo/>
            <transporte/>
            <otros/>
            <calculate2/>
            <total/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.41>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.41/mes/arriendo" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.41/mes/transporte" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.41/mes/otros" required="true()" type="int"/>
      <bind calculate=" /C1.41/mes/arriendo + /C1.41/mes/transporte + /C1.41/mes/otros " nodeset="/C1.41/mes/calculate2" type="string"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no v치lidos" nodeset="/C1.41/mes/total" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.41/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="Col4 well-white" ref="/C1.41/mes">
      <label>Enero</label>
      <input ref="/C1.41/mes/arriendo">
        <label>Arriendo de Circuitos LDN</label>
        <hint>Arriendo de Circuitos LDN [en miles de pesos]</hint>
      </input>
      <input ref="/C1.41/mes/transporte">
        <label>Transporte LDN</label>
        <hint>Transporte LDN [en miles de pesos]</hint>
      </input>
      <input ref="/C1.41/mes/otros">
        <label>Otros</label>
        <hint>Otros [en miles de pesos]</hint>
      </input>
      <input ref="/C1.41/mes/total">
        <label>Total <output value=" /C1.41/mes/calculate2 "/></label><hint>Total [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
