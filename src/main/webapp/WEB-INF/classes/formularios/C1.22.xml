<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.22 - Tarifas / precio vigente</h:title>
    <model>
      <instance>
        <C1.22 id="C1.22">
          <instalacion>
            <instalacion_familia/>
            <instalacion_comercial/>
            <instalacion_gobierno/>
            <instalacion_jubilados/>
            <instalacion_profesional/>
            <instalacion_otros/>
          </instalacion>
          <abono>
            <abono_familia/>
            <abono_comercial/>
            <abono_gobierno/>
            <abono_jubilados/>
            <abono_profesional/>
            <abono_otros/>
          </abono>
          <abono_entrante>
            <entrante_familia/>
            <entrante_comercial/>
            <entrante_gobierno/>
            <entrante_jubilados/>
            <entrante_profesional/>
            <entrante_otros/>
          </abono_entrante>
          <meta>
            <instanceID/>
          </meta>
        </C1.22>
      </instance>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_familia" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_comercial" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_gobierno" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_jubilados" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_profesional" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/instalacion/instalacion_otros" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_familia" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_comercial" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_gobierno" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_jubilados" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_profesional" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono/abono_otros" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_familia" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_comercial" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_gobierno" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_jubilados" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_profesional" required="true()" type="decimal"/>
      <bind constraint="mask=999.999,99" jr:constraintMsg="Datos no válidos" nodeset="/C1.22/abono_entrante/entrante_otros" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.22/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col6 well-white" ref="/C1.22/instalacion">
      <label>Instalación</label>
      <input ref="/C1.22/instalacion/instalacion_familia">
        <label>Hogares</label>
        <hint>Hogares [en pesos]</hint>
      </input>
      <input ref="/C1.22/instalacion/instalacion_comercial">
        <label>Comercial</label>
        <hint>Comercial [en pesos]</hint>
      </input>
      <input ref="/C1.22/instalacion/instalacion_gobierno">
        <label>Gobierno</label>
        <hint>Gobierno [en pesos]</hint>
      </input>
      <input ref="/C1.22/instalacion/instalacion_jubilados">
        <label>Jubilados</label>
        <hint>Jubilados [en pesos]</hint>
      </input>
      <input ref="/C1.22/instalacion/instalacion_profesional">
        <label>Profesional</label>
        <hint>Profesional [en pesos]</hint>
      </input>
      <input ref="/C1.22/instalacion/instalacion_otros">
        <label>Otros</label>
        <hint>Otros [en pesos]</hint>
      </input>
    </group>
    <group appearance="col6 well-white" ref="/C1.22/abono">
      <label>Abono</label>
      <input ref="/C1.22/abono/abono_familia">
        <label>Hogares</label>
        <hint>Hogares [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono/abono_comercial">
        <label>Comercial</label>
        <hint>Comercial [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono/abono_gobierno">
        <label>Gobierno</label>
        <hint>Gobierno [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono/abono_jubilados">
        <label>Jubilados</label>
        <hint>Jubilados [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono/abono_profesional">
        <label>Profesional</label>
        <hint>Profesional [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono/abono_otros">
        <label>Otros</label>
        <hint>Otros [en pesos]</hint>
      </input>
    </group>
    <group appearance="col6 well-white" ref="/C1.22/abono_entrante">
      <label>Abono Entrante</label>
      <input ref="/C1.22/abono_entrante/entrante_familia">
        <label>Hogares</label>
        <hint>Hogares [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono_entrante/entrante_comercial">
        <label>Comercial</label>
        <hint>Comercial [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono_entrante/entrante_gobierno">
        <label>Gobierno</label>
        <hint>Gobierno [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono_entrante/entrante_jubilados">
        <label>Jubilados</label>
        <hint>Jubilados [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono_entrante/entrante_profesional">
        <label>Profesional</label>
        <hint>Profesional [en pesos]</hint>
      </input>
      <input ref="/C1.22/abono_entrante/entrante_otros">
        <label>Otros</label>
        <hint>Otros [en pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
