<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C1.30 - Ingresos / instalación de líneas</h:title>
    <model>
      <instance>
        <C1.30 id="C1.30">
          <mes>
            <hogares/>
            <comercial/>
            <gobierno/>
            <jubilados/>
            <profesional/>
            <otros/>
          </mes>
          <meta>
            <instanceID/>
          </meta>
        </C1.30>
      </instance>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/hogares" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/comercial" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/gobierno" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/jubilados" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/profesional" required="true()" type="int"/>
      <bind constraint="mask=999.999.999.999" jr:constraintMsg="Datos no válidos" nodeset="/C1.30/mes/otros" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.30/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4 well-white" ref="/C1.30/mes">
      <label>Enero</label>
      <input ref="/C1.30/mes/hogares">
        <label>Hogares:</label>
        <hint>Hogares [en miles de pesos]</hint>
      </input>
      <input ref="/C1.30/mes/comercial">
        <label>Comercial:</label>
        <hint>Comercial [en miles de pesos]</hint>
      </input>
      <input ref="/C1.30/mes/gobierno">
        <label>Gobierno:</label>
        <hint>Gobierno [en miles de pesos]</hint>
      </input>
      <input ref="/C1.30/mes/jubilados">
        <label>Jubilados:</label>
        <hint>Jubilados [en miles de pesos]</hint>
      </input>
      <input ref="/C1.30/mes/profesional">
        <label>Profesional:</label>
        <hint>Profesional [en miles de pesos]</hint>
      </input>
      <input ref="/C1.30/mes/otros">
        <label>Otros:</label>
        <hint>Otros [en miles de pesos]</hint>
      </input>
    </group>
  </h:body>
</h:html>
