<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario C.1.30 - Ingresos / instalacion de linea</h:title>
    <model>
      <itext>
        <translation lang="Spanish">
          <text id="/C1.30/Enero/comercial:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
          <text id="/C1.30/Enero/familia:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
          <text id="/C1.30/Enero/jubilados:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
          <text id="/C1.30/Enero/gobierno:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
          <text id="/C1.30/Enero/otros:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
          <text id="/C1.30/Enero/profesional:hint">
            <value>Ingrese número entero con 11 digitos sin guión</value>
          </text>
        </translation>
      </itext>
      <instance>
        <C1.30 id="C1.30">
          <Enero>
            <familia>0</familia>
            <comercial>0</comercial>
            <gobierno>0</gobierno>
            <jubilados>0</jubilados>
            <profesional>0</profesional>
            <otros>0</otros>
          </Enero>
          <meta>
            <instanceID/>
          </meta>
        </C1.30>
      </instance>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/familia" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/comercial" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/gobierno" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/jubilados" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/profesional" required="true()" type="int"/>
      <bind constraint="regex(., '[0-9]*') and .&lt;1000000000000 and .&gt;0" jr:constraintMsg="Debe ingresar un número entero de 12 dígitos" nodeset="/C1.30/Enero/otros" required="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/C1.30/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="col4" ref="/C1.30/Enero">
      <label>Enero</label>
      <input ref="/C1.30/Enero/familia">
        <label>Hogares:</label>
        <hint ref="jr:itext('/C1.30/Enero/familia:hint')"/>
      </input>
      <input ref="/C1.30/Enero/comercial">
        <label>Comercial:</label>
        <hint ref="jr:itext('/C1.30/Enero/comercial:hint')"/>
      </input>
      <input ref="/C1.30/Enero/gobierno">
        <label>Gobierno:</label>
        <hint ref="jr:itext('/C1.30/Enero/gobierno:hint')"/>
      </input>
      <input ref="/C1.30/Enero/jubilados">
        <label>Jubilados:</label>
        <hint ref="jr:itext('/C1.30/Enero/jubilados:hint')"/>
      </input>
      <input ref="/C1.30/Enero/profesional">
        <label>Profesional:</label>
        <hint ref="jr:itext('/C1.30/Enero/profesional:hint')"/>
      </input>
      <input ref="/C1.30/Enero/otros">
        <label>Otros:</label>
        <hint ref="jr:itext('/C1.30/Enero/otros:hint')"/>
      </input>
    </group>
  </h:body>
</h:html>
