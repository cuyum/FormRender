<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Formulario D1.30 - Tarifas LDI</h:title>
    <model>
      <submission action="/api/deposition/draft" method="form-data-post"/>
      <instance>
        <D1.30 id="D1.30">
          <limitrofes>
            <roaming1>
              <roamingPre1/>
              <roamingPos1/>
            </roaming1>
            <ldn1>
              <ldnPre1/>
              <ldnPos1/>
            </ldn1>
          </limitrofes>
          <sud>
            <roaming2>
              <roamingPre2/>
              <roamingPos2/>
            </roaming2>
            <ldn2>
              <ldnPre2/>
              <ldnPos2/>
            </ldn2>
          </sud>
          <centroNorte>
            <roaming3>
              <roamingPre3/>
              <roamingPos3/>
            </roaming3>
            <ldn3>
              <ldnPre3/>
              <ldnPos3/>
            </ldn3>
          </centroNorte>
          <europa>
            <roaming4>
              <roamingPre4/>
              <roamingPos4/>
            </roaming4>
            <ldn4>
              <ldnPre4/>
              <ldnPos4/>
            </ldn4>
          </europa>
          <mundo>
            <roaming5>
              <roamingPre5/>
              <roamingPos5/>
            </roaming5>
            <ldn5>
              <ldnPre5/>
              <ldnPos5/>
            </ldn5>
          </mundo>
          <meta>
            <instanceID/>
          </meta>
        </D1.30>
      </instance>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/limitrofes/roaming1/roamingPre1" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/limitrofes/roaming1/roamingPos1" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/limitrofes/ldn1/ldnPre1" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/limitrofes/ldn1/ldnPos1" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/sud/roaming2/roamingPre2" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/sud/roaming2/roamingPos2" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/sud/ldn2/ldnPre2" required="true()" type="decimal"/>
