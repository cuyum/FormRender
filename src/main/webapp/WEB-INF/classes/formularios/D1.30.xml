      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/centroNorte/roaming3/roamingPos3" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/centroNorte/ldn3/ldnPre3" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/centroNorte/ldn3/ldnPos3" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/europa/roaming4/roamingPre4" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/europa/roaming4/roamingPos4" required="true()" type="decimal"/>
