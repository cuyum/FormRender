      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/mundo/roaming5/roamingPre5" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/mundo/roaming5/roamingPos5" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/mundo/ldn5/ldnPre5" required="true()" type="decimal"/>
      <bind constraint="mask=99,999" jr:constraintMsg="Datos no válidos" nodeset="/D1.30/mundo/ldn5/ldnPos5" required="true()" type="decimal"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/D1.30/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
